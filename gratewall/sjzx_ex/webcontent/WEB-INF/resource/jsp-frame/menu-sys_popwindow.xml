<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_popwindow.xml" app-path="/sysmgr/popWindow/" app-name="sys_popwindow" description="弹窗设置" template="single-table.xml"/>
  <table name="sys_popwindow.xml" app-name="sys_popwindow" app-desc="弹窗任务" delete-action="delete" description="弹窗设置" class-name="com.gwssi.sysmgr.txn.TxnSysPopwindow" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_popwindow.jsp" txn-code="60800001" description="查询弹窗任务列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加弹窗任务" path="insert-sys_popwindow.jsp" description="增加弹窗任务" func-name="addRecord"/>
	    <button type="button" name="修改弹窗任务" path="/txn60800004.do" description="修改弹窗任务" func-name="updateRecord"/>
	    <button type="button" name="删除弹窗任务" path="/txn60800005.do" description="删除弹窗任务" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn60800001.do" description="查询弹窗任务信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_popwindow.jsp" txn-code="60800003" description="增加弹窗任务信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60800003.do" description="保存弹窗任务信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn60800003.do" description="保存弹窗任务信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn60800001.do" description="保存弹窗任务信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60800001.do" description="返回到弹窗任务明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_popwindow.jsp" txn-code="60800002" description="修改弹窗任务信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60800001.do" description="修改弹窗任务信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60800001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_popwindow.jsp" txn-code="60800001" description="查询弹窗任务列表">
	    <forward error-code="success" path="query-sys_popwindow.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_popwindow.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_popwindow.jsp" txn-code="60800002" description="修改弹窗任务信息">
	    <forward error-code="success" path="update-sys_popwindow.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_popwindow.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_popwindow.jsp" txn-code="60800003" description="增加弹窗任务信息">
	    <forward error-code="success" path="insert-sys_popwindow.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_popwindow.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_popwindow.jsp" txn-code="60800004" description="查询弹窗任务用于修改">
	    <forward error-code="success" path="update-sys_popwindow.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_popwindow.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_popwindow.jsp" txn-code="60800005" description="删除弹窗任务信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.sysmgr.vo.SysPopwindowContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.sysmgr.vo.VoSysPopwindow" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.sysmgr.vo.VoSysPopwindowSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.sysmgr.vo.VoSysPopwindowPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
