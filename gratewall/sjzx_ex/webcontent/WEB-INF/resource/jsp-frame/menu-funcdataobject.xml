<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="funcdataobject.xml" app-path="/xtgl/qxgl/sjqxlxgl/" app-name="funcdataobject" description="数据权限类型管理" template="single-table.xml"/>
  <table name="funcdataobject.xml" app-name="funcdataobject" app-desc="配置功能关系" delete-action="delete" description="数据权限类型管理" class-name="cn.gwssi.csdb.xtgl.txn.TxnFuncdataobject" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-funcdataobject.jsp" txn-code="103021" description="查询配置功能关系列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加配置功能关系" path="insert-funcdataobject.jsp" description="增加配置功能关系" func-name="addRecord"/>
	    <button type="button" name="修改配置功能关系" path="/txn103024.do" description="修改配置功能关系" func-name="updateRecord"/>
	    <button type="button" name="删除配置功能关系" path="/txn103025.do" description="删除配置功能关系" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn103021.do" description="查询配置功能关系信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-funcdataobject.jsp" txn-code="103023" description="增加配置功能关系信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn103023.do" description="保存配置功能关系信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn103023.do" description="保存配置功能关系信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn103021.do" description="保存配置功能关系信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn103021.do" description="返回到配置功能关系明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-funcdataobject.jsp" txn-code="103022" description="修改配置功能关系信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn103021.do" description="修改配置功能关系信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn103021.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-funcdataobject.jsp" txn-code="103021" description="查询配置功能关系列表">
	    <forward error-code="success" path="query-funcdataobject.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-funcdataobject.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-funcdataobject.jsp" txn-code="103022" description="修改配置功能关系信息">
	    <forward error-code="success" path="update-funcdataobject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-funcdataobject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-funcdataobject.jsp" txn-code="103023" description="增加配置功能关系信息">
	    <forward error-code="success" path="insert-funcdataobject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-funcdataobject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-funcdataobject.jsp" txn-code="103024" description="查询配置功能关系用于修改">
	    <forward error-code="success" path="update-funcdataobject.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-funcdataobject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-funcdataobject.jsp" txn-code="103025" description="删除配置功能关系信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.FuncdataobjectContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoFuncdataobject" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoFuncdataobjectSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoFuncdataobjectPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
