	    <button type="button" name="返 回" path="/txn620200101.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-log_report_create.jsp" txn-code="620200101" description="查询系统使用情况报告列表">
	    <forward error-code="success" path="query-log_report_create.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-log_report_create.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-log_report_create.jsp" txn-code="620200102" description="修改系统使用情况报告信息">
	    <forward error-code="success" path="update-log_report_create.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-log_report_create.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-log_report_create.jsp" txn-code="620200103" description="增加系统使用情况报告信息">
	    <forward error-code="success" path="insert-log_report_create.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-log_report_create.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-log_report_create.jsp" txn-code="620200104" description="查询系统使用情况报告用于修改">
	    <forward error-code="success" path="update-log_report_create.jsp" description="成功时返回记录"/>
