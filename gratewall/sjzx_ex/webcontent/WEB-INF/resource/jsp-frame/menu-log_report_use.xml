<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="log_report_use.xml" app-path="/dw/rd/log/report/" app-name="log_report_use" description="日志报告使用情况" template="single-table.xml"/>
  <table name="log_report_use.xml" app-name="log_report_use" app-desc="报告操作情况" delete-action="delete" description="日志报告使用情况" class-name="cn.gwssi.dw.rd.log.txn.TxnLogReportUse" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-log_report_use.jsp" txn-code="620200201" description="查询报告操作情况列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加报告操作情况" path="insert-log_report_use.jsp" description="增加报告操作情况" func-name="addRecord"/>
	    <button type="button" name="修改报告操作情况" path="/txn620200204.do" description="修改报告操作情况" func-name="updateRecord"/>
	    <button type="button" name="删除报告操作情况" path="/txn620200205.do" description="删除报告操作情况" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn620200201.do" description="查询报告操作情况信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-log_report_use.jsp" txn-code="620200203" description="增加报告操作情况信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn620200203.do" description="保存报告操作情况信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn620200203.do" description="保存报告操作情况信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn620200201.do" description="保存报告操作情况信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn620200201.do" description="返回到报告操作情况明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-log_report_use.jsp" txn-code="620200202" description="修改报告操作情况信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn620200201.do" description="修改报告操作情况信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn620200201.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-log_report_use.jsp" txn-code="620200201" description="查询报告操作情况列表">
	    <forward error-code="success" path="query-log_report_use.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-log_report_use.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-log_report_use.jsp" txn-code="620200202" description="修改报告操作情况信息">
	    <forward error-code="success" path="update-log_report_use.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-log_report_use.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-log_report_use.jsp" txn-code="620200203" description="增加报告操作情况信息">
	    <forward error-code="success" path="insert-log_report_use.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-log_report_use.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-log_report_use.jsp" txn-code="620200204" description="查询报告操作情况用于修改">
	    <forward error-code="success" path="update-log_report_use.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-log_report_use.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-log_report_use.jsp" txn-code="620200205" description="删除报告操作情况信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.LogReportUseContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.VoLogReportUse" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.VoLogReportUseSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.VoLogReportUsePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
