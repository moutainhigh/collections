<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="gz_tb_jbxx.xml" app-path="/gzk/bbgl/" app-name="gz_tb_jbxx" description="报表管理" template="single-table.xml"/>
  <table name="gz_tb_jbxx.xml" app-name="gz_tb_jbxx" app-desc="报表" delete-action="delete" description="报表管理" class-name="cn.gwssi.csdb.gzk.txn.TxnGzTbJbxx" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-gz_tb_jbxx.jsp" txn-code="40400001" description="查询报表列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加报表" path="insert-gz_tb_jbxx.jsp" description="增加报表" func-name="addRecord"/>
	    <button type="button" name="修改报表" path="/txn40400004.do" description="修改报表" func-name="updateRecord"/>
	    <button type="button" name="删除报表" path="/txn40400005.do" description="删除报表" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn40400001.do" description="查询报表信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-gz_tb_jbxx.jsp" txn-code="40400003" description="增加报表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn40400003.do" description="保存报表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn40400003.do" description="保存报表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn40400001.do" description="保存报表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40400001.do" description="返回到报表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-gz_tb_jbxx.jsp" txn-code="40400002" description="修改报表信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn40400001.do" description="修改报表信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40400001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-gz_tb_jbxx.jsp" txn-code="40400001" description="查询报表列表">
	    <forward error-code="success" path="query-gz_tb_jbxx.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-gz_tb_jbxx.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-gz_tb_jbxx.jsp" txn-code="40400002" description="修改报表信息">
	    <forward error-code="success" path="update-gz_tb_jbxx.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-gz_tb_jbxx.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-gz_tb_jbxx.jsp" txn-code="40400003" description="增加报表信息">
	    <forward error-code="success" path="insert-gz_tb_jbxx.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-gz_tb_jbxx.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-gz_tb_jbxx.jsp" txn-code="40400004" description="查询报表用于修改">
	    <forward error-code="success" path="update-gz_tb_jbxx.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-gz_tb_jbxx.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-gz_tb_jbxx.jsp" txn-code="40400005" description="删除报表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.gzk.vo.GzTbJbxxContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.gzk.vo.VoGzTbJbxx" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.gzk.vo.VoGzTbJbxxSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.gzk.vo.VoGzTbJbxxPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
