<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_svr_config.xml" app-path="/dw/runmgr/services/svrcfg/" app-name="sys_svr_config" description="服务配置" template="single-table.xml"/>
  <table name="sys_svr_config.xml" app-name="sys_svr_config" app-desc="服务配置" delete-action="delete" description="服务配置" class-name="com.gwssi.dw.runmgr.services.txn.TxnSysSvrConfig" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_svr_config.jsp" txn-code="50203001" description="查询服务配置列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加服务配置" path="insert-sys_svr_config.jsp" description="增加服务配置" func-name="addRecord"/>
	    <button type="button" name="修改服务配置" path="/txn50203004.do" description="修改服务配置" func-name="updateRecord"/>
	    <button type="button" name="删除服务配置" path="/txn50203005.do" description="删除服务配置" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn50203001.do" description="查询服务配置信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_svr_config.jsp" txn-code="50203003" description="增加服务配置信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn50203003.do" description="保存服务配置信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn50203003.do" description="保存服务配置信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn50203001.do" description="保存服务配置信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50203001.do" description="返回到服务配置明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_svr_config.jsp" txn-code="50203002" description="修改服务配置信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn50203001.do" description="修改服务配置信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50203001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_svr_config.jsp" txn-code="50203001" description="查询服务配置列表">
	    <forward error-code="success" path="query-sys_svr_config.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_svr_config.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_svr_config.jsp" txn-code="50203002" description="修改服务配置信息">
	    <forward error-code="success" path="update-sys_svr_config.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_svr_config.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_svr_config.jsp" txn-code="50203003" description="增加服务配置信息">
	    <forward error-code="success" path="insert-sys_svr_config.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_svr_config.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_svr_config.jsp" txn-code="50203004" description="查询服务配置用于修改">
	    <forward error-code="success" path="update-sys_svr_config.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_svr_config.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_svr_config.jsp" txn-code="50203005" description="删除服务配置信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.runmgr.services.vo.SysSvrConfigContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.services.vo.VoSysSvrConfig" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.services.vo.VoSysSvrConfigSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.services.vo.VoSysSvrConfigPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
