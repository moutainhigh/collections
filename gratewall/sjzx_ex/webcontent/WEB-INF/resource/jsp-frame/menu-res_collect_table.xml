<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="res_collect_table.xml" app-path="/page/resource/colRes/colTable/" app-name="res_collect_table" description="采集数据表信息表" template="single-table.xml"/>
  <table name="res_collect_table.xml" app-name="res_collect_table" app-desc="采集数据表信息表" delete-action="delete" description="采集数据表信息表" class-name="com.gwssi.resource.collect.txn.TxnResCollectTable" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-res_collect_table.jsp" txn-code="20201001" description="查询采集数据表信息表列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加采集数据表信息表" path="insert-res_collect_table.jsp" description="增加采集数据表信息表" func-name="addRecord"/>
	    <button type="button" name="修改采集数据表信息表" path="/txn20201004.do" description="修改采集数据表信息表" func-name="updateRecord"/>
	    <button type="button" name="删除采集数据表信息表" path="/txn20201005.do" description="删除采集数据表信息表" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn20201001.do" description="查询采集数据表信息表信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-res_collect_table.jsp" txn-code="20201003" description="增加采集数据表信息表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn20201003.do" description="保存采集数据表信息表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn20201003.do" description="保存采集数据表信息表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn20201001.do" description="保存采集数据表信息表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn20201001.do" description="返回到采集数据表信息表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-res_collect_table.jsp" txn-code="20201002" description="修改采集数据表信息表信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn20201001.do" description="修改采集数据表信息表信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn20201001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-res_collect_table.jsp" txn-code="20201001" description="查询采集数据表信息表列表">
	    <forward error-code="success" path="query-res_collect_table.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-res_collect_table.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-res_collect_table.jsp" txn-code="20201002" description="修改采集数据表信息表信息">
	    <forward error-code="success" path="update-res_collect_table.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-res_collect_table.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-res_collect_table.jsp" txn-code="20201003" description="增加采集数据表信息表信息">
	    <forward error-code="success" path="insert-res_collect_table.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-res_collect_table.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_collect_table.jsp" txn-code="20201004" description="查询采集数据表信息表用于修改">
	    <forward error-code="success" path="update-res_collect_table.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-res_collect_table.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_collect_table.jsp" txn-code="20201005" description="删除采集数据表信息表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.resource.collect.vo.ResCollectTableContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.resource.collect.vo.VoResCollectTable" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.resource.collect.vo.VoResCollectTableSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.resource.collect.vo.VoResCollectTablePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
