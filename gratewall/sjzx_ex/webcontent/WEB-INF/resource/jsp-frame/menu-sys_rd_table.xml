<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_rd_table.xml" app-path="/dw/rd/metadata/" app-name="sys_rd_table" description="已认领表" template="single-table.xml"/>
  <table name="sys_rd_table.xml" app-name="sys_rd_table" app-desc="已认领" delete-action="delete" description="已认领表" class-name="cn.gwssi.dw.rd.metadata.txn.TxnSysRdTable" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_rd_table.jsp" txn-code="80002201" description="查询已认领列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加已认领" path="insert-sys_rd_table.jsp" description="增加已认领" func-name="addRecord"/>
	    <button type="button" name="修改已认领" path="/txn80002204.do" description="修改已认领" func-name="updateRecord"/>
	    <button type="button" name="删除已认领" path="/txn80002205.do" description="删除已认领" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn80002201.do" description="查询已认领信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_rd_table.jsp" txn-code="80002203" description="增加已认领信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn80002203.do" description="保存已认领信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn80002203.do" description="保存已认领信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn80002201.do" description="保存已认领信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80002201.do" description="返回到已认领明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_rd_table.jsp" txn-code="80002202" description="修改已认领信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn80002201.do" description="修改已认领信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80002201.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_rd_table.jsp" txn-code="80002201" description="查询已认领列表">
	    <forward error-code="success" path="query-sys_rd_table.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_rd_table.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_rd_table.jsp" txn-code="80002202" description="修改已认领信息">
	    <forward error-code="success" path="update-sys_rd_table.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_rd_table.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_rd_table.jsp" txn-code="80002203" description="增加已认领信息">
	    <forward error-code="success" path="insert-sys_rd_table.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_rd_table.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_table.jsp" txn-code="80002204" description="查询已认领用于修改">
	    <forward error-code="success" path="update-sys_rd_table.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_rd_table.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_table.jsp" txn-code="80002205" description="删除已认领信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.SysRdTableContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdTable" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdTableSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdTablePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
