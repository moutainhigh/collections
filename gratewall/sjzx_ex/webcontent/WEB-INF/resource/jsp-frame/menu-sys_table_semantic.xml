<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_table_semantic.xml" app-path="/ysjgl/jcxx//ywbyydy/" app-name="sys_table_semantic" description="业务表定义" template="single-table.xml"/>
  <table name="sys_table_semantic.xml" app-name="sys_table_semantic" app-desc="业务表" delete-action="delete" description="业务表定义" class-name="cn.gwssi.gwaic.matedata.txn.TxnSysTableSemantic" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_table_semantic.jsp" txn-code="30402001" description="查询业务表列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加业务表" path="insert-sys_table_semantic.jsp" description="增加业务表" func-name="addRecord"/>
	    <button type="button" name="修改业务表" path="/txn30402004.do" description="修改业务表" func-name="updateRecord"/>
	    <button type="button" name="删除业务表" path="/txn30402005.do" description="删除业务表" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn30402001.do" description="查询业务表信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_table_semantic.jsp" txn-code="30402003" description="增加业务表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn30402003.do" description="保存业务表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn30402003.do" description="保存业务表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn30402001.do" description="保存业务表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30402001.do" description="返回到业务表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_table_semantic.jsp" txn-code="30402002" description="修改业务表信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn30402001.do" description="修改业务表信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30402001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_table_semantic.jsp" txn-code="30402001" description="查询业务表列表">
	    <forward error-code="success" path="query-sys_table_semantic.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_table_semantic.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_table_semantic.jsp" txn-code="30402002" description="修改业务表信息">
	    <forward error-code="success" path="update-sys_table_semantic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_table_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_table_semantic.jsp" txn-code="30402003" description="增加业务表信息">
	    <forward error-code="success" path="insert-sys_table_semantic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_table_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_table_semantic.jsp" txn-code="30402004" description="查询业务表用于修改">
	    <forward error-code="success" path="update-sys_table_semantic.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_table_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_table_semantic.jsp" txn-code="30402005" description="删除业务表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.gwaic.matedata.vo.SysTableSemanticContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.matedata.vo.VoSysTableSemantic" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.matedata.vo.VoSysTableSemanticSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.matedata.vo.VoSysTableSemanticPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
