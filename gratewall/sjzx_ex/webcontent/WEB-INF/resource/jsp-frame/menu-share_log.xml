<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="share_log.xml" app-path="/page/sharelog/" app-name="share_log" description="共享日志" template="single-table.xml"/>
  <table name="share_log.xml" app-name="share_log" app-desc="共享日志" delete-action="delete" description="共享日志" class-name="com.gwssi.sharelog.txn.TxnShareLog" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-share_log.jsp" txn-code="601011" description="查询共享日志列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加共享日志" path="insert-share_log.jsp" description="增加共享日志" func-name="addRecord"/>
	    <button type="button" name="修改共享日志" path="/txn601014.do" description="修改共享日志" func-name="updateRecord"/>
	    <button type="button" name="删除共享日志" path="/txn601015.do" description="删除共享日志" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn601011.do" description="查询共享日志信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-share_log.jsp" txn-code="601013" description="增加共享日志信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn601013.do" description="保存共享日志信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn601013.do" description="保存共享日志信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn601011.do" description="保存共享日志信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn601011.do" description="返回到共享日志明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-share_log.jsp" txn-code="601012" description="修改共享日志信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn601011.do" description="修改共享日志信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn601011.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-share_log.jsp" txn-code="601011" description="查询共享日志列表">
	    <forward error-code="success" path="query-share_log.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-share_log.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-share_log.jsp" txn-code="601012" description="修改共享日志信息">
	    <forward error-code="success" path="update-share_log.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-share_log.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-share_log.jsp" txn-code="601013" description="增加共享日志信息">
	    <forward error-code="success" path="insert-share_log.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-share_log.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_log.jsp" txn-code="601014" description="查询共享日志用于修改">
	    <forward error-code="success" path="update-share_log.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-share_log.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_log.jsp" txn-code="601015" description="删除共享日志信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.sharelog.vo.ShareLogContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.sharelog.vo.VoShareLog" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.sharelog.vo.VoShareLogSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.sharelog.vo.VoShareLogPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
