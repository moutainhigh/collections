<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="share_service.xml" app-path="/page/share/service/" app-name="share_service" description="共享服务的服务表" template="single-table.xml"/>
  <table name="share_service.xml" app-name="share_service" app-desc="服务表" delete-action="delete" description="共享服务的服务表" class-name="com.gwssi.share.service.txn.TxnShareService" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-share_service.jsp" txn-code="40200001" description="查询服务表列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加服务表" path="insert-share_service.jsp" description="增加服务表" func-name="addRecord"/>
	    <button type="button" name="修改服务表" path="/txn40200004.do" description="修改服务表" func-name="updateRecord"/>
	    <button type="button" name="删除服务表" path="/txn40200005.do" description="删除服务表" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn40200001.do" description="查询服务表信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-share_service.jsp" txn-code="40200003" description="增加服务表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn40200003.do" description="保存服务表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn40200003.do" description="保存服务表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn40200001.do" description="保存服务表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40200001.do" description="返回到服务表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-share_service.jsp" txn-code="40200002" description="修改服务表信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn40200001.do" description="修改服务表信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40200001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-share_service.jsp" txn-code="40200001" description="查询服务表列表">
	    <forward error-code="success" path="query-share_service.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-share_service.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-share_service.jsp" txn-code="40200002" description="修改服务表信息">
	    <forward error-code="success" path="update-share_service.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-share_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-share_service.jsp" txn-code="40200003" description="增加服务表信息">
	    <forward error-code="success" path="insert-share_service.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-share_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_service.jsp" txn-code="40200004" description="查询服务表用于修改">
	    <forward error-code="success" path="update-share_service.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-share_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_service.jsp" txn-code="40200005" description="删除服务表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.share.service.vo.ShareServiceContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.share.service.vo.VoShareService" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.share.service.vo.VoShareServiceSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.share.service.vo.VoShareServicePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
