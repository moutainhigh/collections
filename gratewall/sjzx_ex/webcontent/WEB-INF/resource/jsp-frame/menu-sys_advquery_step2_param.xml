	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.SysAdvqueryStep2ParamContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.VoSysAdvqueryStep2Param" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.VoSysAdvqueryStep2ParamSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.VoSysAdvqueryStep2ParamPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
