  <struts name="sys_advquery_step2_param.xml" app-path="/dw/aic/gjcx/cxtjdy/step2/" app-name="sys_advquery_step2_param" description="自定义查询步骤二" template="single-table.xml"/>
  <table name="sys_advquery_step2_param.xml" app-name="sys_advquery_step2_param" app-desc="自定义查询步骤二" delete-action="delete" description="自定义查询步骤二" class-name="com.gwssi.dw.component.commonquery.txn.TxnSysAdvqueryStep2Param" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_advquery_step2_param.jsp" txn-code="6022301" description="查询自定义查询步骤二列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加自定义查询步骤二" path="insert-sys_advquery_step2_param.jsp" description="增加自定义查询步骤二" func-name="addRecord"/>
	    <button type="button" name="修改自定义查询步骤二" path="/txn6022304.do" description="修改自定义查询步骤二" func-name="updateRecord"/>
	    <button type="button" name="删除自定义查询步骤二" path="/txn6022305.do" description="删除自定义查询步骤二" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn6022301.do" description="查询自定义查询步骤二信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_advquery_step2_param.jsp" txn-code="6022303" description="增加自定义查询步骤二信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn6022303.do" description="保存自定义查询步骤二信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn6022303.do" description="保存自定义查询步骤二信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn6022301.do" description="保存自定义查询步骤二信息并返回" func-name="saveAndExit"/>
