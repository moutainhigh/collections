	    <button type="button" name="返 回" path="/txn6022301.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_advquery_step2_param.jsp" txn-code="6022301" description="查询自定义查询步骤二列表">
	    <forward error-code="success" path="query-sys_advquery_step2_param.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_advquery_step2_param.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_advquery_step2_param.jsp" txn-code="6022302" description="修改自定义查询步骤二信息">
	    <forward error-code="success" path="update-sys_advquery_step2_param.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_advquery_step2_param.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_advquery_step2_param.jsp" txn-code="6022303" description="增加自定义查询步骤二信息">
	    <forward error-code="success" path="insert-sys_advquery_step2_param.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_advquery_step2_param.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_advquery_step2_param.jsp" txn-code="6022304" description="查询自定义查询步骤二用于修改">
	    <forward error-code="success" path="update-sys_advquery_step2_param.jsp" description="成功时返回记录"/>
