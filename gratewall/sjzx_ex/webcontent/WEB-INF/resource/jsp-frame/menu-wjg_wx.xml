<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="wjg_wx.xml" app-path="/wjg/rwgl/wxrw/" app-name="wjg_wx" description="文献任务设置" template="single-table.xml"/>
  <table name="wjg_wx.xml" app-name="wjg_wx" app-desc="任务" description="文献任务设置" class-name="cn.gwssi.csdb.wjg.txn.TxnWjgWx" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-wjg_wx.jsp" txn-code="202021" description="查询任务列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加任务" path="insert-wjg_wx.jsp" description="增加任务" func-name="addRecord"/>
	    <button type="button" name="修改任务" path="/txn202024.do" description="修改任务" func-name="updateRecord"/>
	    <button type="button" name="删除任务" path="/txn202025.do" description="删除任务" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn202021.do" description="查询任务信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-wjg_wx.jsp" txn-code="202023" description="增加任务信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn202023.do" description="保存任务信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn202023.do" description="保存任务信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn202021.do" description="保存任务信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn202021.do" description="返回到任务明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-wjg_wx.jsp" txn-code="202022" description="修改任务信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn202021.do" description="修改任务信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn202021.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>
	  <txn input="query-wjg_wx.jsp" txn-code="202021" description="查询任务列表">
	    <forward error-code="success" path="query-wjg_wx.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-wjg_wx.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-wjg_wx.jsp" txn-code="202022" description="修改任务信息">
	    <forward error-code="success" path="update-wjg_wx.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-wjg_wx.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-wjg_wx.jsp" txn-code="202023" description="增加任务信息">
	    <forward error-code="success" path="insert-wjg_wx.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-wjg_wx.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_wx.jsp" txn-code="202024" description="查询任务用于修改">
	    <forward error-code="success" path="update-wjg_wx.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-wjg_wx.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_wx.jsp" txn-code="202025" description="删除任务信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.WjgWxContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWx" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWxSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWxPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
