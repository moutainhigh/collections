<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="wjg_wjml.xml" app-path="/wjg/mlgl/wjml/" app-name="wjg_wjml" description="文件目录" template="single-table.xml"/>
  <table name="wjg_wjml.xml" app-name="wjg_wjml" app-desc="文件目录" description="文件目录" class-name="cn.gwssi.csdb.wjg.txn.TxnWjgWjml" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-wjg_wjml.jsp" txn-code="2010111" description="查询文件目录列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加文件目录" path="insert-wjg_wjml.jsp" description="增加文件目录" func-name="addRecord"/>
	    <button type="button" name="修改文件目录" path="/txn2010114.do" description="修改文件目录" func-name="updateRecord"/>
	    <button type="button" name="删除文件目录" path="/txn2010115.do" description="删除文件目录" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn2010111.do" description="查询文件目录信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-wjg_wjml.jsp" txn-code="2010113" description="增加文件目录信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn2010113.do" description="保存文件目录信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn2010113.do" description="保存文件目录信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn2010111.do" description="保存文件目录信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn2010111.do" description="返回到文件目录明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-wjg_wjml.jsp" txn-code="2010112" description="修改文件目录信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn2010111.do" description="修改文件目录信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn2010111.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>
	  <txn input="query-wjg_wjml.jsp" txn-code="2010111" description="查询文件目录列表">
	    <forward error-code="success" path="query-wjg_wjml.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-wjg_wjml.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-wjg_wjml.jsp" txn-code="2010112" description="修改文件目录信息">
	    <forward error-code="success" path="update-wjg_wjml.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-wjg_wjml.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-wjg_wjml.jsp" txn-code="2010113" description="增加文件目录信息">
	    <forward error-code="success" path="insert-wjg_wjml.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-wjg_wjml.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_wjml.jsp" txn-code="2010114" description="查询文件目录用于修改">
	    <forward error-code="success" path="update-wjg_wjml.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-wjg_wjml.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_wjml.jsp" txn-code="2010115" description="删除文件目录信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.WjgWjmlContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjml" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjmlSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjmlPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
