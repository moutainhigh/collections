	  </txn>
	  <txn input="update-reg_bus_emp.jsp" txn-code="2001030002" description="修改雇员信息">
	    <forward error-code="success" path="update-reg_bus_emp.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-reg_bus_emp.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-reg_bus_emp.jsp" txn-code="2001030003" description="增加雇员信息">
	    <forward error-code="success" path="insert-reg_bus_emp.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-reg_bus_emp.jsp" description="错误时返回页面"/>
	  </txn>
