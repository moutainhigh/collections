	  <vo clazz-name="cn.gwssi.bjaic.bizqry.vo.VoRegBusEmp" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.bjaic.bizqry.vo.VoRegBusEmpSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.bjaic.bizqry.vo.VoRegBusEmpPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
