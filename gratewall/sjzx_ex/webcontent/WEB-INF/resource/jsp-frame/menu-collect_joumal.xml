<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="collect_joumal.xml" app-path="/page/log/" app-name="collect_joumal" description="采集日志查询" template="single-table.xml"/>
  <table name="collect_joumal.xml" app-name="collect_joumal" app-desc="采集日志" delete-action="delete" description="采集日志查询" class-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-collect_joumal.jsp" txn-code="602001" description="查询采集日志列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加采集日志" path="insert-collect_joumal.jsp" description="增加采集日志" func-name="addRecord"/>
	    <button type="button" name="修改采集日志" path="/txn602004.do" description="修改采集日志" func-name="updateRecord"/>
	    <button type="button" name="删除采集日志" path="/txn602005.do" description="删除采集日志" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn602001.do" description="查询采集日志信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-collect_joumal.jsp" txn-code="602003" description="增加采集日志信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn602003.do" description="保存采集日志信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn602003.do" description="保存采集日志信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn602001.do" description="保存采集日志信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn602001.do" description="返回到采集日志明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-collect_joumal.jsp" txn-code="602002" description="修改采集日志信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn602001.do" description="修改采集日志信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn602001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-collect_joumal.jsp" txn-code="602001" description="查询采集日志列表">
	    <forward error-code="success" path="query-collect_joumal.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-collect_joumal.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-collect_joumal.jsp" txn-code="602002" description="修改采集日志信息">
	    <forward error-code="success" path="update-collect_joumal.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-collect_joumal.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-collect_joumal.jsp" txn-code="602003" description="增加采集日志信息">
	    <forward error-code="success" path="insert-collect_joumal.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-collect_joumal.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-collect_joumal.jsp" txn-code="602004" description="查询采集日志用于修改">
	    <forward error-code="success" path="update-collect_joumal.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-collect_joumal.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-collect_joumal.jsp" txn-code="602005" description="删除采集日志信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.log.collectlog.vo.CollectJoumalContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.log.collectlog.vo.VoCollectJoumal" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.log.collectlog.vo.VoCollectJoumalSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.log.collectlog.vo.VoCollectJoumalPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
