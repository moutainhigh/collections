<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="share_interface.xml" app-path="/page/share/interfaces/" app-name="share_interface" description="共享服务-基础接口表" template="single-table.xml"/>
  <table name="share_interface.xml" app-name="share_interface" app-desc="共享服务基础接口配置" delete-action="delete" description="共享服务-基础接口表" class-name="com.gwssi.share.interfaces.txn.TxnShareInterface" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-share_interface.jsp" txn-code="401001" description="查询共享服务基础接口配置列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加共享服务基础接口配置" path="insert-share_interface.jsp" description="增加共享服务基础接口配置" func-name="addRecord"/>
	    <button type="button" name="修改共享服务基础接口配置" path="/txn401004.do" description="修改共享服务基础接口配置" func-name="updateRecord"/>
	    <button type="button" name="删除共享服务基础接口配置" path="/txn401005.do" description="删除共享服务基础接口配置" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn401001.do" description="查询共享服务基础接口配置信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-share_interface.jsp" txn-code="401003" description="增加共享服务基础接口配置信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn401003.do" description="保存共享服务基础接口配置信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn401003.do" description="保存共享服务基础接口配置信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn401001.do" description="保存共享服务基础接口配置信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn401001.do" description="返回到共享服务基础接口配置明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-share_interface.jsp" txn-code="401002" description="修改共享服务基础接口配置信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn401001.do" description="修改共享服务基础接口配置信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn401001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-share_interface.jsp" txn-code="401001" description="查询共享服务基础接口配置列表">
	    <forward error-code="success" path="query-share_interface.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-share_interface.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-share_interface.jsp" txn-code="401002" description="修改共享服务基础接口配置信息">
	    <forward error-code="success" path="update-share_interface.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-share_interface.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-share_interface.jsp" txn-code="401003" description="增加共享服务基础接口配置信息">
	    <forward error-code="success" path="insert-share_interface.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-share_interface.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_interface.jsp" txn-code="401004" description="查询共享服务基础接口配置用于修改">
	    <forward error-code="success" path="update-share_interface.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-share_interface.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_interface.jsp" txn-code="401005" description="删除共享服务基础接口配置信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.share.interfaces.vo.ShareInterfaceContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.share.interfaces.vo.VoShareInterface" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.share.interfaces.vo.VoShareInterfaceSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.share.interfaces.vo.VoShareInterfacePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
