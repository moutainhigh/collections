<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="mon_mov_dom.xml" app-path="/dw/aic/bj/reg/monmov/" app-name="mon_mov_dom" description="检查任务" template="single-table.xml"/>
  <table name="mon_mov_dom.xml" app-name="mon_mov_dom" app-desc="异地经营" delete-action="delete" description="检查任务" class-name="com.gwssi.dw.aic.bj.newmon.mov.txn.TxnMonMovDom" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-mon_mov_dom.jsp" txn-code="81120001" description="查询异地经营列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加异地经营" path="insert-mon_mov_dom.jsp" description="增加异地经营" func-name="addRecord"/>
	    <button type="button" name="修改异地经营" path="/txn81120004.do" description="修改异地经营" func-name="updateRecord"/>
	    <button type="button" name="删除异地经营" path="/txn81120005.do" description="删除异地经营" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn81120001.do" description="查询异地经营信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-mon_mov_dom.jsp" txn-code="81120003" description="增加异地经营信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn81120003.do" description="保存异地经营信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn81120003.do" description="保存异地经营信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn81120001.do" description="保存异地经营信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn81120001.do" description="返回到异地经营明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-mon_mov_dom.jsp" txn-code="81120002" description="修改异地经营信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn81120001.do" description="修改异地经营信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn81120001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-mon_mov_dom.jsp" txn-code="81120001" description="查询异地经营列表">
	    <forward error-code="success" path="query-mon_mov_dom.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-mon_mov_dom.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-mon_mov_dom.jsp" txn-code="81120002" description="修改异地经营信息">
	    <forward error-code="success" path="update-mon_mov_dom.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-mon_mov_dom.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-mon_mov_dom.jsp" txn-code="81120003" description="增加异地经营信息">
	    <forward error-code="success" path="insert-mon_mov_dom.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-mon_mov_dom.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-mon_mov_dom.jsp" txn-code="81120004" description="查询异地经营用于修改">
	    <forward error-code="success" path="update-mon_mov_dom.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-mon_mov_dom.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-mon_mov_dom.jsp" txn-code="81120005" description="删除异地经营信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.newmon.mov.vo.MonMovDomContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.newmon.mov.vo.VoMonMovDom" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.newmon.mov.vo.VoMonMovDomSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.newmon.mov.vo.VoMonMovDomPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
