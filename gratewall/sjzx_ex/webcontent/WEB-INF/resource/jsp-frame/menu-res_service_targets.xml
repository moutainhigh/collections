<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="res_service_targets.xml" app-path="/page/resource/svrobj/" app-name="res_service_targets" description="服务对象表" template="single-table.xml"/>
  <table name="res_service_targets.xml" app-name="res_service_targets" app-desc="对象" delete-action="delete" description="服务对象表" class-name="com.gwssi.resource.svrobj.txn.TxnResServiceTargets" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-res_service_targets.jsp" txn-code="201001" description="查询对象列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加对象" path="insert-res_service_targets.jsp" description="增加对象" func-name="addRecord"/>
	    <button type="button" name="修改对象" path="/txn201004.do" description="修改对象" func-name="updateRecord"/>
	    <button type="button" name="删除对象" path="/txn201005.do" description="删除对象" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn201001.do" description="查询对象信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-res_service_targets.jsp" txn-code="201003" description="增加对象信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn201003.do" description="保存对象信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn201003.do" description="保存对象信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn201001.do" description="保存对象信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn201001.do" description="返回到对象明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-res_service_targets.jsp" txn-code="201002" description="修改对象信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn201001.do" description="修改对象信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn201001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-res_service_targets.jsp" txn-code="201001" description="查询对象列表">
	    <forward error-code="success" path="query-res_service_targets.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-res_service_targets.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-res_service_targets.jsp" txn-code="201002" description="修改对象信息">
	    <forward error-code="success" path="update-res_service_targets.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-res_service_targets.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-res_service_targets.jsp" txn-code="201003" description="增加对象信息">
	    <forward error-code="success" path="insert-res_service_targets.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-res_service_targets.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_service_targets.jsp" txn-code="201004" description="查询对象用于修改">
	    <forward error-code="success" path="update-res_service_targets.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-res_service_targets.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_service_targets.jsp" txn-code="201005" description="删除对象信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.ResServiceTargetsContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.VoResServiceTargets" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.VoResServiceTargetsSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.VoResServiceTargetsPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
