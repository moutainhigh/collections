<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="wjg_wj.xml" app-path="/wjg/rwgl/wjrw/" app-name="wjg_wj" description="文件上传" template="single-table.xml"/>
  <table name="wjg_wj.xml" app-name="wjg_wj" app-desc="文件任务" description="文件上传" class-name="cn.gwssi.csdb.wjg.txn.TxnWjgWj" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-wjg_wj.jsp" txn-code="6100211" description="上传文件列表" template="single/single-table-query.jsp">
	    <button type="button" name="查 询" path="/txn6100211.do" description="上传文件列表" func-name="submit"/>
	  </jsp>

	 
	  <txn input="wjsc.jsp" txn-code="6100211" description="上传文件列表">
	    <forward error-code="success" path="wjsc.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="wjsc.jsp" description="错误时返回错误信息"/>
	  </txn>
	 
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.WjgWjContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWj" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
