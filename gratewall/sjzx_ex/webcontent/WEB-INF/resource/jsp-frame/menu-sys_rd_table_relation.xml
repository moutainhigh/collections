<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_rd_table_relation.xml" app-path="/dw/rd/metadata/datastructure/claim/relation/" app-name="sys_rd_table_relation" description="表关系" template="single-table.xml"/>
  <table name="sys_rd_table_relation.xml" app-name="sys_rd_table_relation" app-desc="表关系" delete-action="delete" description="表关系" class-name="cn.gwssi.dw.rd.metadata.txn.TxnSysRdTableRelation" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_rd_table_relation.jsp" txn-code="80002601" description="查询表关系列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加表关系" path="insert-sys_rd_table_relation.jsp" description="增加表关系" func-name="addRecord"/>
	    <button type="button" name="修改表关系" path="/txn80002604.do" description="修改表关系" func-name="updateRecord"/>
	    <button type="button" name="删除表关系" path="/txn80002605.do" description="删除表关系" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn80002601.do" description="查询表关系信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_rd_table_relation.jsp" txn-code="80002603" description="增加表关系信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn80002603.do" description="保存表关系信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn80002603.do" description="保存表关系信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn80002601.do" description="保存表关系信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80002601.do" description="返回到表关系明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_rd_table_relation.jsp" txn-code="80002602" description="修改表关系信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn80002601.do" description="修改表关系信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80002601.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_rd_table_relation.jsp" txn-code="80002601" description="查询表关系列表">
	    <forward error-code="success" path="query-sys_rd_table_relation.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_rd_table_relation.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_rd_table_relation.jsp" txn-code="80002602" description="修改表关系信息">
	    <forward error-code="success" path="update-sys_rd_table_relation.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_rd_table_relation.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_rd_table_relation.jsp" txn-code="80002603" description="增加表关系信息">
	    <forward error-code="success" path="insert-sys_rd_table_relation.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_rd_table_relation.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_table_relation.jsp" txn-code="80002604" description="查询表关系用于修改">
	    <forward error-code="success" path="update-sys_rd_table_relation.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_rd_table_relation.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_table_relation.jsp" txn-code="80002605" description="删除表关系信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.SysRdTableRelationContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdTableRelation" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdTableRelationSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdTableRelationPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
