<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_system_semantic.xml" app-path="/ysjgl/jcxx//ywxtdy/" app-name="sys_system_semantic" description="业务系统定义" template="single-table.xml"/>
  <table name="sys_system_semantic.xml" app-name="sys_system_semantic" app-desc="系统" delete-action="delete" description="业务系统定义" class-name="cn.gwssi.gwaic.metadata.txn.TxnSysSystemSemantic" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_system_semantic.jsp" txn-code="30401001" description="查询系统列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加系统" path="insert-sys_system_semantic.jsp" description="增加系统" func-name="addRecord"/>
	    <button type="button" name="修改系统" path="/txn30401004.do" description="修改系统" func-name="updateRecord"/>
	    <button type="button" name="删除系统" path="/txn30401005.do" description="删除系统" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn30401001.do" description="查询系统信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_system_semantic.jsp" txn-code="30401003" description="增加系统信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn30401003.do" description="保存系统信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn30401003.do" description="保存系统信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn30401001.do" description="保存系统信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30401001.do" description="返回到系统明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_system_semantic.jsp" txn-code="30401002" description="修改系统信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn30401001.do" description="修改系统信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30401001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_system_semantic.jsp" txn-code="30401001" description="查询系统列表">
	    <forward error-code="success" path="query-sys_system_semantic.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_system_semantic.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_system_semantic.jsp" txn-code="30401002" description="修改系统信息">
	    <forward error-code="success" path="update-sys_system_semantic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_system_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_system_semantic.jsp" txn-code="30401003" description="增加系统信息">
	    <forward error-code="success" path="insert-sys_system_semantic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_system_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_system_semantic.jsp" txn-code="30401004" description="查询系统用于修改">
	    <forward error-code="success" path="update-sys_system_semantic.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_system_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_system_semantic.jsp" txn-code="30401005" description="删除系统信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.gwaic.metadata.vo.SysSystemSemanticContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.metadata.vo.VoSysSystemSemantic" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.metadata.vo.VoSysSystemSemanticSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.metadata.vo.VoSysSystemSemanticPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
