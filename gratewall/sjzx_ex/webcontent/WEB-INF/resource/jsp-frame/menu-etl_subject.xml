<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="etl_subject.xml" app-path="/page/collect/etl/" app-name="etl_subject" description="etl任务表" template="single-table.xml"/>
  <table name="etl_subject.xml" app-name="etl_subject" app-desc="etl任务" delete-action="delete" description="etl任务表" class-name="com.gwssi.collect.etl.txn.TxnEtlSubject" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-etl_subject.jsp" txn-code="30300001" description="查询etl任务列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加etl任务" path="insert-etl_subject.jsp" description="增加etl任务" func-name="addRecord"/>
	    <button type="button" name="修改etl任务" path="/txn30300004.do" description="修改etl任务" func-name="updateRecord"/>
	    <button type="button" name="删除etl任务" path="/txn30300005.do" description="删除etl任务" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn30300001.do" description="查询etl任务信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-etl_subject.jsp" txn-code="30300003" description="增加etl任务信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn30300003.do" description="保存etl任务信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn30300003.do" description="保存etl任务信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn30300001.do" description="保存etl任务信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30300001.do" description="返回到etl任务明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-etl_subject.jsp" txn-code="30300002" description="修改etl任务信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn30300001.do" description="修改etl任务信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30300001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-etl_subject.jsp" txn-code="30300001" description="查询etl任务列表">
	    <forward error-code="success" path="query-etl_subject.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-etl_subject.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-etl_subject.jsp" txn-code="30300002" description="修改etl任务信息">
	    <forward error-code="success" path="update-etl_subject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-etl_subject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-etl_subject.jsp" txn-code="30300003" description="增加etl任务信息">
	    <forward error-code="success" path="insert-etl_subject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-etl_subject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-etl_subject.jsp" txn-code="30300004" description="查询etl任务用于修改">
	    <forward error-code="success" path="update-etl_subject.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-etl_subject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-etl_subject.jsp" txn-code="30300005" description="删除etl任务信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.collect.etl.vo.EtlSubjectContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.collect.etl.vo.VoEtlSubject" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.collect.etl.vo.VoEtlSubjectSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.collect.etl.vo.VoEtlSubjectPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
