<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="dataobject.xml" app-path="/xtgl/qxgl/sjqxlxgl/" app-name="dataobject" description="数据权限类型管理" template="single-table.xml"/>
  <table name="dataobject.xml" app-name="dataobject" app-desc="数据权限类型管理" delete-action="delete" description="数据权限类型管理" class-name="cn.gwssi.csdb.xtgl.txn.TxnDataObject" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-dataobject.jsp" txn-code="103011" description="查询数据权限类型管理列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加数据权限类型管理" path="insert-dataobject.jsp" description="增加数据权限类型管理" func-name="addRecord"/>
	    <button type="button" name="修改数据权限类型管理" path="/txn103014.do" description="修改数据权限类型管理" func-name="updateRecord"/>
	    <button type="button" name="删除数据权限类型管理" path="/txn103015.do" description="删除数据权限类型管理" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn103011.do" description="查询数据权限类型管理信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-dataobject.jsp" txn-code="103013" description="增加数据权限类型管理信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn103013.do" description="保存数据权限类型管理信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn103013.do" description="保存数据权限类型管理信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn103011.do" description="保存数据权限类型管理信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn103011.do" description="返回到数据权限类型管理明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-dataobject.jsp" txn-code="103012" description="修改数据权限类型管理信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn103011.do" description="修改数据权限类型管理信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn103011.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-dataobject.jsp" txn-code="103011" description="查询数据权限类型管理列表">
	    <forward error-code="success" path="query-dataobject.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-dataobject.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-dataobject.jsp" txn-code="103012" description="修改数据权限类型管理信息">
	    <forward error-code="success" path="update-dataobject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-dataobject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-dataobject.jsp" txn-code="103013" description="增加数据权限类型管理信息">
	    <forward error-code="success" path="insert-dataobject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-dataobject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-dataobject.jsp" txn-code="103014" description="查询数据权限类型管理用于修改">
	    <forward error-code="success" path="update-dataobject.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-dataobject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-dataobject.jsp" txn-code="103015" description="删除数据权限类型管理信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.DataobjectContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoDataobject" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoDataobjectSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoDataobjectPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
