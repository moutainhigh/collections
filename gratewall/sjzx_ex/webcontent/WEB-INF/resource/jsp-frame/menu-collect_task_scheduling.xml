	  </txn>
	  <txn input="update-collect_task_scheduling.jsp" txn-code="30801002" description="修改任务调度信息">
	    <forward error-code="success" path="update-collect_task_scheduling.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-collect_task_scheduling.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-collect_task_scheduling.jsp" txn-code="30801003" description="增加任务调度信息">
	    <forward error-code="success" path="insert-collect_task_scheduling.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-collect_task_scheduling.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-collect_task_scheduling.jsp" txn-code="30801004" description="查询任务调度用于修改">
	    <forward error-code="success" path="update-collect_task_scheduling.jsp" description="成功时返回记录"/>
