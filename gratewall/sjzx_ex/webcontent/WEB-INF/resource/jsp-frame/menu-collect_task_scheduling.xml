	    <button type="button" name="返 回" path="/txn30801001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-collect_task_scheduling.jsp" txn-code="30801001" description="查询任务调度列表">
	    <forward error-code="success" path="query-collect_task_scheduling.jsp" description="成功时返回列表"/>
