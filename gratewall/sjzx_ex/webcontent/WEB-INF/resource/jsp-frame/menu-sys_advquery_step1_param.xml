<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_advquery_step1_param.xml" app-path="/dw/aic/gjcx/cxtjdy/step1/" app-name="sys_advquery_step1_param" description="自定义查询步骤一" template="single-table.xml"/>
  <table name="sys_advquery_step1_param.xml" app-name="sys_advquery_step1_param" app-desc="自定义查询步骤一" delete-action="delete" description="自定义查询步骤一" class-name="com.gwssi.dw.component.commonquery.txn.TxnSysAdvqueryStep1Param" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_advquery_step1_param.jsp" txn-code="6022201" description="查询自定义查询步骤一列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加自定义查询步骤一" path="insert-sys_advquery_step1_param.jsp" description="增加自定义查询步骤一" func-name="addRecord"/>
	    <button type="button" name="修改自定义查询步骤一" path="/txn6022204.do" description="修改自定义查询步骤一" func-name="updateRecord"/>
	    <button type="button" name="删除自定义查询步骤一" path="/txn6022205.do" description="删除自定义查询步骤一" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn6022201.do" description="查询自定义查询步骤一信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_advquery_step1_param.jsp" txn-code="6022203" description="增加自定义查询步骤一信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn6022203.do" description="保存自定义查询步骤一信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn6022203.do" description="保存自定义查询步骤一信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn6022201.do" description="保存自定义查询步骤一信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn6022201.do" description="返回到自定义查询步骤一明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_advquery_step1_param.jsp" txn-code="6022202" description="修改自定义查询步骤一信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn6022201.do" description="修改自定义查询步骤一信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn6022201.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_advquery_step1_param.jsp" txn-code="6022201" description="查询自定义查询步骤一列表">
	    <forward error-code="success" path="query-sys_advquery_step1_param.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_advquery_step1_param.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_advquery_step1_param.jsp" txn-code="6022202" description="修改自定义查询步骤一信息">
	    <forward error-code="success" path="update-sys_advquery_step1_param.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_advquery_step1_param.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_advquery_step1_param.jsp" txn-code="6022203" description="增加自定义查询步骤一信息">
	    <forward error-code="success" path="insert-sys_advquery_step1_param.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_advquery_step1_param.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_advquery_step1_param.jsp" txn-code="6022204" description="查询自定义查询步骤一用于修改">
	    <forward error-code="success" path="update-sys_advquery_step1_param.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_advquery_step1_param.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_advquery_step1_param.jsp" txn-code="6022205" description="删除自定义查询步骤一信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.SysAdvqueryStep1ParamContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.VoSysAdvqueryStep1Param" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.VoSysAdvqueryStep1ParamSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.component.commonquery.vo.VoSysAdvqueryStep1ParamPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
