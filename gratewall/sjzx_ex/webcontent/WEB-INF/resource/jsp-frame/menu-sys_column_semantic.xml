<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_column_semantic.xml" app-path="/ysjgl/jcxx//ywbzdyydy/" app-name="sys_column_semantic" description="业务字段定义" template="single-table.xml"/>
  <table name="sys_column_semantic.xml" app-name="sys_column_semantic" app-desc="业务字段" delete-action="delete" description="业务字段定义" class-name="cn.gwssi.gwaic.metedata.txn.TxnSysColumnSemantic" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_column_semantic.jsp" txn-code="30403001" description="查询业务字段列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加业务字段" path="insert-sys_column_semantic.jsp" description="增加业务字段" func-name="addRecord"/>
	    <button type="button" name="修改业务字段" path="/txn30403004.do" description="修改业务字段" func-name="updateRecord"/>
	    <button type="button" name="删除业务字段" path="/txn30403005.do" description="删除业务字段" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn30403001.do" description="查询业务字段信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_column_semantic.jsp" txn-code="30403003" description="增加业务字段信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn30403003.do" description="保存业务字段信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn30403003.do" description="保存业务字段信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn30403001.do" description="保存业务字段信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30403001.do" description="返回到业务字段明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_column_semantic.jsp" txn-code="30403002" description="修改业务字段信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn30403001.do" description="修改业务字段信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30403001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_column_semantic.jsp" txn-code="30403001" description="查询业务字段列表">
	    <forward error-code="success" path="query-sys_column_semantic.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_column_semantic.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_column_semantic.jsp" txn-code="30403002" description="修改业务字段信息">
	    <forward error-code="success" path="update-sys_column_semantic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_column_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_column_semantic.jsp" txn-code="30403003" description="增加业务字段信息">
	    <forward error-code="success" path="insert-sys_column_semantic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_column_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_column_semantic.jsp" txn-code="30403004" description="查询业务字段用于修改">
	    <forward error-code="success" path="update-sys_column_semantic.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_column_semantic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_column_semantic.jsp" txn-code="30403005" description="删除业务字段信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.gwaic.metedata.vo.SysColumnSemanticContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.metedata.vo.VoSysColumnSemantic" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.metedata.vo.VoSysColumnSemanticSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.gwaic.metedata.vo.VoSysColumnSemanticPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
