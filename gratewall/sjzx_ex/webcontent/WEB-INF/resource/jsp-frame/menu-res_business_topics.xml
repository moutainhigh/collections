<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="res_business_topics.xml" app-path="/page/resource/share/" app-name="res_business_topics" description="业务主题表" template="single-table.xml"/>
  <table name="res_business_topics.xml" app-name="res_business_topics" app-desc="共享主题" delete-action="delete" description="业务主题表" class-name="com.gwssi.resource.share.txn.TxnResBusinessTopics" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-res_business_topics.jsp" txn-code="203011" description="查询共享主题列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加共享主题" path="insert-res_business_topics.jsp" description="增加共享主题" func-name="addRecord"/>
	    <button type="button" name="修改共享主题" path="/txn203014.do" description="修改共享主题" func-name="updateRecord"/>
	    <button type="button" name="删除共享主题" path="/txn203015.do" description="删除共享主题" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn203011.do" description="查询共享主题信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-res_business_topics.jsp" txn-code="203013" description="增加共享主题信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn203013.do" description="保存共享主题信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn203013.do" description="保存共享主题信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn203011.do" description="保存共享主题信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn203011.do" description="返回到共享主题明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-res_business_topics.jsp" txn-code="203012" description="修改共享主题信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn203011.do" description="修改共享主题信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn203011.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-res_business_topics.jsp" txn-code="203011" description="查询共享主题列表">
	    <forward error-code="success" path="query-res_business_topics.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-res_business_topics.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-res_business_topics.jsp" txn-code="203012" description="修改共享主题信息">
	    <forward error-code="success" path="update-res_business_topics.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-res_business_topics.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-res_business_topics.jsp" txn-code="203013" description="增加共享主题信息">
	    <forward error-code="success" path="insert-res_business_topics.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-res_business_topics.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_business_topics.jsp" txn-code="203014" description="查询共享主题用于修改">
	    <forward error-code="success" path="update-res_business_topics.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-res_business_topics.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_business_topics.jsp" txn-code="203015" description="删除共享主题信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.resource.share.vo.ResBusinessTopicsContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.resource.share.vo.VoResBusinessTopics" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.resource.share.vo.VoResBusinessTopicsSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.resource.share.vo.VoResBusinessTopicsPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
