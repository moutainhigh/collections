	    <button type="button" name="查 询" path="/txn60181201.do" description="查询良好信息信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-ad_cred_nice.jsp" txn-code="60181203" description="增加良好信息信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60181203.do" description="保存良好信息信息" func-name="saveRecord"/>
