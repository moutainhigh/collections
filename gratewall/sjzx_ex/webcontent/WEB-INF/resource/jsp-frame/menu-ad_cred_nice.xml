	  <jsp page-name="query-ad_cred_nice.jsp" txn-code="60181201" description="查询良好信息列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加良好信息" path="insert-ad_cred_nice.jsp" description="增加良好信息" func-name="addRecord"/>
	    <button type="button" name="修改良好信息" path="/txn60181204.do" description="修改良好信息" func-name="updateRecord"/>
