	  </jsp>
	  <jsp page-name="update-ad_cred_nice.jsp" txn-code="60181202" description="修改良好信息信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60181201.do" description="修改良好信息信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60181201.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-ad_cred_nice.jsp" txn-code="60181201" description="查询良好信息列表">
	    <forward error-code="success" path="query-ad_cred_nice.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-ad_cred_nice.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-ad_cred_nice.jsp" txn-code="60181202" description="修改良好信息信息">
	    <forward error-code="success" path="update-ad_cred_nice.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-ad_cred_nice.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-ad_cred_nice.jsp" txn-code="60181203" description="增加良好信息信息">
	    <forward error-code="success" path="insert-ad_cred_nice.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-ad_cred_nice.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-ad_cred_nice.jsp" txn-code="60181204" description="查询良好信息用于修改">
	    <forward error-code="success" path="update-ad_cred_nice.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-ad_cred_nice.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-ad_cred_nice.jsp" txn-code="60181205" description="删除良好信息信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.AdCredNiceContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.VoAdCredNice" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.VoAdCredNiceSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.VoAdCredNicePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
