<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="first_page_query_sl.xml" app-path="/page/log/" app-name="first_page_query_" description="系统日志" template="single-table.xml"/>
  <table name="first_page_query_sl.xml" app-name="first_page_query" app-desc="first_page_query" delete-action="delete" description="系统日志" class-name="com.gwssi.log.systemlog.txn.TxnFirstPageQuery" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-first_page_query_systemlog.jsp" txn-code="601011" description="查询系统日志列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加系统日志" path="insert-first_page_query_systemlog.jsp" description="增加系统日志" func-name="addRecord"/>
	    <button type="button" name="修改系统日志" path="/txn601014.do" description="修改系统日志" func-name="updateRecord"/>
	    <button type="button" name="删除系统日志" path="/txn601015.do" description="删除系统日志" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn601011.do" description="查询系统日志信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-first_page_query_systemlog.jsp" txn-code="601013" description="增加系统日志信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn601013.do" description="保存系统日志信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn601013.do" description="保存系统日志信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn601011.do" description="保存系统日志信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn601011.do" description="返回到系统日志明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-first_page_query_systemlog.jsp" txn-code="601012" description="修改系统日志信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn601011.do" description="修改系统日志信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn601011.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-first_page_query_systemlog.jsp" txn-code="601011" description="查询系统日志列表">
	    <forward error-code="success" path="query-first_page_query_systemlog.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-first_page_query_systemlog.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-first_page_query_systemlog.jsp" txn-code="601012" description="修改系统日志信息">
	    <forward error-code="success" path="update-first_page_query_systemlog.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-first_page_query_systemlog.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-first_page_query_systemlog.jsp" txn-code="601013" description="增加系统日志信息">
	    <forward error-code="success" path="insert-first_page_query_systemlog.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-first_page_query_systemlog.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-first_page_query_systemlog.jsp" txn-code="601014" description="查询系统日志用于修改">
	    <forward error-code="success" path="update-first_page_query_systemlog.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-first_page_query_systemlog.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-first_page_query_systemlog.jsp" txn-code="601015" description="删除系统日志信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.log.systemlog.vo.FirstPageQuerySystemlogContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.log.systemlog.vo.VoFirstPageQuerySystemlog" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.log.systemlog.vo.VoFirstPageQuerySystemlogSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	 <vo clazz-name="com.gwssi.log.systemlog.vo.VoFirstPageQuery_sl" description="" template="single-table-vo-select-key.java"/>
	 <vo clazz-name="com.gwssi.log.systemlog.vo.VoFirstPageQuery" description="" template="single-table-vo-select-key.java"/>
	 
	  <vo clazz-name="com.gwssi.log.systemlog.vo.VoFirstPageQuerySystemlogPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
