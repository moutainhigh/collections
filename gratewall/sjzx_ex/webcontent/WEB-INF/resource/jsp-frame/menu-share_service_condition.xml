<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="share_service_condition.xml" app-path="/page/share/service/" app-name="share_service_condition" description="服务条件表" template="single-table.xml"/>
  <table name="share_service_condition.xml" app-name="share_service_condition" app-desc="服务查询条件" delete-action="delete" description="服务条件表" class-name="com.gwssi.share.service.txn.TxnShareServiceCondition" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-share_service_condition.jsp" txn-code="40210001" description="查询服务查询条件列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加服务查询条件" path="insert-share_service_condition.jsp" description="增加服务查询条件" func-name="addRecord"/>
	    <button type="button" name="修改服务查询条件" path="/txn40210004.do" description="修改服务查询条件" func-name="updateRecord"/>
	    <button type="button" name="删除服务查询条件" path="/txn40210005.do" description="删除服务查询条件" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn40210001.do" description="查询服务查询条件信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-share_service_condition.jsp" txn-code="40210003" description="增加服务查询条件信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn40210003.do" description="保存服务查询条件信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn40210003.do" description="保存服务查询条件信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn40210001.do" description="保存服务查询条件信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40210001.do" description="返回到服务查询条件明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-share_service_condition.jsp" txn-code="40210002" description="修改服务查询条件信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn40210001.do" description="修改服务查询条件信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40210001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-share_service_condition.jsp" txn-code="40210001" description="查询服务查询条件列表">
	    <forward error-code="success" path="query-share_service_condition.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-share_service_condition.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-share_service_condition.jsp" txn-code="40210002" description="修改服务查询条件信息">
	    <forward error-code="success" path="update-share_service_condition.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-share_service_condition.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-share_service_condition.jsp" txn-code="40210003" description="增加服务查询条件信息">
	    <forward error-code="success" path="insert-share_service_condition.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-share_service_condition.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_service_condition.jsp" txn-code="40210004" description="查询服务查询条件用于修改">
	    <forward error-code="success" path="update-share_service_condition.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-share_service_condition.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-share_service_condition.jsp" txn-code="40210005" description="删除服务查询条件信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.share.service.vo.ShareServiceConditionContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.share.service.vo.VoShareServiceCondition" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.share.service.vo.VoShareServiceConditionSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.share.service.vo.VoShareServiceConditionPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
