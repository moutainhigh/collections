<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="work_status.xml" app-path="/sa/" app-name="work_status" description="tttt" template="single-table.xml"/>
  <table name="work_status.xml" app-name="work_status" app-desc="待办任务" delete-action="delete" description="tttt" class-name="com.gwssi.dw.aic.bj.homepage.txn.TxnWorkStatus" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-work_status.jsp" txn-code="54000001" description="查询待办任务列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加待办任务" path="insert-work_status.jsp" description="增加待办任务" func-name="addRecord"/>
	    <button type="button" name="修改待办任务" path="/txn54000004.do" description="修改待办任务" func-name="updateRecord"/>
	    <button type="button" name="删除待办任务" path="/txn54000005.do" description="删除待办任务" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn54000001.do" description="查询待办任务信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-work_status.jsp" txn-code="54000003" description="增加待办任务信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn54000003.do" description="保存待办任务信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn54000003.do" description="保存待办任务信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn54000001.do" description="保存待办任务信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn54000001.do" description="返回到待办任务明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-work_status.jsp" txn-code="54000002" description="修改待办任务信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn54000001.do" description="修改待办任务信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn54000001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-work_status.jsp" txn-code="54000001" description="查询待办任务列表">
	    <forward error-code="success" path="query-work_status.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-work_status.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-work_status.jsp" txn-code="54000002" description="修改待办任务信息">
	    <forward error-code="success" path="update-work_status.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-work_status.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-work_status.jsp" txn-code="54000003" description="增加待办任务信息">
	    <forward error-code="success" path="insert-work_status.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-work_status.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-work_status.jsp" txn-code="54000004" description="查询待办任务用于修改">
	    <forward error-code="success" path="update-work_status.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-work_status.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-work_status.jsp" txn-code="54000005" description="删除待办任务信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.homepage.vo.WorkStatusContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.homepage.vo.VoWorkStatus" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.homepage.vo.VoWorkStatusSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.homepage.vo.VoWorkStatusPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
