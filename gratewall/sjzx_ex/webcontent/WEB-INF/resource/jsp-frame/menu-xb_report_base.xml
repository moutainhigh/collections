<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="xb_report_base.xml" app-path="/dw/aic/bj/xb/jbxx/" app-name="xb_report_base" description="举报信息" template="single-table.xml"/>
  <table name="xb_report_base.xml" app-name="xb_report_base" app-desc="举报信息" delete-action="delete" description="举报信息" class-name="com.gwssi.dw.aic.bj.xb.txn.TxnXbReportBase" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-xb_report_base.jsp" txn-code="60172001" description="查询举报信息列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加举报信息" path="insert-xb_report_base.jsp" description="增加举报信息" func-name="addRecord"/>
	    <button type="button" name="修改举报信息" path="/txn60172004.do" description="修改举报信息" func-name="updateRecord"/>
	    <button type="button" name="删除举报信息" path="/txn60172005.do" description="删除举报信息" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn60172001.do" description="查询举报信息信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-xb_report_base.jsp" txn-code="60172003" description="增加举报信息信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60172003.do" description="保存举报信息信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn60172003.do" description="保存举报信息信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn60172001.do" description="保存举报信息信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60172001.do" description="返回到举报信息明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-xb_report_base.jsp" txn-code="60172002" description="修改举报信息信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60172001.do" description="修改举报信息信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60172001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-xb_report_base.jsp" txn-code="60172001" description="查询举报信息列表">
	    <forward error-code="success" path="query-xb_report_base.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-xb_report_base.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-xb_report_base.jsp" txn-code="60172002" description="修改举报信息信息">
	    <forward error-code="success" path="update-xb_report_base.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-xb_report_base.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-xb_report_base.jsp" txn-code="60172003" description="增加举报信息信息">
	    <forward error-code="success" path="insert-xb_report_base.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-xb_report_base.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-xb_report_base.jsp" txn-code="60172004" description="查询举报信息用于修改">
	    <forward error-code="success" path="update-xb_report_base.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-xb_report_base.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-xb_report_base.jsp" txn-code="60172005" description="删除举报信息信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.xb.vo.XbReportBaseContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.xb.vo.VoXbReportBase" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.xb.vo.VoXbReportBaseSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.xb.vo.VoXbReportBasePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
