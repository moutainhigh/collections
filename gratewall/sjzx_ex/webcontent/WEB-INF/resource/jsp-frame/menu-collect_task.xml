<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="collect_task.xml" app-path="/collect/config/webservice/" app-name="collect_task" description="采集任务表" template="single-table.xml"/>
  <table name="collect_task.xml" app-name="collect_task" app-desc="webservice任务对应方法" delete-action="delete" description="采集任务表" class-name="com.gwssi.collect.webservice.txn.TxnCollectTask" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-collect_task.jsp" txn-code="30102001" description="查询webservice任务对应方法列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加webservice任务对应方法" path="insert-collect_task.jsp" description="增加webservice任务对应方法" func-name="addRecord"/>
	    <button type="button" name="修改webservice任务对应方法" path="/txn30102004.do" description="修改webservice任务对应方法" func-name="updateRecord"/>
	    <button type="button" name="删除webservice任务对应方法" path="/txn30102005.do" description="删除webservice任务对应方法" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn30102001.do" description="查询webservice任务对应方法信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-collect_task.jsp" txn-code="30102003" description="增加webservice任务对应方法信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn30102003.do" description="保存webservice任务对应方法信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn30102003.do" description="保存webservice任务对应方法信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn30102001.do" description="保存webservice任务对应方法信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30102001.do" description="返回到webservice任务对应方法明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-collect_task.jsp" txn-code="30102002" description="修改webservice任务对应方法信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn30102001.do" description="修改webservice任务对应方法信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn30102001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-collect_task.jsp" txn-code="30102001" description="查询webservice任务对应方法列表">
	    <forward error-code="success" path="query-collect_task.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-collect_task.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-collect_task.jsp" txn-code="30102002" description="修改webservice任务对应方法信息">
	    <forward error-code="success" path="update-collect_task.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-collect_task.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-collect_task.jsp" txn-code="30102003" description="增加webservice任务对应方法信息">
	    <forward error-code="success" path="insert-collect_task.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-collect_task.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-collect_task.jsp" txn-code="30102004" description="查询webservice任务对应方法用于修改">
	    <forward error-code="success" path="update-collect_task.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-collect_task.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-collect_task.jsp" txn-code="30102005" description="删除webservice任务对应方法信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.collect.webservice.vo.CollectTaskContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.collect.webservice.vo.VoCollectTask" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.collect.webservice.vo.VoCollectTaskSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.collect.webservice.vo.VoCollectTaskPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
