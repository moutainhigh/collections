<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="xt_ccgl_wjlb.xml" app-path="/page/file/manage/" app-name="xt_ccgl_wjlb" description="文件类别" template="single-table.xml"/>
  <table name="xt_ccgl_wjlb.xml" app-name="xt_ccgl_wjlb" app-desc="文件类别" delete-action="delete" description="文件类别" class-name="com.gwssi.file.manage.txn.TxnXtCcglWjlb" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-xt_ccgl_wjlb.jsp" txn-code="604050101" description="查询文件类别列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加文件类别" path="insert-xt_ccgl_wjlb.jsp" description="增加文件类别" func-name="addRecord"/>
	    <button type="button" name="修改文件类别" path="/txn604050104.do" description="修改文件类别" func-name="updateRecord"/>
	    <button type="button" name="删除文件类别" path="/txn604050105.do" description="删除文件类别" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn604050101.do" description="查询文件类别信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-xt_ccgl_wjlb.jsp" txn-code="604050103" description="增加文件类别信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn604050103.do" description="保存文件类别信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn604050103.do" description="保存文件类别信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn604050101.do" description="保存文件类别信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn604050101.do" description="返回到文件类别明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-xt_ccgl_wjlb.jsp" txn-code="604050102" description="修改文件类别信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn604050101.do" description="修改文件类别信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn604050101.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-xt_ccgl_wjlb.jsp" txn-code="604050101" description="查询文件类别列表">
	    <forward error-code="success" path="query-xt_ccgl_wjlb.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-xt_ccgl_wjlb.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-xt_ccgl_wjlb.jsp" txn-code="604050102" description="修改文件类别信息">
	    <forward error-code="success" path="update-xt_ccgl_wjlb.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-xt_ccgl_wjlb.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-xt_ccgl_wjlb.jsp" txn-code="604050103" description="增加文件类别信息">
	    <forward error-code="success" path="insert-xt_ccgl_wjlb.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-xt_ccgl_wjlb.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-xt_ccgl_wjlb.jsp" txn-code="604050104" description="查询文件类别用于修改">
	    <forward error-code="success" path="update-xt_ccgl_wjlb.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-xt_ccgl_wjlb.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-xt_ccgl_wjlb.jsp" txn-code="604050105" description="删除文件类别信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.file.manage.vo.XtCcglWjlbContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.file.manage.vo.VoXtCcglWjlb" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.file.manage.vo.VoXtCcglWjlbSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.file.manage.vo.VoXtCcglWjlbPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
