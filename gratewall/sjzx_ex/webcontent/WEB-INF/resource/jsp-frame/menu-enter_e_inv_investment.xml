<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="enter_e_inv_investment.xml" app-path="/dw/aic/bj/reg/" app-name="enter_e_inv_investment" description="投资信息表" template="single-table.xml"/>
  <table name="enter_e_inv_investment.xml" app-name="enter_e_inv_investment" app-desc="出资人" delete-action="delete" description="投资信息表" class-name="com.gwssi.dw.aic.bj.reg.txn.TxnEnterEInvInvestment" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-enter_e_inv_investment.jsp" txn-code="60119201" description="查询出资人列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加出资人" path="insert-enter_e_inv_investment.jsp" description="增加出资人" func-name="addRecord"/>
	    <button type="button" name="修改出资人" path="/txn60119204.do" description="修改出资人" func-name="updateRecord"/>
	    <button type="button" name="删除出资人" path="/txn60119205.do" description="删除出资人" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn60119201.do" description="查询出资人信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-enter_e_inv_investment.jsp" txn-code="60119203" description="增加出资人信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60119203.do" description="保存出资人信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn60119203.do" description="保存出资人信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn60119201.do" description="保存出资人信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60119201.do" description="返回到出资人明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-enter_e_inv_investment.jsp" txn-code="60119202" description="修改出资人信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60119201.do" description="修改出资人信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60119201.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-enter_e_inv_investment.jsp" txn-code="60119201" description="查询出资人列表">
	    <forward error-code="success" path="query-enter_e_inv_investment.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-enter_e_inv_investment.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-enter_e_inv_investment.jsp" txn-code="60119202" description="修改出资人信息">
	    <forward error-code="success" path="update-enter_e_inv_investment.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-enter_e_inv_investment.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-enter_e_inv_investment.jsp" txn-code="60119203" description="增加出资人信息">
	    <forward error-code="success" path="insert-enter_e_inv_investment.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-enter_e_inv_investment.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-enter_e_inv_investment.jsp" txn-code="60119204" description="查询出资人用于修改">
	    <forward error-code="success" path="update-enter_e_inv_investment.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-enter_e_inv_investment.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-enter_e_inv_investment.jsp" txn-code="60119205" description="删除出资人信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.EnterEInvInvestmentContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.VoEnterEInvInvestment" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.VoEnterEInvInvestmentSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.VoEnterEInvInvestmentPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
