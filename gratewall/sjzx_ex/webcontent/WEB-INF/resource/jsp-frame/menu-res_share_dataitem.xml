<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="res_share_dataitem.xml" app-path="/page/resource/shareRes/" app-name="res_share_dataitem" description="共享数据项信息表" template="single-table.xml"/>
  <table name="res_share_dataitem.xml" app-name="res_share_dataitem" app-desc="共享数据项信息" delete-action="delete" description="共享数据项信息表" class-name="com.gwssi.resource.share.txn.TxnResShareDataitem" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-res_share_dataitem.jsp" txn-code="20301021" description="查询共享数据项信息列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加共享数据项信息" path="insert-res_share_dataitem.jsp" description="增加共享数据项信息" func-name="addRecord"/>
	    <button type="button" name="修改共享数据项信息" path="/txn20301024.do" description="修改共享数据项信息" func-name="updateRecord"/>
	    <button type="button" name="删除共享数据项信息" path="/txn20301025.do" description="删除共享数据项信息" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn20301021.do" description="查询共享数据项信息信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-res_share_dataitem.jsp" txn-code="20301023" description="增加共享数据项信息信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn20301023.do" description="保存共享数据项信息信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn20301023.do" description="保存共享数据项信息信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn20301021.do" description="保存共享数据项信息信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn20301021.do" description="返回到共享数据项信息明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-res_share_dataitem.jsp" txn-code="20301022" description="修改共享数据项信息信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn20301021.do" description="修改共享数据项信息信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn20301021.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-res_share_dataitem.jsp" txn-code="20301021" description="查询共享数据项信息列表">
	    <forward error-code="success" path="query-res_share_dataitem.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-res_share_dataitem.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-res_share_dataitem.jsp" txn-code="20301022" description="修改共享数据项信息信息">
	    <forward error-code="success" path="update-res_share_dataitem.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-res_share_dataitem.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-res_share_dataitem.jsp" txn-code="20301023" description="增加共享数据项信息信息">
	    <forward error-code="success" path="insert-res_share_dataitem.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-res_share_dataitem.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_share_dataitem.jsp" txn-code="20301024" description="查询共享数据项信息用于修改">
	    <forward error-code="success" path="update-res_share_dataitem.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-res_share_dataitem.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_share_dataitem.jsp" txn-code="20301025" description="删除共享数据项信息信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.resource.share.vo.ResShareDataitemContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.resource.share.vo.VoResShareDataitem" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.resource.share.vo.VoResShareDataitemSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.resource.share.vo.VoResShareDataitemPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
