<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_svr_user.xml" app-path="/yxgl/gxfwgl/fwdxgl/" app-name="sys_svr_user" description="服务对象管理" template="single-table.xml"/>
  <table name="sys_svr_user.xml" app-name="sys_svr_user" app-desc="服务对象" delete-action="delete" description="服务对象管理" class-name="com.gwssi.runmgr.services.txn.TxnSysSvrUser" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_svr_user.jsp" txn-code="50201001" description="查询服务对象列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加服务对象" path="insert-sys_svr_user.jsp" description="增加服务对象" func-name="addRecord"/>
	    <button type="button" name="修改服务对象" path="/txn50201004.do" description="修改服务对象" func-name="updateRecord"/>
	    <button type="button" name="删除服务对象" path="/txn50201005.do" description="删除服务对象" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn50201001.do" description="查询服务对象信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_svr_user.jsp" txn-code="50201003" description="增加服务对象信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn50201003.do" description="保存服务对象信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn50201003.do" description="保存服务对象信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn50201001.do" description="保存服务对象信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50201001.do" description="返回到服务对象明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_svr_user.jsp" txn-code="50201002" description="修改服务对象信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn50201001.do" description="修改服务对象信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50201001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_svr_user.jsp" txn-code="50201001" description="查询服务对象列表">
	    <forward error-code="success" path="query-sys_svr_user.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_svr_user.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_svr_user.jsp" txn-code="50201002" description="修改服务对象信息">
	    <forward error-code="success" path="update-sys_svr_user.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_svr_user.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_svr_user.jsp" txn-code="50201003" description="增加服务对象信息">
	    <forward error-code="success" path="insert-sys_svr_user.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_svr_user.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_svr_user.jsp" txn-code="50201004" description="查询服务对象用于修改">
	    <forward error-code="success" path="update-sys_svr_user.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_svr_user.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_svr_user.jsp" txn-code="50201005" description="删除服务对象信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.SysSvrUserContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.VoSysSvrUser" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.VoSysSvrUserSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.VoSysSvrUserPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
