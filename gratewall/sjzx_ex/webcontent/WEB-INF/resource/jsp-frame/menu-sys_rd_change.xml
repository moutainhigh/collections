<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_rd_change.xml" app-path="/dw/rd/metadata/datastructure/change/" app-name="sys_rd_change" description="变更记录表" template="single-table.xml"/>
  <table name="sys_rd_change.xml" app-name="sys_rd_change" app-desc="变更记录表" delete-action="delete" description="变更记录表" class-name="cn.gwssi.dw.rd.metadata.txn.TxnSysRdChange" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_rd_change.jsp" txn-code="80002301" description="查询变更记录表列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加变更记录表" path="insert-sys_rd_change.jsp" description="增加变更记录表" func-name="addRecord"/>
	    <button type="button" name="修改变更记录表" path="/txn80002304.do" description="修改变更记录表" func-name="updateRecord"/>
	    <button type="button" name="删除变更记录表" path="/txn80002305.do" description="删除变更记录表" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn80002301.do" description="查询变更记录表信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_rd_change.jsp" txn-code="80002303" description="增加变更记录表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn80002303.do" description="保存变更记录表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn80002303.do" description="保存变更记录表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn80002301.do" description="保存变更记录表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80002301.do" description="返回到变更记录表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_rd_change.jsp" txn-code="80002302" description="修改变更记录表信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn80002301.do" description="修改变更记录表信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80002301.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_rd_change.jsp" txn-code="80002301" description="查询变更记录表列表">
	    <forward error-code="success" path="query-sys_rd_change.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_rd_change.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_rd_change.jsp" txn-code="80002302" description="修改变更记录表信息">
	    <forward error-code="success" path="update-sys_rd_change.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_rd_change.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_rd_change.jsp" txn-code="80002303" description="增加变更记录表信息">
	    <forward error-code="success" path="insert-sys_rd_change.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_rd_change.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_change.jsp" txn-code="80002304" description="查询变更记录表用于修改">
	    <forward error-code="success" path="update-sys_rd_change.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_rd_change.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_change.jsp" txn-code="80002305" description="删除变更记录表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.SysRdChangeContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdChange" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdChangeSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.metadata.vo.VoSysRdChangePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
