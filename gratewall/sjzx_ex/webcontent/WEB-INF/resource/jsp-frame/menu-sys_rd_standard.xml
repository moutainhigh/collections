<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_rd_standard.xml" app-path="/dw/rd/standard/gfgl/" app-name="sys_rd_standard" description="信息实体管理" template="single-table.xml"/>
  <table name="sys_rd_standard.xml" app-name="sys_rd_standard" app-desc="规范" delete-action="delete" description="信息实体管理" class-name="cn.gwssi.dw.rd.standard.txn.TxnSysRdStandard" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_rd_standard.jsp" txn-code="7000201" description="查询规范列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加规范" path="insert-sys_rd_standard.jsp" description="增加规范" func-name="addRecord"/>
	    <button type="button" name="修改规范" path="/txn7000204.do" description="修改规范" func-name="updateRecord"/>
	    <button type="button" name="删除规范" path="/txn7000205.do" description="删除规范" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn7000201.do" description="查询规范信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_rd_standard.jsp" txn-code="7000203" description="增加规范信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn7000203.do" description="保存规范信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn7000203.do" description="保存规范信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn7000201.do" description="保存规范信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn7000201.do" description="返回到规范明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_rd_standard.jsp" txn-code="7000202" description="修改规范信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn7000201.do" description="修改规范信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn7000201.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_rd_standard.jsp" txn-code="7000201" description="查询规范列表">
	    <forward error-code="success" path="query-sys_rd_standard.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_rd_standard.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_rd_standard.jsp" txn-code="7000202" description="修改规范信息">
	    <forward error-code="success" path="update-sys_rd_standard.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_rd_standard.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_rd_standard.jsp" txn-code="7000203" description="增加规范信息">
	    <forward error-code="success" path="insert-sys_rd_standard.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_rd_standard.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_standard.jsp" txn-code="7000204" description="查询规范用于修改">
	    <forward error-code="success" path="update-sys_rd_standard.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_rd_standard.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_rd_standard.jsp" txn-code="7000205" description="删除规范信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.dw.rd.standard.vo.SysRdStandardContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.standard.vo.VoSysRdStandard" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.standard.vo.VoSysRdStandardSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.standard.vo.VoSysRdStandardPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
