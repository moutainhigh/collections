	  </txn>
	  <txn input="update-sys_notice_info.jsp" txn-code="50220002" description="修改通知通告表信息">
	    <forward error-code="success" path="update-sys_notice_info.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_notice_info.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_notice_info.jsp" txn-code="50220003" description="增加通知通告表信息">
	    <forward error-code="success" path="insert-sys_notice_info.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_notice_info.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_notice_info.jsp" txn-code="50220004" description="查询通知通告表用于修改">
	    <forward error-code="success" path="update-sys_notice_info.jsp" description="成功时返回记录"/>
