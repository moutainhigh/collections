	    <button type="button" name="返 回" path="/txn50220001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_notice_info.jsp" txn-code="50220001" description="查询通知通告表列表">
	    <forward error-code="success" path="query-sys_notice_info.jsp" description="成功时返回列表"/>
