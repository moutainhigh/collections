	  <vo clazz-name="com.gwssi.dw.runmgr.etl.vo.VoSysNoticeInfo" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.etl.vo.VoSysNoticeInfoSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
