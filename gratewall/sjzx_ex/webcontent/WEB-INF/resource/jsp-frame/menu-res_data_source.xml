<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="res_data_source.xml" app-path="/page/resource/svrobj/datasource/" app-name="res_data_source" description="数据源表" template="single-table.xml"/>
  <table name="res_data_source.xml" app-name="res_data_source" app-desc="数据源表" delete-action="delete" description="数据源表" class-name="com.gwssi.resource.svrobj.txn.TxnResDataSource" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-res_data_source.jsp" txn-code="20102001" description="查询数据源表列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加数据源表" path="insert-res_data_source.jsp" description="增加数据源表" func-name="addRecord"/>
	    <button type="button" name="修改数据源表" path="/txn20102004.do" description="修改数据源表" func-name="updateRecord"/>
	    <button type="button" name="删除数据源表" path="/txn20102005.do" description="删除数据源表" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn20102001.do" description="查询数据源表信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-res_data_source.jsp" txn-code="20102003" description="增加数据源表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn20102003.do" description="保存数据源表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn20102003.do" description="保存数据源表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn20102001.do" description="保存数据源表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn20102001.do" description="返回到数据源表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-res_data_source.jsp" txn-code="20102002" description="修改数据源表信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn20102001.do" description="修改数据源表信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn20102001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-res_data_source.jsp" txn-code="20102001" description="查询数据源表列表">
	    <forward error-code="success" path="query-res_data_source.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-res_data_source.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-res_data_source.jsp" txn-code="20102002" description="修改数据源表信息">
	    <forward error-code="success" path="update-res_data_source.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-res_data_source.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-res_data_source.jsp" txn-code="20102003" description="增加数据源表信息">
	    <forward error-code="success" path="insert-res_data_source.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-res_data_source.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_data_source.jsp" txn-code="20102004" description="查询数据源表用于修改">
	    <forward error-code="success" path="update-res_data_source.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-res_data_source.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_data_source.jsp" txn-code="20102005" description="删除数据源表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.ResDataSourceContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.VoResDataSource" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.VoResDataSourceSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.resource.svrobj.vo.VoResDataSourcePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
