	    <button type="button" name="返 回" path="/txn80000001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-etl_table_count.jsp" txn-code="80000001" description="查询中心库数据量统计表列表">
	    <forward error-code="success" path="query-etl_table_count.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-etl_table_count.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-etl_table_count.jsp" txn-code="80000002" description="修改中心库数据量统计表信息">
	    <forward error-code="success" path="update-etl_table_count.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-etl_table_count.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-etl_table_count.jsp" txn-code="80000003" description="增加中心库数据量统计表信息">
	    <forward error-code="success" path="insert-etl_table_count.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-etl_table_count.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-etl_table_count.jsp" txn-code="80000004" description="查询中心库数据量统计表用于修改">
	    <forward error-code="success" path="update-etl_table_count.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-etl_table_count.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-etl_table_count.jsp" txn-code="80000005" description="删除中心库数据量统计表信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.runmgr.etl.vo.EtlTableCountContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.etl.vo.VoEtlTableCount" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.etl.vo.VoEtlTableCountSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.runmgr.etl.vo.VoEtlTableCountPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
