	    <button type="button" name="修改中心库数据量统计表" path="/txn80000004.do" description="修改中心库数据量统计表" func-name="updateRecord"/>
	    <button type="button" name="删除中心库数据量统计表" path="/txn80000005.do" description="删除中心库数据量统计表" func-name="deleteRecord"/>
