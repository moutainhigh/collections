	  </jsp>
	  <jsp page-name="insert-etl_table_count.jsp" txn-code="80000003" description="增加中心库数据量统计表信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn80000003.do" description="保存中心库数据量统计表信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn80000003.do" description="保存中心库数据量统计表信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn80000001.do" description="保存中心库数据量统计表信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn80000001.do" description="返回到中心库数据量统计表明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-etl_table_count.jsp" txn-code="80000002" description="修改中心库数据量统计表信息" template="single/single-table-update.jsp">
