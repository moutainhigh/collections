<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="res_exception_date.xml" app-path="/page/resource/svrdate/" app-name="res_exception_date" description="例外日期表" template="single-table.xml"/>
  <table name="res_exception_date.xml" app-name="res_exception_date" app-desc="例外日期" delete-action="delete" description="例外日期表" class-name="com.gwssi.resource.exception.txn.TxnResExceptionDate" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-res_exception_date.jsp" txn-code="205001" description="查询例外日期列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加例外日期" path="insert-res_exception_date.jsp" description="增加例外日期" func-name="addRecord"/>
	    <button type="button" name="修改例外日期" path="/txn205004.do" description="修改例外日期" func-name="updateRecord"/>
	    <button type="button" name="删除例外日期" path="/txn205005.do" description="删除例外日期" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn205001.do" description="查询例外日期信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-res_exception_date.jsp" txn-code="205003" description="增加例外日期信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn205003.do" description="保存例外日期信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn205003.do" description="保存例外日期信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn205001.do" description="保存例外日期信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn205001.do" description="返回到例外日期明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-res_exception_date.jsp" txn-code="205002" description="修改例外日期信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn205001.do" description="修改例外日期信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn205001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-res_exception_date.jsp" txn-code="205001" description="查询例外日期列表">
	    <forward error-code="success" path="query-res_exception_date.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-res_exception_date.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-res_exception_date.jsp" txn-code="205002" description="修改例外日期信息">
	    <forward error-code="success" path="update-res_exception_date.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-res_exception_date.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-res_exception_date.jsp" txn-code="205003" description="增加例外日期信息">
	    <forward error-code="success" path="insert-res_exception_date.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-res_exception_date.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_exception_date.jsp" txn-code="205004" description="查询例外日期用于修改">
	    <forward error-code="success" path="update-res_exception_date.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-res_exception_date.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-res_exception_date.jsp" txn-code="205005" description="删除例外日期信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.resource.exception.vo.ResExceptionDateContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.resource.exception.vo.VoResExceptionDate" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.resource.exception.vo.VoResExceptionDateSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.resource.exception.vo.VoResExceptionDatePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
