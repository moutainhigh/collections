	  </txn>
	  <txn input="update-ad_cred_cue.jsp" txn-code="txn60181302" description="修改提示信息表信息">
	    <forward error-code="success" path="update-ad_cred_cue.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-ad_cred_cue.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-ad_cred_cue.jsp" txn-code="txn60181303" description="增加提示信息表信息">
	    <forward error-code="success" path="insert-ad_cred_cue.jsp" description="成功时返回页面"/>
