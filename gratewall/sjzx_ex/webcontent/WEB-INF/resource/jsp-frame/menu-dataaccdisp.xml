<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="dataaccdisp.xml" app-path="/csdb/xtgl/" app-name="dataaccdisp" description="数据权限分配" template="single-table.xml"/>
  <table name="dataaccdisp.xml" app-name="dataaccdisp" app-desc="数据权限分配" delete-action="delete" description="数据权限分配" class-name="cn.gwssi.csdb.xtgl.txn.TxnDataaccdisp" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-dataaccdisp.jsp" txn-code="103041" description="查询数据权限分配列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加数据权限分配" path="insert-dataaccdisp.jsp" description="增加数据权限分配" func-name="addRecord"/>
	    <button type="button" name="修改数据权限分配" path="/txn103044.do" description="修改数据权限分配" func-name="updateRecord"/>
	    <button type="button" name="删除数据权限分配" path="/txn103045.do" description="删除数据权限分配" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn103041.do" description="查询数据权限分配信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-dataaccdisp.jsp" txn-code="103043" description="增加数据权限分配信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn103043.do" description="保存数据权限分配信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn103043.do" description="保存数据权限分配信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn103041.do" description="保存数据权限分配信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn103041.do" description="返回到数据权限分配明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-dataaccdisp.jsp" txn-code="103042" description="修改数据权限分配信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn103041.do" description="修改数据权限分配信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn103041.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-dataaccdisp.jsp" txn-code="103041" description="查询数据权限分配列表">
	    <forward error-code="success" path="query-dataaccdisp.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-dataaccdisp.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-dataaccdisp.jsp" txn-code="103042" description="修改数据权限分配信息">
	    <forward error-code="success" path="update-dataaccdisp.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-dataaccdisp.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-dataaccdisp.jsp" txn-code="103043" description="增加数据权限分配信息">
	    <forward error-code="success" path="insert-dataaccdisp.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-dataaccdisp.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-dataaccdisp.jsp" txn-code="103044" description="查询数据权限分配用于修改">
	    <forward error-code="success" path="update-dataaccdisp.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-dataaccdisp.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-dataaccdisp.jsp" txn-code="103045" description="删除数据权限分配信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.DataaccdispContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoDataaccdisp" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoDataaccdispSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.xtgl.vo.VoDataaccdispPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
