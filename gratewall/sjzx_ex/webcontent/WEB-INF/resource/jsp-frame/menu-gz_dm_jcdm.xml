<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="同一页面的主从表数据库操作" page="master-detail-table-1.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="multi-table-1.gif"/>
  <struts name="gz_dm_jcdm.xml" app-path="/ysjgl/jcsjgl/jcdmgl/" app-name="gz_dm_jcdm" description="基础代码管理" template="master-detail-table-1.xml"/>
  <table name="gz_dm_jcdm.xml" app-name="gz_dm_jcdm" app-desc="基础代码" delete-action="delete" description="基础代码管理" class-name="cn.gwssi.csdb.ysjgl.txn.TxnGzDmJcdm" template="master-detail-table-1.master.java" dao-class-template="single-table.java" type="master">
	  <jsp page-name="query-gz_dm_jcdm.jsp" txn-code="301011" description="查询基础代码列表" template="master-detail-1/master-table-query.jsp">
	    <button type="button" name="增加基础代码" path="insert-gz_dm_jcdm.jsp" description="增加基础代码" func-name="addRecord"/>
	    <button type="button" name="修改基础代码" path="/txn301014.do" description="修改基础代码" func-name="updateRecord"/>
	    <button type="button" name="删除基础代码" path="/txn301015.do" description="删除基础代码" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn301011.do" description="查询基础代码信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-gz_dm_jcdm.jsp" txn-code="301013" description="增加基础代码信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn301013.do" description="保存基础代码信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn301013.do" description="保存基础代码信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn301011.do" description="保存基础代码信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn301011.do" description="返回到基础代码明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-gz_dm_jcdm.jsp" txn-code="301012" description="修改基础代码信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn301011.do" description="修改基础代码信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn301011.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-gz_dm_jcdm.jsp" txn-code="301011" description="查询基础代码列表">
	    <forward error-code="success" path="query-gz_dm_jcdm.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-gz_dm_jcdm.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-gz_dm_jcdm.jsp" txn-code="301012" description="修改基础代码信息">
	    <forward error-code="success" path="update-gz_dm_jcdm.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-gz_dm_jcdm.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-gz_dm_jcdm.jsp" txn-code="301013" description="增加基础代码信息">
	    <forward error-code="success" path="insert-gz_dm_jcdm.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-gz_dm_jcdm.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-gz_dm_jcdm.jsp" txn-code="301014" description="查询基础代码用于修改">
	    <forward error-code="success" path="update-gz_dm_jcdm.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-gz_dm_jcdm.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-gz_dm_jcdm.jsp" txn-code="301015" description="删除基础代码信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.ysjgl.vo.VoGzDmJcdm" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.ysjgl.vo.VoGzDmJcdmSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.ysjgl.vo.VoGzDmJcdmPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.ysjgl.vo.GzDmJcdmContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
  </table>
  
</resource>
