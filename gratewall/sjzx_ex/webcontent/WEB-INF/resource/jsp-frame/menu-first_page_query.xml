	    <button type="button" name="查 询" path="/txn61000001.do" description="查询first_page_query信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-first_page_query.jsp" txn-code="61000003" description="增加first_page_query信息" template="single/single-table-insert.jsp">
