	  <jsp page-name="query-first_page_query.jsp" txn-code="61000001" description="查询first_page_query列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加first_page_query" path="insert-first_page_query.jsp" description="增加first_page_query" func-name="addRecord"/>
