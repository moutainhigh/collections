	  </txn>
	  <vo clazz-name="com.gwssi.sysmgr.vo.FirstPageQueryContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.sysmgr.vo.VoFirstPageQuery" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.sysmgr.vo.VoFirstPageQuerySelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.sysmgr.vo.VoFirstPageQueryPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
