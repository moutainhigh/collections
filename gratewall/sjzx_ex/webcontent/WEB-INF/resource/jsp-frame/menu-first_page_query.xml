	  <jsp page-name="update-first_page_query.jsp" txn-code="61000002" description="修改first_page_query信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn61000001.do" description="修改first_page_query信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn61000001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-first_page_query.jsp" txn-code="61000001" description="查询first_page_query列表">
	    <forward error-code="success" path="query-first_page_query.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-first_page_query.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-first_page_query.jsp" txn-code="61000002" description="修改first_page_query信息">
	    <forward error-code="success" path="update-first_page_query.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-first_page_query.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-first_page_query.jsp" txn-code="61000003" description="增加first_page_query信息">
	    <forward error-code="success" path="insert-first_page_query.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-first_page_query.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-first_page_query.jsp" txn-code="61000004" description="查询first_page_query用于修改">
	    <forward error-code="success" path="update-first_page_query.jsp" description="成功时返回记录"/>
