<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="trs_share_service.xml" app-path="/page/share/trs/" app-name="trs_share_service" description="trs服务接口" template="single-table.xml"/>
  <table name="trs_share_service.xml" app-name="trs_share_service" app-desc="trs接口" delete-action="delete" description="trs服务接口" class-name="com.gwssi.share.trs.txn.TxnTrsShareService" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-trs_share_service.jsp" txn-code="40300001" description="查询trs接口列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加trs接口" path="insert-trs_share_service.jsp" description="增加trs接口" func-name="addRecord"/>
	    <button type="button" name="修改trs接口" path="/txn40300004.do" description="修改trs接口" func-name="updateRecord"/>
	    <button type="button" name="删除trs接口" path="/txn40300005.do" description="删除trs接口" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn40300001.do" description="查询trs接口信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-trs_share_service.jsp" txn-code="40300003" description="增加trs接口信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn40300003.do" description="保存trs接口信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn40300003.do" description="保存trs接口信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn40300001.do" description="保存trs接口信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40300001.do" description="返回到trs接口明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-trs_share_service.jsp" txn-code="40300002" description="修改trs接口信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn40300001.do" description="修改trs接口信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn40300001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-trs_share_service.jsp" txn-code="40300001" description="查询trs接口列表">
	    <forward error-code="success" path="query-trs_share_service.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-trs_share_service.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-trs_share_service.jsp" txn-code="40300002" description="修改trs接口信息">
	    <forward error-code="success" path="update-trs_share_service.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-trs_share_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-trs_share_service.jsp" txn-code="40300003" description="增加trs接口信息">
	    <forward error-code="success" path="insert-trs_share_service.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-trs_share_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-trs_share_service.jsp" txn-code="40300004" description="查询trs接口用于修改">
	    <forward error-code="success" path="update-trs_share_service.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-trs_share_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-trs_share_service.jsp" txn-code="40300005" description="删除trs接口信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.share.trs.vo.TrsShareServiceContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.share.trs.vo.VoTrsShareService" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.share.trs.vo.VoTrsShareServiceSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.share.trs.vo.VoTrsShareServicePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
