<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="同一页面的主从表数据库操作" page="master-detail-table-1.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="multi-table-1.gif"/>
  <struts name="ad_mon_midm.xml" app-path="/dw/aic/bj/ad/" app-name="ad_mon_midm" description="广告发布主体" template="master-detail-table-1.xml"/>
  <table name="ad_mon_midm.xml" app-name="ad_mon_midm" app-desc="广告" delete-action="delete" description="广告发布主体" class-name="com.gwssi.dw.aic.bj.ad.txn.TxnAdMonMidm" template="master-detail-table-1.master.java" dao-class-template="single-table.java" type="master">
	  <jsp page-name="query-ad_mon_midm.jsp" txn-code="60181001" description="查询广告列表" template="master-detail-1/master-table-query.jsp">
	    <button type="button" name="增加广告" path="insert-ad_mon_midm.jsp" description="增加广告" func-name="addRecord"/>
	    <button type="button" name="修改广告" path="/txn60181004.do" description="修改广告" func-name="updateRecord"/>
	    <button type="button" name="删除广告" path="/txn60181005.do" description="删除广告" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn60181001.do" description="查询广告信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-ad_mon_midm.jsp" txn-code="60181003" description="增加广告信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60181003.do" description="保存广告信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn60181003.do" description="保存广告信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn60181001.do" description="保存广告信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60181001.do" description="返回到广告明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-ad_mon_midm.jsp" txn-code="60181002" description="修改广告信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60181001.do" description="修改广告信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60181001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-ad_mon_midm.jsp" txn-code="60181001" description="查询广告列表">
	    <forward error-code="success" path="query-ad_mon_midm.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-ad_mon_midm.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-ad_mon_midm.jsp" txn-code="60181002" description="修改广告信息">
	    <forward error-code="success" path="update-ad_mon_midm.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-ad_mon_midm.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-ad_mon_midm.jsp" txn-code="60181003" description="增加广告信息">
	    <forward error-code="success" path="insert-ad_mon_midm.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-ad_mon_midm.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-ad_mon_midm.jsp" txn-code="60181004" description="查询广告用于修改">
	    <forward error-code="success" path="update-ad_mon_midm.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-ad_mon_midm.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-ad_mon_midm.jsp" txn-code="60181005" description="删除广告信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.VoAdMonMidm" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.VoAdMonMidmSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.VoAdMonMidmPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.vo.AdMonMidmContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
  </table>
  
</resource>
