<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_search_config.xml" app-path="/search/" app-name="sys_search_config" description="全文检索" template="single-table.xml"/>
  <table name="sys_search_config.xml" app-name="sys_search_config" app-desc="全文检索" delete-action="delete" description="全文检索" class-name="cn.gwssi.search.txn.TxnSysSearchConfig" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_search_config.jsp" txn-code="50030101" description="查询全文检索列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加全文检索" path="insert-sys_search_config.jsp" description="增加全文检索" func-name="addRecord"/>
	    <button type="button" name="修改全文检索" path="/txn50030104.do" description="修改全文检索" func-name="updateRecord"/>
	    <button type="button" name="删除全文检索" path="/txn50030105.do" description="删除全文检索" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn50030101.do" description="查询全文检索信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_search_config.jsp" txn-code="50030103" description="增加全文检索信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn50030103.do" description="保存全文检索信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn50030103.do" description="保存全文检索信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn50030101.do" description="保存全文检索信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50030101.do" description="返回到全文检索明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_search_config.jsp" txn-code="50030102" description="修改全文检索信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn50030101.do" description="修改全文检索信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50030101.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_search_config.jsp" txn-code="50030101" description="查询全文检索列表">
	    <forward error-code="success" path="query-sys_search_config.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_search_config.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_search_config.jsp" txn-code="50030102" description="修改全文检索信息">
	    <forward error-code="success" path="update-sys_search_config.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_search_config.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_search_config.jsp" txn-code="50030103" description="增加全文检索信息">
	    <forward error-code="success" path="insert-sys_search_config.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_search_config.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_search_config.jsp" txn-code="50030104" description="查询全文检索用于修改">
	    <forward error-code="success" path="update-sys_search_config.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_search_config.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_search_config.jsp" txn-code="50030105" description="删除全文检索信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.search.vo.SysSearchConfigContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.search.vo.VoSysSearchConfig" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.search.vo.VoSysSearchConfigSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.search.vo.VoSysSearchConfigPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
