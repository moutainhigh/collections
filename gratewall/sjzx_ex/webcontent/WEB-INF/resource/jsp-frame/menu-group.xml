<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="reg_bus_group.xml" app-path="/dw/aic/bj/reg/" app-name="group" description="集团登记查询" template="single-table.xml"/>
  <table name="reg_bus_group.xml" app-name="group" app-desc="集团登记查询" delete-action="delete" description="集团登记查询" class-name="com.gwssi.dw.aic.bj.reg.txn.TxnRegBusGroup" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-group.jsp" txn-code="60710001" description="查询集团登记查询列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加集团登记查询" path="insert-group.jsp" description="增加集团登记查询" func-name="addRecord"/>
	    <button type="button" name="修改集团登记查询" path="/txn60710004.do" description="修改集团登记查询" func-name="updateRecord"/>
	    <button type="button" name="删除集团登记查询" path="/txn60710005.do" description="删除集团登记查询" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn60710001.do" description="查询集团登记查询信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-group.jsp" txn-code="60710003" description="增加集团登记查询信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60710003.do" description="保存集团登记查询信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn60710003.do" description="保存集团登记查询信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn60710001.do" description="保存集团登记查询信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60710001.do" description="返回到集团登记查询明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-group.jsp" txn-code="60710002" description="修改集团登记查询信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60710001.do" description="修改集团登记查询信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60710001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-group.jsp" txn-code="60710001" description="查询集团登记查询列表">
	    <forward error-code="success" path="query-group.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-group.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-group.jsp" txn-code="60710002" description="修改集团登记查询信息">
	    <forward error-code="success" path="update-group.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-group.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-group.jsp" txn-code="60710003" description="增加集团登记查询信息">
	    <forward error-code="success" path="insert-group.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-group.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-group.jsp" txn-code="60710004" description="查询集团登记查询用于修改">
	    <forward error-code="success" path="update-group.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-group.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-group.jsp" txn-code="60710005" description="删除集团登记查询信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.GroupContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.VoGroup" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.VoGroupSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.reg.vo.VoGroupPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
