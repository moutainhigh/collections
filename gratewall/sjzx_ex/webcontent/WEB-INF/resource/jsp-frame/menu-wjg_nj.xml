<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="wjg_nj.xml" app-path="/wjg/njgl/" app-name="wjg_nj" description="年鉴下载" template="single-table.xml"/>
  <table name="wjg_nj.xml" app-name="wjg_nj" app-desc="年鉴" description="年鉴下载" class-name="cn.gwssi.csdb.wjg.txn.TxnWjgNj" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-wjg_nj.jsp" txn-code="205021" description="查询年鉴列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加年鉴" path="insert-wjg_nj.jsp" description="增加年鉴" func-name="addRecord"/>
	    <button type="button" name="修改年鉴" path="/txn205024.do" description="修改年鉴" func-name="updateRecord"/>
	    <button type="button" name="删除年鉴" path="/txn205025.do" description="删除年鉴" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn205021.do" description="查询年鉴信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-wjg_nj.jsp" txn-code="205023" description="增加年鉴信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn205023.do" description="保存年鉴信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn205023.do" description="保存年鉴信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn205021.do" description="保存年鉴信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn205021.do" description="返回到年鉴明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-wjg_nj.jsp" txn-code="205022" description="修改年鉴信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn205021.do" description="修改年鉴信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn205021.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>
	  <txn input="query-wjg_nj.jsp" txn-code="205021" description="查询年鉴列表">
	    <forward error-code="success" path="query-wjg_nj.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-wjg_nj.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-wjg_nj.jsp" txn-code="205022" description="修改年鉴信息">
	    <forward error-code="success" path="update-wjg_nj.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-wjg_nj.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-wjg_nj.jsp" txn-code="205023" description="增加年鉴信息">
	    <forward error-code="success" path="insert-wjg_nj.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-wjg_nj.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_nj.jsp" txn-code="205024" description="查询年鉴用于修改">
	    <forward error-code="success" path="update-wjg_nj.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-wjg_nj.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_nj.jsp" txn-code="205025" description="删除年鉴信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.WjgNjContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgNj" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgNjSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgNjPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
