<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="view_sys_app_log_query.xml" app-path="/dw/rd/log/" app-name="view_sys_app_log_query" description="日志查询" template="single-table.xml"/>
  <table name="view_sys_app_log_query.xml" app-name="view_sys_app_log_query" app-desc="日志查询" delete-action="delete" description="日志查询" class-name="cn.gwssi.dw.rd.log.txn.TxnViewSysAppLogQuery" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-view_sys_app_log_query.jsp" txn-code="620100101" description="查询日志查询列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加日志查询" path="insert-view_sys_app_log_query.jsp" description="增加日志查询" func-name="addRecord"/>
	    <button type="button" name="修改日志查询" path="/txn620100104.do" description="修改日志查询" func-name="updateRecord"/>
	    <button type="button" name="删除日志查询" path="/txn620100105.do" description="删除日志查询" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn620100101.do" description="查询日志查询信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-view_sys_app_log_query.jsp" txn-code="620100103" description="增加日志查询信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn620100103.do" description="保存日志查询信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn620100103.do" description="保存日志查询信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn620100101.do" description="保存日志查询信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn620100101.do" description="返回到日志查询明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-view_sys_app_log_query.jsp" txn-code="620100102" description="修改日志查询信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn620100101.do" description="修改日志查询信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn620100101.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-view_sys_app_log_query.jsp" txn-code="620100101" description="查询日志查询列表">
	    <forward error-code="success" path="query-view_sys_app_log_query.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-view_sys_app_log_query.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-view_sys_app_log_query.jsp" txn-code="620100102" description="修改日志查询信息">
	    <forward error-code="success" path="update-view_sys_app_log_query.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-view_sys_app_log_query.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-view_sys_app_log_query.jsp" txn-code="620100103" description="增加日志查询信息">
	    <forward error-code="success" path="insert-view_sys_app_log_query.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-view_sys_app_log_query.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-view_sys_app_log_query.jsp" txn-code="620100104" description="查询日志查询用于修改">
	    <forward error-code="success" path="update-view_sys_app_log_query.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-view_sys_app_log_query.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-view_sys_app_log_query.jsp" txn-code="620100105" description="删除日志查询信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.ViewSysAppLogQueryContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.VoViewSysAppLogQuery" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.VoViewSysAppLogQuerySelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.dw.rd.log.vo.VoViewSysAppLogQueryPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
