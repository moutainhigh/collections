<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="wjg_wj.xml" app-path="/wjg/rwgl/wjrw/" app-name="wjg_wj" description="文件任务设置" template="single-table.xml"/>
  <table name="wjg_wj.xml" app-name="wjg_wj" app-desc="文件任务" description="文件任务设置" class-name="cn.gwssi.csdb.wjg.txn.TxnWjgWj" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-wjg_wj.jsp" txn-code="2020111" description="查询文件任务列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加文件任务" path="insert-wjg_wj.jsp" description="增加文件任务" func-name="addRecord"/>
	    <button type="button" name="修改文件任务" path="/txn2020114.do" description="修改文件任务" func-name="updateRecord"/>
	    <button type="button" name="删除文件任务" path="/txn2020115.do" description="删除文件任务" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn2020111.do" description="查询文件任务信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-wjg_wj.jsp" txn-code="2020113" description="增加文件任务信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn2020113.do" description="保存文件任务信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn2020113.do" description="保存文件任务信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn2020111.do" description="保存文件任务信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn2020111.do" description="返回到文件任务明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-wjg_wj.jsp" txn-code="2020112" description="修改文件任务信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn2020111.do" description="修改文件任务信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn2020111.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>
	  <txn input="query-wjg_wj.jsp" txn-code="2020111" description="查询文件任务列表">
	    <forward error-code="success" path="query-wjg_wj.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-wjg_wj.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-wjg_wj.jsp" txn-code="2020112" description="修改文件任务信息">
	    <forward error-code="success" path="update-wjg_wj.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-wjg_wj.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-wjg_wj.jsp" txn-code="2020113" description="增加文件任务信息">
	    <forward error-code="success" path="insert-wjg_wj.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-wjg_wj.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_wj.jsp" txn-code="2020114" description="查询文件任务用于修改">
	    <forward error-code="success" path="update-wjg_wj.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-wjg_wj.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-wjg_wj.jsp" txn-code="2020115" description="删除文件任务信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.WjgWjContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWj" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.csdb.wjg.vo.VoWjgWjPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
