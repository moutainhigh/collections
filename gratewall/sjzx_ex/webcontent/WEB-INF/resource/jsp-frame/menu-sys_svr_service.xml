<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="com.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="sys_svr_service.xml" app-path="/yxgl/gxfwgl/gxfwgl/" app-name="sys_svr_service" description="共享服务管理" template="single-table.xml"/>
  <table name="sys_svr_service.xml" app-name="sys_svr_service" app-desc="共享服务" delete-action="delete" description="共享服务管理" class-name="com.gwssi.runmgr.services.txn.TxnSysSvrService" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-sys_svr_service.jsp" txn-code="50202001" description="查询共享服务列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加共享服务" path="insert-sys_svr_service.jsp" description="增加共享服务" func-name="addRecord"/>
	    <button type="button" name="修改共享服务" path="/txn50202004.do" description="修改共享服务" func-name="updateRecord"/>
	    <button type="button" name="删除共享服务" path="/txn50202005.do" description="删除共享服务" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn50202001.do" description="查询共享服务信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-sys_svr_service.jsp" txn-code="50202003" description="增加共享服务信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn50202003.do" description="保存共享服务信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn50202003.do" description="保存共享服务信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn50202001.do" description="保存共享服务信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50202001.do" description="返回到共享服务明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-sys_svr_service.jsp" txn-code="50202002" description="修改共享服务信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn50202001.do" description="修改共享服务信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50202001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-sys_svr_service.jsp" txn-code="50202001" description="查询共享服务列表">
	    <forward error-code="success" path="query-sys_svr_service.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-sys_svr_service.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-sys_svr_service.jsp" txn-code="50202002" description="修改共享服务信息">
	    <forward error-code="success" path="update-sys_svr_service.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-sys_svr_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-sys_svr_service.jsp" txn-code="50202003" description="增加共享服务信息">
	    <forward error-code="success" path="insert-sys_svr_service.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-sys_svr_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_svr_service.jsp" txn-code="50202004" description="查询共享服务用于修改">
	    <forward error-code="success" path="update-sys_svr_service.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-sys_svr_service.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-sys_svr_service.jsp" txn-code="50202005" description="删除共享服务信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.SysSvrServiceContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.VoSysSvrService" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.VoSysSvrServiceSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.runmgr.services.vo.VoSysSvrServicePrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
