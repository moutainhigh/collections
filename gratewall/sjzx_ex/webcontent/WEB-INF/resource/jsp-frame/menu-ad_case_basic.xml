<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="ad_case_basic.xml" app-path="/dw/aic/bj/ad/cas/" app-name="ad_case_basic" description="案件管理" template="single-table.xml"/>
  <table name="ad_case_basic.xml" app-name="ad_case_basic" app-desc="广告案件" delete-action="delete" description="案件管理" class-name="com.gwssi.dw.aic.bj.ad.cas.txn.TxnAdCaseBasic" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-ad_case_basic.jsp" txn-code="60186001" description="查询广告案件列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加广告案件" path="insert-ad_case_basic.jsp" description="增加广告案件" func-name="addRecord"/>
	    <button type="button" name="修改广告案件" path="/txn60186004.do" description="修改广告案件" func-name="updateRecord"/>
	    <button type="button" name="删除广告案件" path="/txn60186005.do" description="删除广告案件" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn60186001.do" description="查询广告案件信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-ad_case_basic.jsp" txn-code="60186003" description="增加广告案件信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn60186003.do" description="保存广告案件信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn60186003.do" description="保存广告案件信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn60186001.do" description="保存广告案件信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60186001.do" description="返回到广告案件明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-ad_case_basic.jsp" txn-code="60186002" description="修改广告案件信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn60186001.do" description="修改广告案件信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn60186001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-ad_case_basic.jsp" txn-code="60186001" description="查询广告案件列表">
	    <forward error-code="success" path="query-ad_case_basic.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-ad_case_basic.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-ad_case_basic.jsp" txn-code="60186002" description="修改广告案件信息">
	    <forward error-code="success" path="update-ad_case_basic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-ad_case_basic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-ad_case_basic.jsp" txn-code="60186003" description="增加广告案件信息">
	    <forward error-code="success" path="insert-ad_case_basic.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-ad_case_basic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-ad_case_basic.jsp" txn-code="60186004" description="查询广告案件用于修改">
	    <forward error-code="success" path="update-ad_case_basic.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-ad_case_basic.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-ad_case_basic.jsp" txn-code="60186005" description="删除广告案件信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.cas.vo.AdCaseBasicContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.cas.vo.VoAdCaseBasic" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.cas.vo.VoAdCaseBasicSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.dw.aic.bj.ad.cas.vo.VoAdCaseBasicPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
