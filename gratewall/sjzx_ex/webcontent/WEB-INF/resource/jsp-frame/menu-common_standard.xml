<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="common_standard.xml" app-path="/page/standard/" app-name="common_standard" description="规则标准" template="single-table.xml"/>
  <table name="common_standard.xml" app-name="common_standard" app-desc="规则" delete-action="delete" description="规则标准" class-name="com.gwssi.standard.txn.TxnCommonStandard" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-common_standard.jsp" txn-code="603001" description="查询规则列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加规则" path="insert-common_standard.jsp" description="增加规则" func-name="addRecord"/>
	    <button type="button" name="修改规则" path="/txn603004.do" description="修改规则" func-name="updateRecord"/>
	    <button type="button" name="删除规则" path="/txn603005.do" description="删除规则" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn603001.do" description="查询规则信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-common_standard.jsp" txn-code="603003" description="增加规则信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn603003.do" description="保存规则信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn603003.do" description="保存规则信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn603001.do" description="保存规则信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn603001.do" description="返回到规则明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-common_standard.jsp" txn-code="603002" description="修改规则信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn603001.do" description="修改规则信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn603001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-common_standard.jsp" txn-code="603001" description="查询规则列表">
	    <forward error-code="success" path="query-common_standard.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-common_standard.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-common_standard.jsp" txn-code="603002" description="修改规则信息">
	    <forward error-code="success" path="update-common_standard.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-common_standard.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-common_standard.jsp" txn-code="603003" description="增加规则信息">
	    <forward error-code="success" path="insert-common_standard.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-common_standard.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-common_standard.jsp" txn-code="603004" description="查询规则用于修改">
	    <forward error-code="success" path="update-common_standard.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-common_standard.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-common_standard.jsp" txn-code="603005" description="删除规则信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="com.gwssi.standard.vo.CommonStandardContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="com.gwssi.standard.vo.VoCommonStandard" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="com.gwssi.standard.vo.VoCommonStandardSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="com.gwssi.standard.vo.VoCommonStandardPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
