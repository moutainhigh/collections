<?xml version="1.0" encoding="GBK"?>
<resource>
  <model name="单表数据库操作" page="single-table.jsp" wizard-class="cn.gwssi.console.txn.wizard.TableModuleWizard" demo-image="single-table.gif"/>
  <struts name="opb_subject.xml" app-path="/yxgl/sjzhgl/test/" app-name="opb_subject" description="test" template="single-table.xml"/>
  <table name="opb_subject.xml" app-name="opb_subject" app-desc="测试" delete-action="delete" description="test" class-name="cn.gwssi.runmgr.etl.txn.TxnOpbSubject" template="single-table.java" dao-class-template="single-table.java">
	  <jsp page-name="query-opb_subject.jsp" txn-code="50105001" description="查询测试列表" template="single/single-table-query.jsp">
	    <button type="button" name="增加测试" path="insert-opb_subject.jsp" description="增加测试" func-name="addRecord"/>
	    <button type="button" name="修改测试" path="/txn50105004.do" description="修改测试" func-name="updateRecord"/>
	    <button type="button" name="删除测试" path="/txn50105005.do" description="删除测试" func-name="deleteRecord"/>
	    <button type="button" name="查 询" path="/txn50105001.do" description="查询测试信息" func-name="submit"/>
	  </jsp>
	  <jsp page-name="insert-opb_subject.jsp" txn-code="50105003" description="增加测试信息" template="single/single-table-insert.jsp">
	    <button type="button" name="保 存" path="/txn50105003.do" description="保存测试信息" func-name="saveRecord"/>
	    <button type="button" name="保存并继续" path="/txn50105003.do" description="保存测试信息并继续" func-name="saveAndContinue"/>
	    <button type="button" name="保存并关闭" path="/txn50105001.do" description="保存测试信息并返回" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50105001.do" description="返回到测试明细窗口" func-name="goBack"/>
	  </jsp>
	  <jsp page-name="update-opb_subject.jsp" txn-code="50105002" description="修改测试信息" template="single/single-table-update.jsp">
	    <button type="button" name="保 存" path="/txn50105001.do" description="修改测试信息" func-name="saveAndExit"/>
	    <button type="button" name="返 回" path="/txn50105001.do" description="返回到明细窗口" func-name="goBack"/>
	  </jsp>

	  <txn input="query-opb_subject.jsp" txn-code="50105001" description="查询测试列表">
	    <forward error-code="success" path="query-opb_subject.jsp" description="成功时返回列表"/>
	    <forward error-code="failure" path="query-opb_subject.jsp" description="错误时返回错误信息"/>
	  </txn>
	  <txn input="update-opb_subject.jsp" txn-code="50105002" description="修改测试信息">
	    <forward error-code="success" path="update-opb_subject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="update-opb_subject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="insert-opb_subject.jsp" txn-code="50105003" description="增加测试信息">
	    <forward error-code="success" path="insert-opb_subject.jsp" description="成功时返回页面"/>
	    <forward error-code="failure" path="insert-opb_subject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-opb_subject.jsp" txn-code="50105004" description="查询测试用于修改">
	    <forward error-code="success" path="update-opb_subject.jsp" description="成功时返回记录"/>
	    <forward error-code="failure" path="update-opb_subject.jsp" description="错误时返回页面"/>
	  </txn>
	  <txn input="query-opb_subject.jsp" txn-code="50105005" description="删除测试信息">
	    <forward error-code="success" path="/script/deletesuccess.jsp" description="成功时返回提示页面"/>
	    <forward error-code="failure" path="/script/deletefailure.jsp" description="错误时返回提示页面"/>
	  </txn>
	  <vo clazz-name="cn.gwssi.runmgr.etl.vo.OpbSubjectContext" description="应用对应的数据上下文对象" template="single-table-context.java"/>
	  <vo clazz-name="cn.gwssi.runmgr.etl.vo.VoOpbSubject" description="数据表字段对应的VO对象" template="single-table-vo.java"/>
	  <vo clazz-name="cn.gwssi.runmgr.etl.vo.VoOpbSubjectSelectKey" description="查询条件对应的VO对象" template="single-table-vo-select-key.java"/>
	  <vo clazz-name="cn.gwssi.runmgr.etl.vo.VoOpbSubjectPrimaryKey" description="数据库主键对应的VO对象" template="single-table-vo-primary-key.java"/>
  </table>
</resource>
