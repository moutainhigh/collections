<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>业务字段语义定义</menu-name>
  <menu-path>/page/resource/res/metadata/datadict/ywbzdyydy/</menu-path>
  <description>业务字段语义定义</description>
  <form-beans>
    <form-bean name="SysColumnSemanticRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="sys_id" type="string" size="32" description="业务系统编码"/>
      <form-property prefix="select-key" name="table_no" type="string" size="32" description="业务表编码"/>
      <form-property prefix="select-key" name="column_no" type="string" size="32" description="业务字段编码"/>
      <form-property prefix="select-key" name="column_name" type="string" size="60" description="业务字段名"/>
      <form-property prefix="select-key" name="column_name_cn" type="string" size="100" description="业务字段中文名"/>
      <form-property prefix="select-key" name="table_name" type="string" size="60" description="业务表名称"/>
      <form-property prefix="record" name="sys_id" type="string" size="32" description="业务系统编码"/>
      <form-property prefix="record" name="table_name" type="string" size="60" cannull="false" description="业务表名"/>
      <form-property prefix="record" name="table_no" type="string" size="32" cannull="false" description="业务表编码"/>
      <form-property prefix="record" name="table_name_cn" type="string" size="100" cannull="false" description="业务表中文名"/>
      <form-property prefix="record" name="column_no" type="string" size="32" cannull="false" description="业务字段编码"/>
      <form-property prefix="record" name="column_name" type="string" size="60" cannull="false" description="业务字段名"/>
      <form-property prefix="record" name="column_name_cn" type="string" size="100" cannull="false" description="业务字段中文名"/>
      <form-property prefix="record" name="column_order" type="int" cannull="false" description="检索顺序"/>
      <form-property prefix="record" name="edit_type" type="string" size="2" cannull="false" description="编辑类型"/>
      <form-property prefix="record" name="edit_content" type="string" size="100" cannull="true" description="编辑内容"/>
      <form-property prefix="record" name="demo" type="string" size="100" cannull="true" description="备注"/>
      <form-property prefix="record" name="sys_name" type="string" size="100" cannull="false" description="系统名称"/>
      <form-property prefix="record" name="column_byname" type="string" size="60" cannull="false" description="业务字段名别名"/>
    </form-bean>

    <form-bean name="SysColumnSemanticUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="column_no" type="string" size="32" cannull="false" description="业务字段编码"/>
      <form-property prefix="record" name="table_no" type="string" size="32" cannull="false" description="业务表编码"/>
      <form-property prefix="record" name="column_no" type="string" size="32" cannull="false" description="业务字段编码"/>
      <form-property prefix="record" name="column_name" type="string" size="60" cannull="false" description="业务字段名"/>
      <form-property prefix="record" name="column_name_cn" type="string" size="100" cannull="false" description="业务字段中文名"/>
      <form-property prefix="record" name="column_order" type="int" cannull="false" description="检索顺序"/>
      <form-property prefix="record" name="edit_type" type="string" size="2" cannull="false" description="编辑类型"/>
      <form-property prefix="record" name="edit_content" type="string" size="100" cannull="true" description="编辑内容"/>
      <form-property prefix="record" name="demo" type="string" size="100" cannull="true" description="备注"/>
      <form-property prefix="record" name="column_byname" type="string" size="60" cannull="false" description="业务字段名别名"/>
      <form-property prefix="record" name="old_column_name" type="string" size="60" cannull="false" description="修改前业务字段名"/>
    </form-bean>

    <form-bean name="SysColumnSemanticDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="column_no" type="string" size="32" cannull="false" description="业务字段编码"/>
      <form-property prefix="primary-key" name="column_name" type="string" size="60" cannull="false" description="业务字段名"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-sys_column_semantic.jsp" name="SysColumnSemanticRowsetForm" txn-code="30403001" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询业务字段列表</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403001" input-node="select-key" output-node="record" description="查询业务字段列表"/>
      <forward error-code="success" path="query-sys_column_semantic.jsp"/>
      <forward error-code="failure" path="query-sys_column_semantic.jsp"/>
    </action>

    <action input="update-sys_column_semantic.jsp" name="SysColumnSemanticUpdateForm" txn-code="30403002" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改业务字段信息</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403002" input-node="record" description="修改业务字段信息"/>
      <forward error-code="success" path="update-sys_column_semantic.jsp"/>
      <forward error-code="failure" path="update-sys_column_semantic.jsp"/>
    </action>

    <action input="insert-sys_column_semantic.jsp" name="SysColumnSemanticUpdateForm" txn-code="30403003" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加业务字段信息</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403003" input-node="record" description="增加业务字段信息"/>
      <forward error-code="success" path="insert-sys_column_semantic.jsp"/>
      <forward error-code="failure" path="insert-sys_column_semantic.jsp"/>
    </action>

    <action input="query-sys_column_semantic.jsp" name="SysColumnSemanticUpdateForm" txn-code="30403004" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询业务字段用于修改</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403004" input-node="primary-key" output-node="record" description="查询业务字段用于修改"/>
      <forward error-code="success" path="update-sys_column_semantic.jsp"/>
      <forward error-code="failure" path="update-sys_column_semantic.jsp"/>
    </action>

    <action input="query-sys_column_semantic.jsp" name="SysColumnSemanticDeleteForm" txn-code="30403005" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除业务字段信息</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403005" input-node="primary-key" description="删除业务字段信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    
    <action input="insert-sys_column_semantic.jsp" name="SysColumnSemanticRowsetForm" txn-code="30403006" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>校验字段能否添加</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403006" input-node="select-key" description="校验字段能否添加"/>
      <forward error-code="success" path="query-sys_column_semantic.jsp"/>
      <forward error-code="failure" path="query-sys_column_semantic.jsp"/>
    </action>
    
    <action input="query-sys_column_semantic.jsp" name="SysColumnSemanticRowsetForm" txn-code="30403007" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>根据表名查询业务字段列表</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysColumnSemantic" func-name="txn30403007" input-node="select-key" output-node="record" description="查询业务字段列表"/>
      <forward error-code="success" path="query-sys_column_semantic.jsp"/>
      <forward error-code="failure" path="query-sys_column_semantic.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-sys_column_semantic.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/resource/res/metadata/datadict/ywbzdyydy/query-sys_column_semantic.jsp" description="查询业务字段列表"/>
    <resource-file type="jsp" file-name="/page/resource/res/metadata/datadict/ywbzdyydy/insert-sys_column_semantic.jsp" description="增加业务字段信息"/>
    <resource-file type="jsp" file-name="/page/resource/res/metadata/datadict/ywbzdyydy/update-sys_column_semantic.jsp" description="修改业务字段信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.SysColumnSemanticContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.VoSysColumnSemantic" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.VoSysColumnSemanticSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.VoSysColumnSemanticPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

