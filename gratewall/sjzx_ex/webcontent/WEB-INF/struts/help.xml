      <forward error-code="success" path=""/>
      <forward error-code="failure" path=""/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="vo-bean" file-name="com.gwssi.help.vo.HelpContext" description="应用对应的数据上下文对象"/>
  </resource>
