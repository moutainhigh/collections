<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>帮助中心</menu-name>
  <menu-path>/page/help/</menu-path>
  <description>帮助中心</description>
  <form-beans>
    <form-bean name="HelpForm">
      </form-bean>
  </form-beans>
  <action-mappings>
    <action input="" name="HelpForm" txn-code="7100001" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>跳转到平台介绍</description>
      <step index="10" bean-name="com.gwssi.help.txn.TxnHelp" func-name="txn7100001" input-node="primary-key" output-node="record" description="跳转到平台介绍"/>
      <forward error-code="success" path=""/>
      <forward error-code="failure" path=""/>
    </action>
   <action input="" name="HelpForm" txn-code="7100002" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>跳转到使用说明</description>
