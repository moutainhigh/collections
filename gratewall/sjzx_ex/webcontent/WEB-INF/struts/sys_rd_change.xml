      <description>查询变更记录表列表</description>
      <step index="10" bean-name="cn.gwssi.dw.rd.metadata.txn.TxnSysRdChange" func-name="txn80002301" input-node="select-key" output-node="record" description="查询变更记录表列表"/>
      <forward error-code="success" path="query-sys_rd_change.jsp"/>
      <forward error-code="failure" path="query-sys_rd_change.jsp"/>
    </action>

    <action input="update-sys_rd_change.jsp" name="SysRdChangeUpdateForm" txn-code="80002302" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
