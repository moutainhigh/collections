<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>日志报告生成</menu-name>
  <menu-path>/page/log/report/</menu-path>
  <description>日志报告生成</description>
  <form-beans>
    <form-bean name="LogReportCreateRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="log_report_create_id" type="string" size="32" description="日志报告id"/>
      <form-property prefix="select-key" name="report_name" type="string" size="1000" description="报告名称"/>
      <form-property prefix="select-key" name="publish_date" type="string" size="10" description="发布日期"/>
      <form-property prefix="select-key" name="state" type="string" size="1" description="状态"/>
      <form-property prefix="select-key" name="report_type" type="string" size="1" description="报告类型"/>
      <form-property prefix="select-key" name="create_date" type="string" size="10" description="建立日期"/>
      <form-property prefix="select-key" name="create_date_start" type="string" size="32" description="查询时间起"/>
      
      <form-property prefix="select-key"  name="created_time"  type="string" size="32" description="建立日期"/>
      
      <form-property prefix="select-key" name="create_date_end" type="string" size="32" description="查询时间止"/>
      <form-property prefix="select-key" name="publish_date_start" type="string" size="32" description="查询时间起"/>
      <form-property prefix="select-key" name="publish_date_end" type="string" size="32" description="查询时间止"/>
      <form-property prefix="record" name="log_report_create_id" type="string" size="32" cannull="false" description="日志报告id"/>
      <form-property prefix="record" name="report_name" type="string" size="1000" cannull="true" description="报告名称"/>
      <form-property prefix="record" name="create_date" type="string" size="10" cannull="true" description="建立日期"/>
      <form-property prefix="record" name="creator" type="string" size="1000" cannull="true" description="上传人"/>
      <form-property prefix="record" name="last_mender" type="string" size="100" cannull="true" description="最后修改者"/>
      <form-property prefix="record" name="publish_date" type="string" size="10" cannull="true" description="发布日期"/>
      <form-property prefix="record" name="publish_person" type="string" size="100" cannull="true" description="发布人"/>
      <form-property prefix="record" name="state" type="string" size="1" cannull="true" description="状态"/>
      <form-property prefix="record" name="report_type" type="string" size="1" cannull="true" description="报告类型"/>
      <form-property prefix="record" name="operate" type="string" size="10" cannull="true" description="操作"/>
      <form-property prefix="record" name="filename" type="string" size="1000" cannull="true" description="文件名"/>
      <form-property prefix="record" name="path" type="string" size="4000" cannull="true" description="路径"/>
      <form-property prefix="record" name="timestamp" type="string" size="32" cannull="true" description="时间戳"/>
    </form-bean>

    <form-bean name="LogReportCreateUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="log_report_create_id" type="string" size="32" cannull="false" description="日志报告id"/>
      <form-property prefix="record" name="year" type="string" size="10" description="选择报告年"/>
      <form-property prefix="record" name="month" type="string" size="10" description="选择报告月份"/>
      <form-property prefix="record" name="log_report_create_id" type="string" size="32" cannull="false" description="日志报告id"/>
      <form-property prefix="record" name="report_name" type="string" size="1000" cannull="true" description="报告名称"/>
      <form-property prefix="record" name="create_date" type="string" size="10" cannull="true" description="建立日期"/>
      <form-property prefix="record" name="creator" type="string" size="1000" cannull="true" description="上传人"/>
      <form-property prefix="record" name="last_mender" type="string" size="100" cannull="true" description="最后修改者"/>
      <form-property prefix="record" name="publish_date" type="string" size="10" cannull="true" description="发布日期"/>
      <form-property prefix="record" name="publish_person" type="string" size="100" cannull="true" description="发布人"/>
      <form-property prefix="record" name="state" type="string" size="1" cannull="true" description="状态"/>
      <form-property prefix="record" name="report_type" type="string" size="1" cannull="true" description="报告类型"/>
      <form-property prefix="record" name="operate" type="string" size="10" cannull="true" description="操作"/>
      <form-property prefix="record" name="filename" type="string" size="1000" cannull="true" description="文件名"/>
      <form-property prefix="record" name="uploadfile" type="string" size="1000" cannull="true" description="文件名"/>
      <form-property prefix="record" name="path" type="string" size="4000" cannull="true" description="路径"/>
      <form-property prefix="record" name="timestamp" type="string" size="32" cannull="true" description="时间戳"/>
    </form-bean>

    <form-bean name="LogReportCreateDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="log_report_create_id" type="string" size="32" cannull="false" description="日志报告id"/>
      <form-property prefix="primary-key" name="path" type="string" size="4000" cannull="false" description="文件路徑"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-log_report_create.jsp" name="LogReportCreateRowsetForm" txn-code="620200101" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询系统使用情况报告列表</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200101" input-node="select-key" output-node="record" description="查询系统使用情况报告列表"/>
      <forward error-code="success" path="query-log_report_create.jsp"/>
      <forward error-code="failure" path="query-log_report_create.jsp"/>
    </action>

    <action input="update-log_report_create.jsp" name="LogReportCreateUpdateForm" txn-code="620200102" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改系统使用情况报告信息</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200102" input-node="record" description="修改系统使用情况报告信息"/>
      <forward error-code="success" path="update-log_report_create.jsp"/>
      <forward error-code="failure" path="update-log_report_create.jsp"/>
    </action>

    <action input="insert-log_report_create.jsp" name="LogReportCreateUpdateForm" txn-code="620200103" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加系统使用情况报告信息</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200103" input-node="record" description="增加系统使用情况报告信息"/>
      <forward error-code="success" path="insert-log_report_create.jsp"/>
      <forward error-code="failure" path="insert-log_report_create.jsp"/>
    </action>

    <action input="query-log_report_create.jsp" name="LogReportCreateUpdateForm" txn-code="620200104" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询系统使用情况报告用于修改</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200104" input-node="primary-key" output-node="record" description="查询系统使用情况报告用于修改"/>
      <forward error-code="success" path="update-log_report_create.jsp"/>
      <forward error-code="failure" path="update-log_report_create.jsp"/>
    </action>

    <action input="query-log_report_create.jsp" name="LogReportCreateDeleteForm" txn-code="620200105" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>删除系统使用情况报告信息</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200105" input-node="primary-key" description="删除系统使用情况报告信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    
    <action input="insert-log_report_create1.jsp" name="LogReportCreateRowsetForm" txn-code="620200106" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>上传日志报告</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200106" input-node="primary-key" description="删除系统使用情况报告信息"/>
      <forward error-code="success" path="insert-log_report_create1.jsp"/>
      <forward error-code="failure" path="insert-log_report_create1.jsp"/>
    </action>
    
    <action input="query-log_report_create.jsp" name="LogReportCreateRowsetForm" txn-code="620200107" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>发布日志报告</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200107" input-node="primary-key" description="发布系统报告"/>
      <forward error-code="success" path="query-log_report_create.jsp"/>
      <forward error-code="failure" path="query-log_report_create.jsp"/>
    </action>
    
    <action input="query-log_report_create.jsp" name="LogReportCreateRowsetForm" txn-code="620200108" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>增加日志报告使用记录</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200108" input-node="select-key" description="增加日志报告使用记录"/>
      <forward error-code="success" path="query-log_report_create.jsp"/>
      <forward error-code="failure" path="query-log_report_create.jsp"/>
    </action>
    
    <action input="query-log_report_create.jsp" name="LogReportCreateRowsetForm" txn-code="620200109" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>退回日志报告</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200109" input-node="primary-key" description="退回系统报告"/>
      <forward error-code="success" path="query-log_report_create.jsp"/>
      <forward error-code="failure" path="query-log_report_create.jsp"/>
    </action>
    
    <action input="insert-log_report_create.jsp" name="LogReportCreateRowsetForm" txn-code="620200110" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询报告名称是否存在</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200110" input-node="select-key" output-node="record" description="查询报告名称是否存在"/>
      <forward error-code="success" path="insert-log_report_create.jsp"/>
      <forward error-code="failure" path="insert-log_report_create.jsp"/>
    </action>
    
    <action input="query-log_report_create.jsp" name="LogReportCreateRowsetForm" txn-code="620200111" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>生成日志报告</description>
      <step index="10" bean-name="com.gwssi.log.report.txn.TxnLogReportCreate" func-name="txn620200111" input-node="primary-key" description="退回系统报告"/>
      <forward error-code="success" path="query-log_report_create.jsp"/>
      <forward error-code="failure" path="query-log_report_create.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-log_report_create.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/log/report/query-log_report_create.jsp" description="查询系统使用情况报告列表"/>
    <resource-file type="jsp" file-name="/page/log/report/insert-log_report_create.jsp" description="增加系统使用情况报告信息"/>
    <resource-file type="jsp" file-name="/page/log/report/update-log_report_create.jsp" description="修改系统使用情况报告信息"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.LogReportCreateContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.VoLogReportCreate" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.VoLogReportCreateSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.VoLogReportCreatePrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

