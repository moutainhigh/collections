      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103008" input-node="primary-key" output-node="record" description="查询参数维护用于修改socket"/>
      <forward error-code="success" path="update-collect_webservice_patameter.jsp"/>
      <forward error-code="failure" path="update-collect_webservice_patameter.jsp"/>
    </action>
    
