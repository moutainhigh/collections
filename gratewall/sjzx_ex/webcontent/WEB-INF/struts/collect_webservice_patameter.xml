      <description>修改webservice方法的参数信息</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103002" input-node="record" description="修改参数维护信息"/>
      <forward error-code="success" path="update-collect_webservice_patameter.jsp"/>
      <forward error-code="failure" path="update-collect_webservice_patameter.jsp"/>
    </action>

    <action input="insert-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103003" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加参数维护信息</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103003" input-node="record" description="增加参数维护信息"/>
      <forward error-code="success" path="insert-collect_webservice_patameter.jsp"/>
      <forward error-code="failure" path="insert-collect_webservice_patameter.jsp"/>
    </action>  -->

    <action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103004" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询参数维护用于修改</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103004" input-node="primary-key" output-node="record" description="查询参数维护用于修改"/>
      <forward error-code="success" path="update-collect_webservice_patameter.jsp"/>
      <forward error-code="failure" path="update-collect_webservice_patameter.jsp"/>
    </action>

    <action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterDeleteForm" txn-code="30103005" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除参数维护信息</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103005" input-node="primary-key" description="删除参数维护信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    
    <action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterDeleteForm" txn-code="30103055" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除参数维护信息</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103055" input-node="primary-key" description="删除参数维护信息"/>
      <forward error-code="success" path="query-collect_ws_info.jsp"/>
      <forward error-code="failure" path="query-collect_ws_info.jsp"/>
    </action>
    
    <action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103006" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询参数维护用于新增</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103006" input-node="primary-key" output-node="record" description="查询参数维护用于新增"/>
      <forward error-code="success" path="/page/collect/config/socket/insert-collect_socket_patameter.jsp"/>
