  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-collect_webservice_patameter.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/collect/config/webservice/query-collect_webservice_patameter.jsp" description="查询参数维护列表"/>
    <resource-file type="jsp" file-name="/page/collect/config/webservice/insert-collect_webservice_patameter.jsp" description="增加参数维护信息"/>
    <resource-file type="jsp" file-name="/page/collect/config/webservice/update-collect_webservice_patameter.jsp" description="修改参数维护信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.collect.webservice.vo.CollectWebservicePatameterContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.collect.webservice.vo.VoCollectWebservicePatameter" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.collect.webservice.vo.VoCollectWebservicePatameterSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.collect.webservice.vo.VoCollectWebservicePatameterPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

