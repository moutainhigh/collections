<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>参数表</menu-name>
  <menu-path>/page/collect/config/webService/</menu-path>
  <description>参数表</description>
  <form-beans>
    <form-bean name="CollectWebservicePatameterRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="record" name="webservice_patameter_id" type="string" size="32" cannull="false" description="参数ID"/>
      <form-property prefix="record" name="webservice_task_id" type="string" size="32" cannull="true" description="webservice任务ID"/>
      <form-property prefix="record" name="patameter_type" type="string" size="20" cannull="true" description="参数类型"/>
      <form-property prefix="record" name="patameter_name" type="string" size="100" cannull="true" description="参数名"/>
      <form-property prefix="record" name="patameter_value" type="string" size="1000" cannull="true" description="参数值"/>
	  <form-property prefix="record" name="patameter_style" type="string" size="20" cannull="true" description="参数格式"/>
    </form-bean>

    <form-bean name="CollectWebservicePatameterUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="webservice_patameter_id" type="string" size="32" cannull="false" description="参数ID"/>
      <form-property prefix="select-key" name="webservice_patameter_id" type="string" size="32" cannull="false" description="参数ID"/>
      <form-property prefix="primary-key" name="webservice_task_id" type="string" size="32" cannull="false" description="webservice任务ID"/>
      <form-property prefix="select-key" name="webservice_task_id" type="string" size="32" cannull="false" description="webservice任务ID"/>
      <form-property prefix="primary-key" name="patameter_name" type="string" size="100" cannull="false" description="参数名"/>
      <form-property prefix="record" name="webservice_patameter_id" type="string" size="32" cannull="false" description="参数ID"/>
      <form-property prefix="record" name="jsondata" type="string" size="4000" cannull="false" description="数据"/>
      <form-property prefix="record" name="webservice_task_id" type="string" size="32" cannull="true" description="webservice任务ID"/>
      <form-property prefix="record" name="service_targets_id" type="string" size="32" cannull="false" description="服务ID"/>
      <form-property prefix="record" name="patameter_type" type="string" size="20" cannull="true" description="参数类型"/>
      <form-property prefix="record" name="patameter_name" type="string" size="100" cannull="true" description="参数名"/>
      <form-property prefix="record" name="patameter_value" type="string" size="1000" cannull="true" description="参数值"/>
      <form-property prefix="record" name="name_nums" type="string" size="11" cannull="true" description="重名数"/>
	  <form-property prefix="record" name="patameter_style" type="string" size="20" cannull="true" description="参数格式"/>
	  <form-property prefix="param" name="patameter_style" type="string" size="20" cannull="true" description="参数格式"/>
	  <form-property name="param_style" type="string" size="20" cannull="true" description="参数类型"/>
	  <form-property name="date_style" type="string" size="20" cannull="true" description="日期格式"/>
    </form-bean>

    <form-bean name="CollectWebservicePatameterDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="webservice_patameter_id" type="string" size="32" cannull="false" description="参数ID"/>
      <form-property prefix="primary-key" name="webservice_task_id" type="string" size="32" cannull="false" description="方法ID"/>
    </form-bean>
  </form-beans>

  <action-mappings>
  	<action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103000" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>查询参数名称是否重复</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103000" input-node="primary-key" output-node="record" description="查询参数名称是否重复"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletesuccess.jsp"/>
    </action>
  	<action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterRowsetForm" txn-code="30103001" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
  		<description>查询参数维护列表</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103001" input-node="select-key" output-node="record" description="查询参数维护列表"/>
      <forward error-code="success" path="query-collect_webservice_patameter.jsp"/>
      <forward error-code="failure" path="query-collect_webservice_patameter.jsp"/>
    </action>

 	<action input="insert-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103033" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>跳转到增加参数维护信息页面</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103033" input-node="record" description="增加参数维护信息"/>
      <forward error-code="success" path="insert-collect_parameter.jsp"/>
      <forward error-code="failure" path="insert-collect_parameter.jsp"/>
    </action>

	<action input="insert-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103003" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加参数维护信息</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103003" input-node="record" description="增加参数维护信息"/>
      <forward error-code="success" path="insert-collect_parameter.jsp"/>
      <forward error-code="failure" path="insert-collect_parameter.jsp"/>
    </action>
    
   
