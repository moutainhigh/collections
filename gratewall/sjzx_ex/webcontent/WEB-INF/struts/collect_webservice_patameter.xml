    </action>
    
     <action input="query-collect_webservice_patameter.jsp" name="CollectWebservicePatameterUpdateForm" txn-code="30103007" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询参数维护用于修改socket</description>
      <step index="10" bean-name="com.gwssi.collect.webservice.txn.TxnCollectWebservicePatameter" func-name="txn30103007" input-node="primary-key" output-node="record" description="查询参数维护用于修改socket"/>
