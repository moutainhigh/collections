      <description>查询采集数据库列表</description>
      <step index="10" bean-name="com.gwssi.collect.database.txn.TxnCollectDatabaseTask" func-name="txn30501001" input-node="select-key" output-node="record" description="查询采集数据库列表"/>
      <forward error-code="success" path="query-collect_database_task.jsp"/>
      <forward error-code="failure" path="query-collect_database_task.jsp"/>
    </action>
    <!-- start   add by dwn 20140306 -->
    <action input="query-collect_database_task.jsp" name="CollectDatabaseTaskRowsetForm" txn-code="30501010" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询采集数据库列表</description>
