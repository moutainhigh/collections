  <form-beans>
    <form-bean name="CollectDatabaseTaskRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="primary-key" name="database_task_id" type="string" size="32" cannull="false" description="采集数据库主键ID"/>
      <form-property prefix="primary-key" name="collect_task_id" type="string" size="32" cannull="false" description="采集任务ID"/>
      <form-property prefix="select-key" name="database_task_id" type="string" size="32" cannull="false" description="采集数据库主键ID"/>
      <form-property prefix="select-key" name="collect_task_id" type="string" size="32" cannull="false" description="采集任务ID"/>
      <form-property prefix="record" name="database_task_id" type="string" size="32" cannull="false" description="采集数据库主键ID"/>
      <form-property prefix="record" name="collect_task_id" type="string" size="32" cannull="true" description="采集任务ID"/>
      <form-property prefix="record" name="source_collect_table" type="string" size="100" cannull="false" description="源采集表"/>
      <form-property prefix="record" name="source_collect_column" type="string" size="100" cannull="true" description="源采集字段"/>
