<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>日志管理-查询采集日志</menu-name>
  <menu-path>/page/log/</menu-path>
  <description>日志管理-查询采集日志</description>
  <form-beans>
    <form-bean name="CollectJoumalRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="service_targets_id" type="string" size="100" description="服务对象ID" />
	  <form-property prefix="select-key" name="service_targets_type" type="string" size="100" description="服务对象类型" />
	  <form-property prefix="select-key" name="type" type="string" size="20" description="类型(采集/共享)" />
	  <form-property prefix="select-key" name="start_time" type="string" size="11" cannull="true" description="开始时间"/>
      <form-property prefix="select-key" name="status" type="string" size="20" cannull="true" description="代码表 启用停用归档"/>
      <form-property prefix="select-key" name="created_time" type="string" size="11" cannull="true" description="开始时间"/>
      
      <form-property prefix="select-key" name="collect_joumal_id" type="string" size="32" cannull="false" description="采集日志ID"/>
      <form-property prefix="select-key" name="task_name" type="string" size="100" description="服务对象名称"/>
      <form-property prefix="select-key" name="service_targets_name" type="string" size="100" description="服务对象名称"/>
      <form-property prefix="select-key" name="collect_type" type="string" size="20" description="代码表 ftp"/>
      <form-property prefix="select-key" name="created_time_start" type="string" size="20" description=""/>
      <form-property prefix="select-key" name="created_time_end" type="string" size="20" description=""/>
      <form-property prefix="select-key" name="task_status" type="string" size="20" description="代码表 启用停用归档"/>
      <form-property prefix="record" name="collect_joumal_id" type="string" size="32" cannull="false" description="采集日志ID"/>
      <form-property prefix="record" name="collect_task_id" type="string" size="32" cannull="true" description="采集任务ID"/>
      <form-property prefix="record" name="task_name" type="string" size="100" cannull="true" description="任务名称"/>
      <form-property prefix="record" name="method_name_cn" type="string" size="100" cannull="true" description="方法名称"/>
      <form-property prefix="record" name="collect_table_name" type="string" size="100" cannull="true" description="采集表名称"/>
      <form-property prefix="record" name="service_targets_id" type="string" size="32" cannull="true" description="服务对象ID"/>
      <form-property prefix="record" name="service_targets_name" type="string" size="100" cannull="true" description="服务对象名称"/>
      <form-property prefix="record" name="collect_type" type="string" size="20" cannull="true" description="代码表 ftp"/>
      <form-property prefix="record" name="task_id" type="string" size="32" cannull="true" description="访问IP"/>
	  <form-property prefix="record" name="service_no" type="string" size="20" cannull="true" description="任务编号"/> 
      <form-property prefix="record" name="task_start_time" type="string" size="11" cannull="true" description="任务开始时间"/>
      <form-property prefix="record" name="task_end_time" type="string" size="11" cannull="true" description="任务结束时间"/>
      <form-property prefix="record" name="task_consume_time" type="string" size="10" cannull="true" description="单位毫秒"/>
      <form-property prefix="record" name="collect_data_amount" type="string" size="10" cannull="true" description="本次采集数据量"/>
      <form-property prefix="record" name="task_status" type="string" size="20" cannull="true" description="代码表 启用停用归档"/>
      <form-property prefix="record" name="patameter" type="string" size="100" cannull="true" description="入口参数"/>
      <form-property prefix="record" name="return_codes" type="string" size="20" cannull="true" description="代码表 数据交换产生错误数据编号"/>
   <!-- 
   	  <form-property prefix="record" name="tsname" type="string" size="19" description="名称" />
	  <form-property prefix="record" name="tsid" type="string" size="32" cannull="false" description="日志ID"/>
      <form-property prefix="record" name="start_time" type="string" size="11" cannull="true" description="开始时间"/>
      <form-property prefix="record" name="consume_time" type="string" size="10" cannull="true" description="单位毫秒"/>
      <form-property prefix="record" name="record_amount" type="string" size="10" cannull="true" description="本次采集数据量"/>
      <form-property prefix="record" name="status" type="string" size="20" cannull="true" description="代码表 启用停用归档"/>
      
    -->
    </form-bean>

    <form-bean name="CollectJoumalUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
       <form-property prefix="select-key" name="service_targets_name" type="string" size="100" description="服务对象名称"/>
      <form-property prefix="select-key" name="collect_type" type="string" size="20" description="代码表 ftp"/>
      <form-property prefix="select-key" name="created_time_start" type="string" size="20" description=""/>
      <form-property prefix="select-key" name="created_time_end" type="string" size="20" description=""/>
      <form-property prefix="select-key" name="task_status" type="string" size="20" description="代码表 启用停用归档"/>
      <form-property prefix="select-key" name="collect_joumal_id" type="string" size="32" cannull="false" description="采集日志ID"/>
      <form-property prefix="record" name="collect_joumal_id" type="string" size="32" cannull="false" description="采集日志ID"/>
      <form-property prefix="record" name="collect_task_id" type="string" size="32" cannull="true" description="采集任务ID"/>
      <form-property prefix="record" name="task_name" type="string" size="100" cannull="true" description="任务名称"/>
      <form-property prefix="record" name="service_targets_id" type="string" size="32" cannull="true" description="服务对象ID"/>
      <form-property prefix="record" name="service_targets_name" type="string" size="100" cannull="true" description="服务对象名称"/>
      <form-property prefix="record" name="collect_type" type="string" size="20" cannull="true" description="代码表 ftp"/>
      <form-property prefix="record" name="task_id" type="string" size="32" cannull="true" description="访问IP"/>
	  <form-property prefix="record" name="service_no" type="string" size="20" cannull="true" description="任务编号"/> 
      <form-property prefix="record" name="task_start_time" type="string" size="11" cannull="true" description="任务开始时间"/>
      <form-property prefix="record" name="task_end_time" type="string" size="11" cannull="true" description="任务结束时间"/>
      <form-property prefix="record" name="task_consume_time" type="string" size="10" cannull="true" description="单位毫秒"/>
      <form-property prefix="record" name="collect_data_amount" type="string" size="10" cannull="true" description="本次采集数据量"/>
      <form-property prefix="record" name="task_status" type="string" size="20" cannull="true" description="代码表 启用停用归档"/>
      <form-property prefix="record" name="patameter" type="string" size="100" cannull="true" description="入口参数"/>
      <form-property prefix="record" name="return_codes" type="string" size="20" cannull="true" description="代码表 数据交换产生错误数据编号"/>
      <form-property prefix="record" name="count" type="string" size="20" cannull="true" description="归档条数"/>
    </form-bean>

    <form-bean name="CollectJoumalDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="collect_joumal_id" type="string" size="32" cannull="false" description="采集日志ID"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="list-query-collect_joumal.jsp" name="CollectJoumalRowsetForm" txn-code="6011010" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询采集日志列</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011010" input-node="select-key" output-node="record" description="查询采集日志列表"/>
      <forward error-code="success" path="list-query-collect_joumal.jsp"/>
      <forward error-code="failure" path="list-query-collect_joumal.jsp"/>
    </action>
    
 	<action input="list-query-collect_joumal.jsp" name="CollectJoumalRowsetForm" txn-code="6011011" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询采集日志列表</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011001" input-node="select-key" output-node="record" description="查询采集日志列表"/>
      <forward error-code="success" path="query-collect_joumal.jsp"/>
      <forward error-code="failure" path="query-collect_joumal.jsp"/>
    </action>
    
    <action input="update-collect_archive.jsp" name="CollectJoumalUpdateForm" txn-code="6011013" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询采集日志列表</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011013" input-node="record" output-node="record" description="采集日志归档"/>
      <forward error-code="success" path="/script/insertsuccess.jsp?nextPage=txn6011020.do" redirect="true"/>
      <forward error-code="failure" path="update-collect_archive.jsp"/>
    </action>
    
    
    
    <action input="update-collect_joumal.jsp" name="CollectJoumalUpdateForm" txn-code="6011002" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改采集日志信息</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011002" input-node="record" description="修改采集日志信息"/>
      <forward error-code="success" path="update-collect_joumal.jsp"/>
      <forward error-code="failure" path="update-collect_joumal.jsp"/>
    </action>

    <action input="insert-collect_joumal.jsp" name="CollectJoumalUpdateForm" txn-code="6011003" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加采集日志信息</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011003" input-node="record" description="增加采集日志信息"/>
      <forward error-code="success" path="insert-collect_joumal.jsp"/>
      <forward error-code="failure" path="insert-collect_joumal.jsp"/>
    </action>

    <action input="query-collect_joumal.jsp" name="CollectJoumalUpdateForm" txn-code="6011004" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询采集日志用于修改</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011004" input-node="primary-key" output-node="record" description="查询采集日志用于修改"/>
      <forward error-code="success" path="update-collect_joumal.jsp"/>
      <forward error-code="failure" path="update-collect_joumal.jsp"/>
    </action>

    <action input="query-collect_joumal.jsp" name="CollectJoumalDeleteForm" txn-code="6011005" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除采集日志信息</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011005" input-node="primary-key" description="删除采集日志信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    <action input="query-collect_joumal.jsp" name="CollectJoumalRowsetForm" txn-code="6011006" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>查看采集日志信息</description>
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011006" input-node="select-key" output-node="record" description="删除采集日志信息"/>
      <forward error-code="success" path="collect_log_dataitem.jsp"/>
      <forward error-code="failure" path="collect_log_dataitem.jsp"/>
    </action>
    
    <action input="list-query-collect_joumal.jsp" name="CollectJoumalRowsetForm" txn-code="6011020" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询日志列表(采集\共享)</description> 
      <step index="10" bean-name="com.gwssi.log.collectlog.txn.TxnCollectJoumal" func-name="txn6011020" input-node="select-key" output-node="record" description="查询采集日志列表"/>
      <forward error-code="success" path="query-log.jsp"/>
      <forward error-code="failure" path="query-log.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-collect_joumal.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/log/query-collect_joumal.jsp" description="查询采集日志列表"/>
    <resource-file type="jsp" file-name="/page/log/insert-collect_joumal.jsp" description="增加采集日志信息"/>
    <resource-file type="jsp" file-name="/page/log/update-collect_joumal.jsp" description="修改采集日志信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.log.collectlog.vo.CollectJoumalContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.log.collectlog.vo.VoCollectJoumal" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.log.collectlog.vo.VoCollectJoumalSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.log.collectlog.vo.VoCollectJoumalPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

