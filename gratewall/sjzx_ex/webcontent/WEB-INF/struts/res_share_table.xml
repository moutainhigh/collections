<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>共享表信息表</menu-name>
  <menu-path>/page/resource/shareRes/</menu-path>
  <description>共享表信息表</description>
  <form-beans>
    <form-bean name="ResShareTableRowsetForm">
      <description>多记录查询</description>
	  <form-property prefix="select-key" name="business_topics_id" type="string" size="32" description="业务主题ID"/>
      <form-property prefix="record" name="share_table_id" type="string" size="32" cannull="false" description="共享表ID"/>
      <form-property prefix="record" name="business_topics_id" type="string" size="32" cannull="true" description="业务主题ID"/>
      <form-property prefix="record" name="table_name_en" type="string" size="100" cannull="true" description="表名称"/>
      <form-property prefix="record" name="table_name_cn" type="string" size="100" cannull="true" description="表中文名称"/>
      <form-property prefix="record" name="table_no" type="string" size="32" cannull="true" description="表编号"/>
      <form-property prefix="record" name="show_order" type="long" cannull="true" description="显示顺序"/>
      <form-property prefix="record" name="time_" type="string" size="10" cannull="true" description="时间字段"/>
      <form-property prefix="record" name="table_type" type="string" size="20" cannull="true" description="表类型"/>
      <form-property prefix="record" name="table_index" type="string" size="256" cannull="true" description="表索引"/>
      <form-property prefix="record" name="is_markup" type="string" size="1" cannull="true" description="有效标记"/>
    </form-bean>

    <form-bean name="ResShareTableUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="share_table_id" type="string" size="32" cannull="false" description="共享表ID"/>
      <form-property prefix="record" name="share_table_id" type="string" size="32" cannull="false" description="共享表ID"/>
      <form-property prefix="record" name="business_topics_id" type="string" size="32" cannull="true" description="业务主题ID"/>
      <form-property prefix="record" name="table_name_en" type="string" size="100" cannull="true" description="表名称"/>
      <form-property prefix="record" name="table_name_cn" type="string" size="100" cannull="true" description="表中文名称"/>
      <form-property prefix="record" name="table_no" type="string" size="32" cannull="true" description="表编号"/>
      <form-property prefix="record" name="show_order" type="long" cannull="true" description="显示顺序"/>
      <form-property prefix="record" name="time_" type="string" size="10" cannull="true" description="时间字段"/>
      <form-property prefix="record" name="table_type" type="string" size="20" cannull="true" description="表类型"/>
      <form-property prefix="record" name="table_index" type="string" size="256" cannull="true" description="表索引"/>
      <form-property prefix="record" name="is_markup" type="string" size="1" cannull="true" description="有效标记"/>
    </form-bean>

    <form-bean name="ResShareTableDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="share_table_id" type="string" size="32" cannull="false" description="共享表ID"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-res_share_table.jsp" name="ResShareTableRowsetForm" txn-code="20301001" check-type="login" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享表列表</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareTable" func-name="txn20301001" input-node="select-key" output-node="record" description="查询共享表列表"/>
      <forward error-code="success" path="query-res_share_table.jsp"/>
      <forward error-code="failure" path="query-res_share_table.jsp"/>
    </action>

    <action input="update-res_share_table.jsp" name="ResShareTableUpdateForm" txn-code="20301002" check-type="login" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改共享表信息</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareTable" func-name="txn20301002" input-node="record" description="修改共享表信息"/>
      <forward error-code="success" path="update-res_share_table.jsp"/>
      <forward error-code="failure" path="update-res_share_table.jsp"/>
    </action>

    <action input="insert-res_share_table.jsp" name="ResShareTableUpdateForm" txn-code="20301003" check-type="login" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加共享表信息</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareTable" func-name="txn20301003" input-node="record" description="增加共享表信息"/>
      <forward error-code="success" path="insert-res_share_table.jsp"/>
      <forward error-code="failure" path="insert-res_share_table.jsp"/>
    </action>

    <action input="query-res_share_table.jsp" name="ResShareTableUpdateForm" txn-code="20301004" check-type="login" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享表用于修改</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareTable" func-name="txn20301004" input-node="primary-key" output-node="record" description="查询共享表用于修改"/>
      <forward error-code="success" path="update-res_share_table.jsp"/>
      <forward error-code="failure" path="update-res_share_table.jsp"/>
    </action>

    <action input="query-res_share_table.jsp" name="ResShareTableDeleteForm" txn-code="20301005" check-type="login" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除共享表信息</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareTable" func-name="txn20301005" input-node="primary-key" description="删除共享表信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
	
	<action input="query-res_share_table.jsp" name="ResShareTableRowsetForm" txn-code="20301006" check-type="login" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享表列表</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareTable" func-name="txn20301006" input-node="select-key" output-node="record" description="查询共享表列表"/>
      <forward error-code="success" path="query-res_share_table2.jsp"/>
      <forward error-code="failure" path="query-res_share_table2.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-res_share_table.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/resource/shareRes/query-res_share_table.jsp" description="查询共享表列表"/>
    <resource-file type="jsp" file-name="/page/resource/shareRes/insert-res_share_table.jsp" description="增加共享表信息"/>
    <resource-file type="jsp" file-name="/page/resource/shareRes/update-res_share_table.jsp" description="修改共享表信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.ResShareTableContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.VoResShareTable" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.VoResShareTableSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.VoResShareTablePrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

