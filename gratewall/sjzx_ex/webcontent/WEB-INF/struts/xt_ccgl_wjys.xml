<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>文件映射</menu-name>
  <menu-path>/page/file/manage/</menu-path>
  <description>文件映射</description>
  <form-beans>
    <form-bean name="XtCcglWjysRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="ysbh_pk" type="string" size="32" description="映射编号"/>
      <form-property prefix="record" name="ysbh_pk" type="string" size="32" cannull="false" description="映射编号"/>
      <form-property prefix="record" name="cclbbh_pk" type="string" size="32" cannull="true" description="类别编号ID"/>
      <form-property prefix="record" name="wybs" type="string" size="255" cannull="false" description="唯一标识"/>
      <form-property prefix="record" name="wjmc" type="string" size="255" cannull="false" description="文件名称"/>
      <form-property prefix="record" name="wjzt" type="string" size="1" cannull="true" description="文件状态"/>
      <form-property prefix="record" name="cclj" type="string" size="255" cannull="true" description="存储路径"/>
      <form-property prefix="record" name="cjsj" type="string" size="20" cannull="true" description="创建时间"/>
      <form-property prefix="record" name="scxgsj" type="string" size="20" cannull="true" description="上次修改时间"/>
      <form-property prefix="record" name="bz" type="string" size="255" cannull="true" description="备注"/>
      <form-property prefix="record" name="ywbz" type="string" size="32" cannull="true" description="业务备注"/>
      <form-property prefix="record" name="xm_fk" type="string" size="32" cannull="true" description="项目fk"/>
    </form-bean>

    <form-bean name="XtCcglWjysUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="ysbh_pk" type="string" size="32" cannull="false" description="映射编号"/>
      <form-property prefix="record" name="ysbh_pk" type="string" size="32" cannull="false" description="映射编号"/>
      <form-property prefix="record" name="cclbbh_pk" type="string" size="32" cannull="true" description="类别编号ID"/>
      <form-property prefix="record" name="wybs" type="string" size="255" cannull="false" description="唯一标识"/>
      <form-property prefix="record" name="wjmc" type="string" size="255" cannull="false" description="文件名称"/>
      <form-property prefix="record" name="wjzt" type="string" size="1" cannull="true" description="文件状态"/>
      <form-property prefix="record" name="cclj" type="string" size="255" cannull="true" description="存储路径"/>
      <form-property prefix="record" name="cjsj" type="string" size="20" cannull="true" description="创建时间"/>
      <form-property prefix="record" name="scxgsj" type="string" size="20" cannull="true" description="上次修改时间"/>
      <form-property prefix="record" name="bz" type="string" size="255" cannull="true" description="备注"/>
      <form-property prefix="record" name="ywbz" type="string" size="32" cannull="true" description="业务备注"/>
      <form-property prefix="record" name="xm_fk" type="string" size="32" cannull="true" description="项目fk"/>
    </form-bean>

    <form-bean name="XtCcglWjysDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="ysbh_pk" type="string" size="32" cannull="false" description="映射编号"/>
    </form-bean>
    
    <form-bean name="Xt_ccgl_downLoadForm">
      <description>系统存储文件下载</description>
      <form-property prefix="primary-key" name="ysbh_pk" type="int" cannull="false" description="文件映射编号"/>
      <form-property prefix="primary-key" name="wjmc" type="string" size="60" cannull="false" description="文件名称"/>
      <form-property prefix="primary-key" name="logFlag" type="string" size="10" cannull="false" description="标志"/>
      <form-property prefix="record" name="cclbbh_pk" type="int" cannull="false" description="文件类别编号"/>
      <form-property prefix="record" name="wjmc" type="string" size="60" cannull="false" description="文件名称"/>
      <form-property prefix="record" name="cclj" type="string" cannull="true" description="存储路径"/>
      <form-property prefix="record" name="wybs" type="int" size="255" cannull="false" description="唯一标识"/>      
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-xt_ccgl_wjys.jsp" name="XtCcglWjysRowsetForm" txn-code="604060101" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询文件映射列表</description>
      <step index="10" bean-name="com.gwssi.file.manage.txn.TxnXtCcglWjys" func-name="txn604060101" input-node="select-key" output-node="record" description="查询文件映射列表"/>
      <forward error-code="success" path="query-xt_ccgl_wjys.jsp"/>
      <forward error-code="failure" path="query-xt_ccgl_wjys.jsp"/>
    </action>

    <action input="update-xt_ccgl_wjys.jsp" name="XtCcglWjysUpdateForm" txn-code="604060102" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改文件映射信息</description>
      <step index="10" bean-name="com.gwssi.file.manage.txn.TxnXtCcglWjys" func-name="txn604060102" input-node="record" description="修改文件映射信息"/>
      <forward error-code="success" path="update-xt_ccgl_wjys.jsp"/>
      <forward error-code="failure" path="update-xt_ccgl_wjys.jsp"/>
    </action>

    <action input="insert-xt_ccgl_wjys.jsp" name="XtCcglWjysUpdateForm" txn-code="604060103" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加文件映射信息</description>
      <step index="10" bean-name="com.gwssi.file.manage.txn.TxnXtCcglWjys" func-name="txn604060103" input-node="record" description="增加文件映射信息"/>
      <forward error-code="success" path="insert-xt_ccgl_wjys.jsp"/>
      <forward error-code="failure" path="insert-xt_ccgl_wjys.jsp"/>
    </action>

    <action input="query-xt_ccgl_wjys.jsp" name="XtCcglWjysUpdateForm" txn-code="604060104" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询文件映射用于修改</description>
      <step index="10" bean-name="com.gwssi.file.manage.txn.TxnXtCcglWjys" func-name="txn604060104" input-node="primary-key" output-node="record" description="查询文件映射用于修改"/>
      <forward error-code="success" path="update-xt_ccgl_wjys.jsp"/>
      <forward error-code="failure" path="update-xt_ccgl_wjys.jsp"/>
    </action>

    <action input="query-xt_ccgl_wjys.jsp" name="XtCcglWjysDeleteForm" txn-code="604060105" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除文件映射信息</description>
      <step index="10" bean-name="com.gwssi.file.manage.txn.TxnXtCcglWjys" func-name="txn604060105" input-node="primary-key" description="删除文件映射信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    
    <action input="query-xt_ccgl_wjlb.jsp" name="Xt_ccgl_downLoadForm" txn-code="604050209" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>下载文件</description>
      <step index="10" bean-name="com.gwssi.file.manage.txn.TxnXtCcglWjys" func-name="txn604050209" input-node="primary-key" output-node="record"/>
      <forward error-code="success" path="/script/deletesuccess.jsp" redirect="false"/>
      <forward error-code="failure" path="/script/deletefailure.jsp" redirect="false"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-xt_ccgl_wjys.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/file/manage/query-xt_ccgl_wjys.jsp" description="查询文件映射列表"/>
    <resource-file type="jsp" file-name="/page/file/manage/insert-xt_ccgl_wjys.jsp" description="增加文件映射信息"/>
    <resource-file type="jsp" file-name="/page/file/manage/update-xt_ccgl_wjys.jsp" description="修改文件映射信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.file.manage.vo.XtCcglWjysContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.file.manage.vo.VoXtCcglWjys" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.file.manage.vo.VoXtCcglWjysSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.file.manage.vo.VoXtCcglWjysPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

