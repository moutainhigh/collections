      <forward error-code="failure" path="query-sys_rd_standard.jsp"/>
    </action>
	    <action input="query-sys_rd_standard.jsp" name="SysRdStandardRowsetForm" txn-code="7000207" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
