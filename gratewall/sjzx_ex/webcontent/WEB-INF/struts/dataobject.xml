<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>数据权限类型管理</menu-name>
  <menu-path>/page/sysmgr/priv/datapriv/dataprivgroup/</menu-path>
  <description>数据权限类型管理</description>
  <form-beans>
    <form-bean name="DataobjectRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="objectsource" type="string" size="500" description="数据权限来源"/>
      <form-property prefix="select-key" name="parentcode" type="string" size="500" description="数据权限来源"/>
      
      <form-property prefix="select-key" name="dataaccgrpid" type="string" size="500" description="数据权限组ID"/>
      <form-property prefix="select-key" name="objectid" type="string" size="500" description="数据权限来源ID"/>
      <form-property prefix="select-key" name="rolefunids" type="string" size="50000" description="数据权限来源ID"/>
      <form-property prefix="select-key" name="dataaccdispobj" type="string" size="1" description="数据权限分配对象"/>
      
      <form-property prefix="record" name="objectid" type="long" cannull="false" description="数据权限类型代码"/>
      <form-property prefix="record" name="objectsource" type="string" size="500" cannull="false" description="数据权限来源"/>
      <form-property prefix="record" name="functionIds" type="string" size="50000" cannull="false" description="对应的功能串"/>

      <form-property prefix="record" name="id" type="string" size="50000" cannull="false" description="数据权限项ID"/>
      <form-property prefix="record" name="name" type="string" size="50000" cannull="false" description="数据权限项名称"/>
      <form-property prefix="record" name="code" type="string" size="50000" cannull="false" description="数据权限项代码"/>
      <form-property prefix="record" name="expand" type="string" size="50000" cannull="false" description="是否有子节点"/>
      <form-property prefix="record" name="checked" type="string" size="50000" cannull="false" description="是否选中"/>
    
    </form-bean>

    <form-bean name="DataobjectUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="objectid" type="long" cannull="false" description="数据权限类型代码"/>
      <form-property prefix="record" name="objectid" type="long" cannull="false" description="数据权限类型代码"/>
      <form-property prefix="record" name="objectsource" type="string" size="500" cannull="false" description="数据权限来源"/>
    </form-bean>

    <form-bean name="DataobjectDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="objectid" type="long" cannull="false" description="数据权限类型代码"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-dataobject.jsp" name="DataobjectRowsetForm" txn-code="103011" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询数据权限类型管理列表</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103011" input-node="select-key" output-node="record" description="查询数据权限类型管理列表"/>
      <forward error-code="success" path="query-dataobject.jsp"/>
      <forward error-code="failure" path="query-dataobject.jsp"/>
    </action>

    <action input="update-dataobject.jsp" name="DataobjectUpdateForm" txn-code="103012" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改数据权限类型管理信息</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103012" input-node="record" description="修改数据权限类型管理信息"/>
      <forward error-code="success" path="update-dataobject.jsp"/>
      <forward error-code="failure" path="update-dataobject.jsp"/>
    </action>

    <action input="insert-dataobject.jsp" name="DataobjectUpdateForm" txn-code="103013" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加数据权限类型管理信息</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103013" input-node="record" description="增加数据权限类型管理信息"/>
      <forward error-code="success" path="insert-dataobject.jsp"/>
      <forward error-code="failure" path="insert-dataobject.jsp"/>
    </action>

    <action input="query-dataobject.jsp" name="DataobjectUpdateForm" txn-code="103014" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询数据权限类型管理用于修改</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103014" input-node="primary-key" output-node="record" description="查询数据权限类型管理用于修改"/>
      <forward error-code="success" path="update-dataobject.jsp"/>
      <forward error-code="failure" path="update-dataobject.jsp"/>
    </action>

    <action input="query-dataobject.jsp" name="DataobjectDeleteForm" txn-code="103015" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除数据权限类型管理信息</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103015" input-node="primary-key" description="删除数据权限类型管理信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    
    <action input="query-dataobject.jsp" name="DataobjectRowsetForm" txn-code="103016" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>保存数据权限类型配置关系</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103016" input-node="record" output-node="record" description="保存数据权限类型配置关系"/>
      <forward error-code="success" path="/txn103011.do"/>
      <forward error-code="failure" path="query-dataobject.jsp"/>
    </action>
    
    <action input="query-dataobject.jsp" name="DataobjectRowsetForm" txn-code="103017" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询数据权限项目(自定义权限组)</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103017" input-node="select-key" output-node="record" description="查询数据权限项目(自定义权限组)"/>
      <forward error-code="success" path="query-dataobject.jsp"/>
      <forward error-code="failure" path="query-dataobject.jsp"/>
    </action>
    <action input="query-dataobject.jsp" name="DataobjectRowsetForm" txn-code="103018" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询数据权限项目</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103018" input-node="select-key" output-node="record" description="查询数据权限项目"/>
      <forward error-code="success" path="query-dataobject.jsp"/>
      <forward error-code="failure" path="query-dataobject.jsp"/>
    </action>
    
    <action input="query-dataobject.jsp" name="DataobjectRowsetForm" txn-code="103019" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>删除数据权限类型配置关系</description>
      <step index="10" bean-name="com.gwssi.sysmgr.priv.datapriv.txn.TxnDataObject" func-name="txn103019" input-node="record" output-node="record" description="删除数据权限类型配置关系"/>
      <forward error-code="success" path="/txn103011.do"/>
      <forward error-code="failure" path="query-dataobject.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-dataobject.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/xtgl/qxgl/sjqxlxgl/query-dataobject.jsp" description="查询数据权限类型管理列表"/>
    <resource-file type="jsp" file-name="/xtgl/qxgl/sjqxlxgl/insert-dataobject.jsp" description="增加数据权限类型管理信息"/>
    <resource-file type="jsp" file-name="/xtgl/qxgl/sjqxlxgl/update-dataobject.jsp" description="修改数据权限类型管理信息"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.xtgl.vo.DataobjectContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.xtgl.vo.VoDataobject" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.xtgl.vo.VoDataobjectSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.xtgl.vo.VoDataobjectPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

