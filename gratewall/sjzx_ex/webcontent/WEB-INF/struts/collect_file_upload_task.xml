      <form-property prefix="record" name="delNAMEs" type="string" size="2000" cannull="true" description="delNAMEs"/>
      <form-property prefix="record" name="delIDs" type="string" size="2000" cannull="true" description="delIDs"/>
    </form-bean>

    <form-bean name="CollectFileUploadTaskUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="file_upload_task_id" type="string" size="32" cannull="false" description="文件上传任务ID"/>
      <form-property prefix="record" name="file_upload_task_id" type="string" size="32" cannull="false" description="文件上传任务ID"/>
      <form-property prefix="record" name="collect_task_id" type="string" size="32" cannull="true" description="采集任务ID"/>
      <form-property prefix="record" name="collect_table" type="string" size="32" cannull="true" description="采集表"/>
      <form-property prefix="record" name="collect_mode" type="string" size="20" cannull="true" description="采集模式"/>
      <form-property prefix="record" name="file_status" type="string" size="20" cannull="true" description="文件状态"/>
      <form-property prefix="record" name="file_description" type="string" size="2000" cannull="true" description="文件描述"/>
      <form-property prefix="record" name="collect_file_name" type="string" size="200" cannull="true" description="采集文件名称"/>
      <form-property prefix="record" name="collect_file_id" type="string" size="32" cannull="true" description="采集文件ID"/>
      <form-property prefix="record" name="check_result_file_name" type="string" size="300" cannull="true" description="校验结果文件名称"/>
