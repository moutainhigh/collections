      <description>查询服务对象列表框架页</description>
      <step index="10" bean-name="com.gwssi.resource.svrobj.txn.TxnResServiceTargets" func-name="txn201012" input-node="select-key" output-node="record" description="查询对象列表"/>
      <forward error-code="success" path="list-res_service_targets.jsp"/>
      <forward error-code="failure" path="list-res_service_targets.jsp"/>
    </action>
  
    <action input="query-res_service_targets.jsp" name="ResServiceTargetsRowsetForm" txn-code="201001" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询服务对象列表</description>
      <step index="10" bean-name="com.gwssi.resource.svrobj.txn.TxnResServiceTargets" func-name="txn201001" input-node="select-key" output-node="record" description="查询对象列表"/>
