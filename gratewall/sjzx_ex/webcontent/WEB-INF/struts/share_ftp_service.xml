<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>共享ftp服务</menu-name>
  <menu-path>/page/share/ftp/service/</menu-path>
  <description>共享ftp服务</description>
  <form-beans>
    <form-bean name="ShareFtpServiceRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="record" name="ftp_service_id" type="string" size="32" cannull="false" description="FTP服务ID"/>
      <form-property prefix="record" name="service_id" type="string" size="32" cannull="true" description="服务ID"/>
      <form-property prefix="record" name="datasource_id" type="string" size="32" cannull="true" description="数据源ID"/>
      <form-property prefix="record" name="srv_scheduling_id" type="string" size="32" cannull="true" description="服务调度ID"/>
      <form-property prefix="record" name="file_name" type="string" size="100" cannull="true" description="文件名称"/>
      <form-property prefix="record" name="file_type" type="string" size="20" cannull="true" description="文件类型"/>
    </form-bean>

    <form-bean name="ShareFtpServiceUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="ftp_service_id" type="string" size="32" cannull="false" description="FTP服务ID"/>
      <form-property prefix="primary-key" name="service_id" type="string" size="32" cannull="false" description="服务ID"/>
      <form-property prefix="primary-key" name="service_no" type="string" size="32" cannull="false" description="服务编号"/>
      <form-property prefix="primary-key" name="service_name" type="string" size="32" cannull="false" description="服务名称"/>
      <form-property prefix="primary-key" name="service_targets_id" type="string" size="32" cannull="false" description="服务对象ID"/>
      <form-property prefix="record1" name="ftp_service_id" type="string" size="32" cannull="false" description="FTP服务ID"/>
      <form-property prefix="record1" name="service_id" type="string" size="32" cannull="true" description="服务ID"/>
      <form-property prefix="record1" name="service_no" type="string" size="32" cannull="false" description="服务编号"/>
      <form-property prefix="record1" name="service_name" type="string" size="32" cannull="false" description="服务名称"/>
      <form-property prefix="record1" name="service_targets_id" type="string" size="32" cannull="true" description="服务对象ID"/>
      <form-property prefix="record1" name="datasource_id" type="string" size="32" cannull="true" description="数据源ID"/>
      <form-property prefix="record1" name="srv_scheduling_id" type="string" size="32" cannull="true" description="服务调度ID"/>
      <form-property prefix="record1" name="scheduling_type" type="string" size="32" cannull="true" description="服务调度类型"/>
      <form-property prefix="record1" name="start_time" type="string" size="32" cannull="true" description="开始时间"/>
      <form-property prefix="record1" name="end_time" type="string" size="32" cannull="true" description="结束时间"/>
      <form-property prefix="record1" name="scheduling_week" type="string" size="32" cannull="true" description="计划任务周天"/>
      <form-property prefix="record1" name="scheduling_day" type="string" size="32" cannull="true" description="计划任务日期"/>
      <form-property prefix="record1" name="scheduling_day1" type="string" size="32" cannull="true" description="计划任务周期中文"/>
      <form-property prefix="record1" name="scheduling_count" type="string" size="32" cannull="true" description="计划任务执行次数"/>
      <form-property prefix="record1" name="interval_time" type="string" size="32" cannull="true" description="每次间隔时间"/>
      
      
      
      
      
      
      <form-property prefix="record1" name="file_name" type="string" size="100" cannull="true" description="文件名称"/>
      <form-property prefix="record1" name="file_type" type="string" size="20" cannull="true" description="文件类型"/>
      <form-property prefix="record1" name="num" type="string" size="32" cannull="false" description="数量"/>
    </form-bean>

    <form-bean name="ShareFtpServiceDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="ftp_service_id" type="string" size="32" cannull="false" description="FTP服务ID"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-share_ftp_service.jsp" name="ShareFtpServiceRowsetForm" txn-code="40401001" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享ftp服务列表</description>
      <step index="10" bean-name="com.gwssi.share.ftp.txn.TxnShareFtpService" func-name="txn40401001" input-node="select-key" output-node="record" description="查询共享ftp服务列表"/>
      <forward error-code="success" path="query-share_ftp_service.jsp"/>
      <forward error-code="failure" path="query-share_ftp_service.jsp"/>
    </action>

    <action input="update-share_ftp_service.jsp" name="ShareFtpServiceUpdateForm" txn-code="40401002" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>修改共享ftp服务信息</description>
      <step index="10" bean-name="com.gwssi.share.ftp.txn.TxnShareFtpService" func-name="txn40401002" input-node="record1" description="修改共享ftp服务信息"/>
      <forward error-code="success" path="update-share_ftp_service.jsp"/>
      <forward error-code="failure" path="update-share_ftp_service.jsp"/>
    </action>

    <action input="insert-share_ftp_service.jsp" name="ShareFtpServiceUpdateForm" txn-code="40401003" check-type="public" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加共享ftp服务信息</description>
      <step index="10" bean-name="com.gwssi.share.ftp.txn.TxnShareFtpService" func-name="txn40401003" input-node="record" description="增加共享ftp服务信息"/>
      <forward error-code="success" path="insert-share_ftp_service.jsp"/>
      <forward error-code="failure" path="insert-share_ftp_service.jsp"/>
    </action>

    <action input="query-share_ftp_service.jsp" name="ShareFtpServiceUpdateForm" txn-code="40401004" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享ftp服务用于修改</description>
      <step index="10" bean-name="com.gwssi.share.ftp.txn.TxnShareFtpService" func-name="txn40401004" input-node="primary-key" output-node="record" description="查询共享ftp服务用于修改"/>
      <forward error-code="success" path="update-share_ftp_service.jsp"/>
      <forward error-code="failure" path="update-share_ftp_service.jsp"/>
    </action>

    <action input="query-share_ftp_service.jsp" name="ShareFtpServiceDeleteForm" txn-code="40401005" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除共享ftp服务信息</description>
      <step index="10" bean-name="com.gwssi.share.ftp.txn.TxnShareFtpService" func-name="txn40401005" input-node="primary-key" description="删除共享ftp服务信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-share_ftp_service.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/share/ftp/service/query-share_ftp_service.jsp" description="查询共享ftp服务列表"/>
    <resource-file type="jsp" file-name="/page/share/ftp/service/insert-share_ftp_service.jsp" description="增加共享ftp服务信息"/>
    <resource-file type="jsp" file-name="/page/share/ftp/service/update-share_ftp_service.jsp" description="修改共享ftp服务信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.ftp.vo.ShareFtpServiceContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.ftp.vo.VoShareFtpService" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.ftp.vo.VoShareFtpServiceSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.ftp.vo.VoShareFtpServicePrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

