<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>trs服务接口</menu-name>
  <menu-path>/page/share/trs/</menu-path>
  <description>trs服务接口</description>
  <form-beans>
    <form-bean name="TrsShareServiceRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="trs_service_name" type="string" size="32" description="服务名称"/>
      <form-property prefix="select-key" name="service_targets_id" type="string" size="32" description="服务名称"/>
      <form-property prefix="select-key" name="service_targets_type" type="string" size="32" description="服务名称"/>
	  <form-property prefix="select-key" name="service_state" type="string" size="32" description="服务状态"/>
      <form-property prefix="select-key" name="created_time" type="string" size="32" description="服务名称"/>
   
      <form-property prefix="record" name="trs_service_id" type="string" size="32" cannull="false" description="服务ID"/>
      <form-property prefix="record" name="service_targets_id" type="string" size="32" cannull="true" description="所属服务对象ID"/>
      <form-property prefix="record" name="trs_service_name" type="string" size="32" cannull="true" description="服务名称"/>
      <form-property prefix="record" name="trs_service_no" type="string" size="20" cannull="true" description="服务编号"/>
      <form-property prefix="record" name="trs_data_base" type="string" size="1000" cannull="true" description="服务库"/>
      <form-property prefix="record" name="trs_column" type="string" size="4000" cannull="true" description="展示字段"/>
      <form-property prefix="record" name="service_state" type="string" size="20" cannull="true" description="是否启用"/>
      <form-property prefix="record" name="is_markup" type="string" size="1" cannull="true" description="是否有效"/>
      <form-property prefix="record" name="creator_id" type="string" size="32" cannull="true" description="创建人ID"/>
      <form-property prefix="record" name="created_time" type="string" size="19" cannull="true" description="创建时间"/>
      <form-property prefix="record" name="last_modify_id" type="string" size="32" cannull="true" description="最后修改人ID"/>
      <form-property prefix="record" name="last_modify_time" type="string" size="19" cannull="true" description="最后修改时间"/>
      <form-property prefix="record" name="service_description" type="string" size="2000" cannull="true" description="服务说明"/>
      <form-property prefix="record" name="trs_search_column" type="string" size="4000" cannull="true" description="检索字段"/>
	  <form-property prefix="record" name="use_template" type="string" size="1" cannull="true" description="是否使用模板文件"/>
	  <form-property prefix="record" name="trs_template" type="string" size="4000" cannull="true" description="模板文件"/>
   	 
	  <form-property name="dataString" type="string"  description="ajax返回select里json数据"/>
   	  <form-property name="svrTarget" type="string"  description="ajax返回select里json数据"/>
	   <form-property name="svrState" type="string"  description="ajax返回select里json数据"/>
    </form-bean>

    <form-bean name="TrsShareServiceUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="trs_service_id" type="string" size="32" cannull="false" description="服务ID"/>
      <form-property prefix="primary-key" name="service_state" type="string" size="20" cannull="false" description="是否启用"/>
	  <form-property prefix="record" name="trs_service_id" type="string" size="32" cannull="false" description="服务ID"/>
      <form-property prefix="record" name="service_targets_id" type="string" size="32" cannull="true" description="所属服务对象ID"/>
      <form-property prefix="record" name="trs_service_name" type="string" size="32" cannull="true" description="服务名称"/>
      <form-property prefix="record" name="trs_service_no" type="string" size="20" cannull="true" description="服务编号"/>
      <form-property prefix="record" name="trs_data_base" type="string" size="1000" cannull="true" description="服务库"/>
      <form-property prefix="record" name="trs_column" type="string" size="4000" cannull="true" description="展示字段"/>
      <form-property prefix="record" name="service_state" type="string" size="20" cannull="true" description="是否启用"/>
      <form-property prefix="record" name="is_markup" type="string" size="1" cannull="true" description="是否有效"/>
      <form-property prefix="record" name="creator_id" type="string" size="32" cannull="true" description="创建人ID"/>
      <form-property prefix="record" name="created_time" type="string" size="19" cannull="true" description="创建时间"/>
      <form-property prefix="record" name="last_modify_id" type="string" size="32" cannull="true" description="最后修改人ID"/>
      <form-property prefix="record" name="last_modify_time" type="string" size="19" cannull="true" description="最后修改时间"/>
      <form-property prefix="record" name="service_description" type="string" size="2000" cannull="true" description="服务说明"/>
   	  <form-property prefix="record" name="trs_search_column" type="string" size="4000" cannull="true" description="检索字段"/>
	  <form-property prefix="record" name="use_template" type="string" size="1" cannull="true" description="是否使用模板文件"/>
	  <form-property prefix="record" name="trs_template" type="string" size="4000" cannull="true" description="模板文件"/>
	  <form-property prefix="record" name="limit_data" type="string" cannull="true" description="ajax提交json数据2"/>
	  
	  <form-property prefix="record" name="service_type" type="string" size="20" cannull="true" description="服务类型"/>
   	 
    </form-bean>

    <form-bean name="TrsShareServiceDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="trs_service_id" type="string" size="32" cannull="false" description="服务ID"/>
    </form-bean>
  </form-beans>

  <action-mappings>
  
    <action input="query-trs_share_service.jsp" name="TrsShareServiceRowsetForm" txn-code="40300011" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询trs接口列表-框架页</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300011" input-node="select-key" output-node="record" description="查询trs接口列表"/>
      <forward error-code="success" path="query-trs_share_service.jsp"/>
      <forward error-code="failure" path="query-trs_share_service.jsp"/>
    </action>
  
    <action input="query-trs_share_service.jsp" name="TrsShareServiceRowsetForm" txn-code="40300001" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询trs接口列表</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300001" input-node="select-key" output-node="record" description="查询trs接口列表"/>
      <forward error-code="success" path="query-trs_share_service.jsp"/>
      <forward error-code="failure" path="query-trs_share_service.jsp"/>
    </action>

    <action input="update-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300002" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改trs接口信息</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300002" input-node="record" description="修改trs接口信息"/>
      <forward error-code="success" path="update-trs_share_service.jsp"/>
      <forward error-code="failure" path="update-trs_share_service.jsp"/>
    </action>

    <action input="insert-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300003" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加trs接口信息</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300003" input-node="record" description="增加trs接口信息"/>
      <forward error-code="success" path="/script/insertsuccess.jsp?nextPage=txn40300001.do" redirect="true" />
      <forward error-code="failure" path="insert-trs_share_service.jsp"/>
    </action>

    <action input="query-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300004" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询trs接口用于修改</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300004" input-node="primary-key" output-node="record" description="查询trs接口用于修改"/>
      <forward error-code="success" path="update-trs_share_service.jsp"/>
      <forward error-code="failure" path="update-trs_share_service.jsp"/>
    </action>

    <action input="query-trs_share_service.jsp" name="TrsShareServiceDeleteForm" txn-code="40300005" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除trs接口信息</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300005" input-node="primary-key" description="删除trs接口信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
	
    <action input="query-trs_share_service.jsp" name="TrsShareServiceRowsetForm" txn-code="40300006" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询trs最大编号</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300006" input-node="select-key" output-node="record" description="查询trs最大编号"/>
      <forward error-code="success" path="query-trs_share_service.jsp"/>
      <forward error-code="failure" path="query-trs_share_service.jsp"/>
    </action>	
	
    <action input="query-trs_share_service.jsp" name="TrsShareServiceRowsetForm" txn-code="40300007" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询已有TRS库</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300007" input-node="select-key" output-node="record" description="查询trs库列表"/>
      <forward error-code="success" path="query-trs_share_service.jsp"/>
      <forward error-code="failure" path="query-trs_share_service.jsp"/>
    </action>	
	
	<action input="query-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300008" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>trs共享服务(启用停用)</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300008" input-node="select-key" description="trs共享服务(启用停用)息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
	
	<action input="query-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300009" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询trs接口用于展示</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300004" input-node="primary-key" output-node="record" description="查询trs接口用于修改"/>
      <forward error-code="success" path="show-trs_share_service.jsp"/>
      <forward error-code="failure" path="show-trs_share_service.jsp"/>
    </action>
    
    <action input="query-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300010" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询trs接口用于修改</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300010" input-node="primary-key" output-node="record" description="查询trs接口用于修改"/>
      <forward error-code="success" path="rule-trs_share_service.jsp"/>
      <forward error-code="failure" path="rule-trs_share_service.jsp"/>
    </action>
    
    <action input="query-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300012" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>配置trs接口规则</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300012" input-node="primary-key" output-node="record" description="查询trs接口用于修改"/>
      <forward error-code="success" path="rule-trs_share_service.jsp"/>
      <forward error-code="failure" path="rule-trs_share_service.jsp"/>
    </action>
    
    <action input="query-trs_share_service.jsp" name="TrsShareServiceUpdateForm" txn-code="40300013" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>导出共享服务</description>
      <step index="10" bean-name="com.gwssi.share.trs.txn.TxnTrsShareService" func-name="txn40300013" input-node="primary-key" output-node="record" description="查询trs接口用于修改"/>
      <forward error-code="success" path="file-trs_share_service.jsp"/>
      <forward error-code="failure" path="file-trs_share_service.jsp"/>
    </action>
    
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-trs_share_service.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/share/trs/query-trs_share_service.jsp" description="查询trs接口列表"/>
    <resource-file type="jsp" file-name="/page/share/trs/insert-trs_share_service.jsp" description="增加trs接口信息"/>
    <resource-file type="jsp" file-name="/page/share/trs/update-trs_share_service.jsp" description="修改trs接口信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.trs.vo.TrsShareServiceContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.trs.vo.VoTrsShareService" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.trs.vo.VoTrsShareServiceSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.share.trs.vo.VoTrsShareServicePrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

