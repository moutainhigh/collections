<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>行政区划</menu-name>
  <menu-path>/page/ysjgl/jcsjgl/xzqhgl/xzqh/</menu-path>
  <description>行政区划</description>
  <form-beans>
    <form-bean name="GzDmXzqhRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="bb_id" type="string" size="2" description="版本ID"/>
      <form-property prefix="select-key" name="xzqh_nm" type="string" size="36" cannull="true" description="行政区划内码"/>
      <form-property prefix="select-key" name="xzqh_fjd" type="string" size="36" cannull="true" description="行政区划父节点"/>
      <form-property prefix="select-key" name="xzqh_mc" type="string" size="255" description="行政区划名称"/>
      <form-property prefix="select-key" name="xzqh_dm" type="string" size="12" description="行政区划代码"/>
      <form-property prefix="record" name="xzqh_id" type="string" size="36" cannull="false" description="行政区划ID"/>
      <form-property prefix="record" name="xzqh_nm" type="string" size="36" cannull="true" description="行政区划内码"/>
      <form-property prefix="record" name="bb_id" type="string" size="2" cannull="true" description="版本ID"/>
      <form-property prefix="record" name="xzqh_dm" type="string" size="12" cannull="true" description="行政区划代码"/>
      <form-property prefix="record" name="xzqh_mc" type="string" size="255" cannull="true" description="行政区划名称"/>
      <form-property prefix="record" name="parent_node_name" type="string" size="255" cannull="true" description="上级行政区划名称"/>
      <form-property prefix="record" name="xzqh_cn_ms" type="string" size="255" cannull="true" description="行政区划中文描述"/>
      <form-property prefix="record" name="xzqh_en_mc" type="string" size="255" cannull="true" description="行政区划英文名称"/>
      <form-property prefix="record" name="xzqh_en_ms" type="string" size="255" cannull="true" description="行政区划英文描述"/>
      <form-property prefix="record" name="xzqh_cjm" type="string" size="12" cannull="true" description="行政区划层级码"/>
      <form-property prefix="record" name="xzqh_fjd" type="string" size="36" cannull="true" description="行政区划父节点"/>
      <form-property prefix="record" name="xzqh_cc" type="string" size="2" cannull="true" description="所在层次"/>
      <form-property prefix="record" name="szcc_sxm" type="int" cannull="true" description="顺序码"/>
      <form-property prefix="record" name="sf_mx" type="string" size="1" cannull="true" description="是否明细"/>
      <form-property prefix="record" name="expand" type="string" size="6" cannull="true" description="是否展开树节点"/>
    </form-bean>

    <form-bean name="GzDmXzqhUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="xzqh_id" type="string" size="36" cannull="false" description="行政区划ID"/>
      <form-property prefix="select-key" name="source_bb_id" type="string" size="2" cannull="true" description="版本ID"/>
      <form-property prefix="select-key" name="xzqh_nm" type="string" size="36" cannull="true" description="行政区划内码"/>
      <form-property prefix="select-key" name="xzqh_fjd" type="string" size="36" cannull="true" description="父节点代码"/>
      <form-property prefix="select-key" name="bb_id" type="string" size="2" cannull="true" description="版本ID"/>
      <form-property prefix="select-key" name="bb_mc" type="string" size="255" description="版本名称"/>
      
      <form-property prefix="record" name="first_id" type="string" size="36" cannull="false" description="行政区划ID"/>
