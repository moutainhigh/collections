<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>地理行政关联</menu-name>
  <menu-path>/page/ysjgl/jcsjgl/xzqhgl/dlxz/</menu-path>
  <description>地理行政关联</description>
  <form-beans>
    <form-bean name="GzDmDlxzRowsetForm">
      <description>多记录查询</description>  
      <form-property prefix="select-key" name="dlqy_dm" type="string" size="2" description="地理区域代码"/>
      <form-property prefix="select-key" name="xzqh_id" type="string" size="36" cannull="true" description="行政区划ID"/>
      <form-property prefix="record" name="gl_id" type="string" size="36" cannull="false" description="地理区域行政区划关联ID"/>
      <form-property prefix="record" name="dlqy_dm" type="string" size="2" cannull="true" description="地理区域代码"/>
      <form-property prefix="record" name="xzqh_id" type="string" size="36" cannull="true" description="行政区划ID"/>
    </form-bean>

    <form-bean name="GzDmDlxzUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="gl_id" type="string" size="36" cannull="false" description="地理区域行政区划关联ID"/>
      <form-property prefix="record" name="gl_id" type="string" size="36" cannull="false" description="地理区域行政区划关联ID"/>
      <form-property prefix="record" name="dlqy_dm" type="string" size="2" cannull="true" description="地理区域代码"/>
      <form-property prefix="record" name="xzqh_id" type="string" size="36" cannull="true" description="行政区划ID"/>
      <form-property prefix="record" name="selectedDlqyString" type="string" size="255" cannull="true" description="已经选择的地理区域"/>
    </form-bean>

    <form-bean name="GzDmDlxzDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="gl_id" type="string" size="36" cannull="false" description="地理区域行政区划关联ID"/>
    </form-bean>
  </form-beans>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-gz_dm_dlxz.xml" description="模板文件"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.ysjgl.vo.GzDmDlxzContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.ysjgl.vo.VoGzDmDlxz" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.ysjgl.vo.VoGzDmDlxzSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.csdb.ysjgl.vo.VoGzDmDlxzPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

