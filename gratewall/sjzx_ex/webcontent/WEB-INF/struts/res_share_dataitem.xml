<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>共享数据项信息表</menu-name>
  <menu-path>/page/resource/shareRes/</menu-path>
  <description>共享数据项信息表</description>
  <form-beans>
    <form-bean name="ResShareDataitemRowsetForm">
      <description>多记录查询</description>
	  <form-property prefix="select-key" name="share_table_id" type="string" size="32" description="共享表ID"/>
      
      <form-property prefix="record" name="share_dataitem_id" type="string" size="32" cannull="false" description="共享数据项ID"/>
      <form-property prefix="record" name="share_table_id" type="string" size="32" cannull="true" description="共享表ID"/>
      <form-property prefix="record" name="dataitem_name_en" type="string" size="100" cannull="true" description="数据项名称"/>
      <form-property prefix="record" name="dataitem_name_cn" type="string" size="100" cannull="true" description="数据项中文名称"/>
      <form-property prefix="record" name="dataitem_type" type="string" size="20" cannull="true" description="数据项类型"/>
      <form-property prefix="record" name="dataitem_long" type="string" size="4" cannull="true" description="数据项长度"/>
      <form-property prefix="record" name="code_table_name" type="string" size="32" cannull="true" description="系统代码名"/>
      <form-property prefix="record" name="code_table" type="string" size="32" cannull="true" description="对应代码集"/>
      <form-property prefix="record" name="is_key" type="string" size="1" cannull="true" description="是否主键"/>
      <form-property prefix="record" name="dataitem_desc" type="string" size="2000" cannull="true" description="描述"/>
      <form-property prefix="record" name="show_order" type="long" cannull="true" description="显示顺序"/>
      <form-property prefix="record" name="is_markup" type="string" size="1" cannull="true" description="有效标记"/>
	  
	  <form-property prefix="fatherRecord" name="service_targets_name" type="string" size="32" cannull="true" description="系统"/>
      <form-property prefix="fatherRecord" name="topics_name" type="string" size="32" cannull="true" description="主题"/>
	  <form-property prefix="fatherRecord" name="table_name_en" type="string" size="32" cannull="true" description="表名"/>
	  <form-property prefix="fatherRecord" name="table_name_cn" type="string" size="32" cannull="true" description="表中文名"/>
	
    </form-bean>

    <form-bean name="ResShareDataitemUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="share_dataitem_id" type="string" size="32" cannull="false" description="共享数据项ID"/>
      <form-property prefix="record" name="share_dataitem_id" type="string" size="32" cannull="false" description="共享数据项ID"/>
      <form-property prefix="record" name="share_table_id" type="string" size="32" cannull="true" description="共享表ID"/>
      <form-property prefix="record" name="dataitem_name_en" type="string" size="100" cannull="true" description="数据项名称"/>
      <form-property prefix="record" name="dataitem_name_cn" type="string" size="100" cannull="true" description="数据项中文名称"/>
      <form-property prefix="record" name="dataitem_type" type="string" size="20" cannull="true" description="数据项类型"/>
      <form-property prefix="record" name="dataitem_long" type="string" size="4" cannull="true" description="数据项长度"/>
      <form-property prefix="record" name="code_table_name" type="string" size="32" cannull="true" description="系统代码名"/>
      <form-property prefix="record" name="code_table" type="string" size="32" cannull="true" description="对应代码集"/>
      <form-property prefix="record" name="is_key" type="string" size="1" cannull="true" description="是否主键"/>
      <form-property prefix="record" name="dataitem_desc" type="string" size="2000" cannull="true" description="描述"/>
      <form-property prefix="record" name="show_order" type="long" cannull="true" description="显示顺序"/>
      <form-property prefix="record" name="is_markup" type="string" size="1" cannull="true" description="有效标记"/>
    </form-bean>

    <form-bean name="ResShareDataitemDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="share_dataitem_id" type="string" size="32" cannull="false" description="共享数据项ID"/>
      <form-property prefix="select-key" name="code_table" type="string" size="32" cannull="false" description="代码集代码"/>
	  
	  <form-property prefix="record" name="jcsjfx_dm" type="string" cannull="true" description="代码"/>
      <form-property prefix="record" name="jcsjfx_mc" type="string" cannull="true" description="代码值"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-res_share_dataitem.jsp" name="ResShareDataitemRowsetForm" txn-code="20301021" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享数据项信息列表</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareDataitem" func-name="txn20301021" input-node="select-key" output-node="record" description="查询共享数据项信息列表"/>
      <forward error-code="success" path="query-res_share_dataitem.jsp"/>
      <forward error-code="failure" path="query-res_share_dataitem.jsp"/>
    </action>

    <action input="update-res_share_dataitem.jsp" name="ResShareDataitemUpdateForm" txn-code="20301022" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改共享数据项信息信息</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareDataitem" func-name="txn20301022" input-node="record" description="修改共享数据项信息信息"/>
      <forward error-code="success" path="update-res_share_dataitem.jsp"/>
      <forward error-code="failure" path="update-res_share_dataitem.jsp"/>
    </action>

    <action input="insert-res_share_dataitem.jsp" name="ResShareDataitemUpdateForm" txn-code="20301023" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加共享数据项信息信息</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareDataitem" func-name="txn20301023" input-node="record" description="增加共享数据项信息信息"/>
      <forward error-code="success" path="insert-res_share_dataitem.jsp"/>
      <forward error-code="failure" path="insert-res_share_dataitem.jsp"/>
    </action>

    <action input="query-res_share_dataitem.jsp" name="ResShareDataitemUpdateForm" txn-code="20301024" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询共享数据项信息用于修改</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareDataitem" func-name="txn20301024" input-node="primary-key" output-node="record" description="查询共享数据项信息用于修改"/>
      <forward error-code="success" path="update-res_share_dataitem.jsp"/>
      <forward error-code="failure" path="update-res_share_dataitem.jsp"/>
    </action>

    <action input="query-res_share_dataitem.jsp" name="ResShareDataitemDeleteForm" txn-code="20301025" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除共享数据项信息信息</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareDataitem" func-name="txn20301025" input-node="primary-key" description="删除共享数据项信息信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
    
    <action input="query-res_share_dataitem.jsp" name="ResShareDataitemDeleteForm" txn-code="20301026" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询代码集列表</description>
      <step index="10" bean-name="com.gwssi.resource.share.txn.TxnResShareDataitem" func-name="txn20301026" input-node="select-key" output-node="record" description="查询共享数据项信息列表"/>
      <forward error-code="success" path="query_code_gather.jsp"/>
      <forward error-code="failure" path="query_code_gather.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-res_share_dataitem.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/resource/shareRes/query-res_share_dataitem.jsp" description="查询共享数据项信息列表"/>
    <resource-file type="jsp" file-name="/page/resource/shareRes/insert-res_share_dataitem.jsp" description="增加共享数据项信息信息"/>
    <resource-file type="jsp" file-name="/page/resource/shareRes/update-res_share_dataitem.jsp" description="修改共享数据项信息信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.ResShareDataitemContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.VoResShareDataitem" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.VoResShareDataitemSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.resource.share.vo.VoResShareDataitemPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

