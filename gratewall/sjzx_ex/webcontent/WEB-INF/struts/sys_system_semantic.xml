<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>业务系统定义</menu-name>
  <menu-path>/page/resource/res/metadata/datadict/ywxtdy/</menu-path>
  <description>业务系统定义</description>
  <form-beans>
    <form-bean name="SysSystemSemanticRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="sys_name" type="string" size="100" description="系统名称"/>
      <form-property prefix="select-key" name="sys_simple" type="string" size="40" description="系统简称"/>
      <form-property prefix="record" name="sys_id" type="string" size="32" cannull="false" description="系统ID"/>
      <form-property prefix="record" name="sys_no" type="string" size="10" cannull="false" description="系统编码"/>
      <form-property prefix="record" name="sys_name" type="string" size="100" cannull="false" description="系统名称"/>
      <form-property prefix="record" name="sys_simple" type="string" size="40" cannull="false" description="系统简称"/>
      <form-property prefix="record" name="sys_order" type="int" cannull="false" description="检索顺序"/>
    </form-bean>

    <form-bean name="SysSystemSemanticUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="sys_id" type="string" size="32" cannull="false" description="系统ID"/>
      <form-property prefix="record" name="sys_id" type="string" size="32" cannull="false" description="系统ID"/>
      <form-property prefix="record" name="sys_no" type="string" size="10" cannull="false" description="系统编码"/>
      <form-property prefix="record" name="sys_name" type="string" size="100" cannull="false" description="系统名称"/>
      <form-property prefix="record" name="sys_simple" type="string" size="40" cannull="false" description="系统简称"/>
      <form-property prefix="record" name="sys_order" type="int" cannull="false" description="检索顺序"/>
    </form-bean>

    <form-bean name="SysSystemSemanticDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="sys_id" type="string" size="32" cannull="false" description="系统ID"/>
      <form-property prefix="primary-key" name="sys_name" type="string" size="100" cannull="false" description="系统名称"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-sys_system_semantic.jsp" name="SysSystemSemanticRowsetForm" txn-code="30401001" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询主题列表</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysSystemSemantic" func-name="txn30401001" input-node="select-key" output-node="record" description="查询主题列表"/>
      <forward error-code="success" path="query-sys_system_semantic.jsp"/>
      <forward error-code="failure" path="query-sys_system_semantic.jsp"/>
    </action>

    <action input="update-sys_system_semantic.jsp" name="SysSystemSemanticUpdateForm" txn-code="30401002" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改系统信息</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysSystemSemantic" func-name="txn30401002" input-node="record" description="修改系统信息"/>
      <forward error-code="success" path="update-sys_system_semantic.jsp"/>
      <forward error-code="failure" path="update-sys_system_semantic.jsp"/>
    </action>

    <action input="insert-sys_system_semantic.jsp" name="SysSystemSemanticUpdateForm" txn-code="30401003" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加系统信息</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysSystemSemantic" func-name="txn30401003" input-node="record" description="增加系统信息"/>
      <forward error-code="success" path="insert-sys_system_semantic.jsp"/>
      <forward error-code="failure" path="insert-sys_system_semantic.jsp"/>
    </action>

    <action input="query-sys_system_semantic.jsp" name="SysSystemSemanticUpdateForm" txn-code="30401004" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询系统用于修改</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysSystemSemantic" func-name="txn30401004" input-node="primary-key" output-node="record" description="查询系统用于修改"/>
      <forward error-code="success" path="update-sys_system_semantic.jsp"/>
      <forward error-code="failure" path="update-sys_system_semantic.jsp"/>
    </action>

    <action input="query-sys_system_semantic.jsp" name="SysSystemSemanticDeleteForm" txn-code="30401005" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除系统信息</description>
      <step index="10" bean-name="com.gwssi.dw.metadata.datadict.txn.TxnSysSystemSemantic" func-name="txn30401005" input-node="primary-key" description="删除系统信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-sys_system_semantic.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/resource/res/metadata/datadict/ywxtdy/query-sys_system_semantic.jsp" description="查询主题列表"/>
    <resource-file type="jsp" file-name="/page/resource/res/metadata/datadict/ywxtdy/insert-sys_system_semantic.jsp" description="增加系统信息"/>
    <resource-file type="jsp" file-name="/page/resource/res/metadata/datadict/ywxtdy/update-sys_system_semantic.jsp" description="修改系统信息"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.SysSystemSemanticContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.VoSysSystemSemantic" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.VoSysSystemSemanticSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="com.gwssi.dw.metadata.datadict.vo.VoSysSystemSemanticPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

