<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config SYSTEM "gwssi-struts.dtd">
<struts-config>
  <menu-name>分类视图统计</menu-name>
  <menu-path>/page/log/</menu-path>
  <description>分类视图统计</description>
  <form-beans>
    <form-bean name="ViewSysCountLogRowsetForm">
      <description>多记录查询</description>
      <form-property prefix="select-key" name="sys_name" type="string" size="100" description="主题名称"/>
      <form-property prefix="select-key" name="table_name_cn" type="string" size="100" description="表中文名"/>
      <form-property prefix="select-key" name="count_date" type="string" size="10" description="统计日期"/>
      <form-property prefix="select-key" name="class_state" type="string" size="32" description="分类状态"/>
      <form-property prefix="record" name="sys_name" type="string" size="100" cannull="true" description="主题名称"/>
      <form-property prefix="record" name="table_name" type="string" size="100" cannull="true" description="表名"/>
      <form-property prefix="record" name="table_name_cn" type="string" size="100" cannull="true" description="表中文名"/>
      <form-property prefix="record" name="class_sort" type="string" size="32" cannull="true" description="分类排序"/>
      <form-property prefix="record" name="class_state" type="string" size="32" cannull="true" description="分类状态"/>
      <form-property prefix="record" name="count_date" type="string" size="10" cannull="true" description="统计日期"/>
      <form-property prefix="record" name="count_full" type="long" cannull="true" description="日全量"/>
      <form-property prefix="record" name="count_incre" type="long" cannull="true" description="日增量"/>
      <form-property prefix="record" name="table_class_id" type="string" size="40" cannull="true" description="分类主键id"/>
      <form-property prefix="record" name="sys_order" type="long" cannull="true" description="主体排序字段"/>
      <form-property prefix="record" name="table_order" type="long" cannull="true" description="表排序字段"/>
      <form-property prefix="record" name="sort_order" type="long" cannull="true" description="分类排序"/>
      <form-property prefix="record" name="state_order" type="long" cannull="true" description="状态排序"/>
    </form-bean>

    <form-bean name="ViewSysCountLogUpdateForm">
      <description>单记录处理：查询、修改、增加</description>
      <form-property prefix="primary-key" name="table_class_id" type="string" size="40" cannull="true" description="分类主键id"/>
      <form-property prefix="record" name="sys_name" type="string" size="100" cannull="true" description="主题名称"/>
      <form-property prefix="record" name="table_name" type="string" size="100" cannull="true" description="表名"/>
      <form-property prefix="record" name="table_name_cn" type="string" size="100" cannull="true" description="表中文名"/>
      <form-property prefix="record" name="class_sort" type="string" size="32" cannull="true" description="分类排序"/>
      <form-property prefix="record" name="class_state" type="string" size="32" cannull="true" description="分类状态"/>
      <form-property prefix="record" name="count_date" type="string" size="10" cannull="true" description="统计日期"/>
      <form-property prefix="record" name="count_full" type="long" cannull="true" description="日全量"/>
      <form-property prefix="record" name="count_incre" type="long" cannull="true" description="日增量"/>
      <form-property prefix="record" name="table_class_id" type="string" size="40" cannull="true" description="分类主键id"/>
      <form-property prefix="record" name="sys_order" type="long" cannull="true" description="主体排序字段"/>
      <form-property prefix="record" name="table_order" type="long" cannull="true" description="表排序字段"/>
      <form-property prefix="record" name="sort_order" type="long" cannull="true" description="分类排序"/>
      <form-property prefix="record" name="state_order" type="long" cannull="true" description="状态排序"/>
    </form-bean>

    <form-bean name="ViewSysCountLogDeleteForm">
      <description>多记录和单记录删除</description>
      <form-property prefix="primary-key" name="table_class_id" type="string" size="40" cannull="true" description="分类主键id"/>
    </form-bean>
  </form-beans>

  <action-mappings>
    <action input="query-view_sys_count_log.jsp" name="ViewSysCountLogRowsetForm" txn-code="620100301" check-type="public" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询数据增长列表</description>
      <step index="10" bean-name="cn.gwssi.dw.rd.log.txn.TxnViewSysCountLog" func-name="txn620100301" input-node="select-key" output-node="record" description="查询数据增长列表"/>
      <forward error-code="success" path="query-view_sys_count_log.jsp"/>
      <forward error-code="failure" path="query-view_sys_count_log.jsp"/>
    </action>

    <action input="update-view_sys_count_log.jsp" name="ViewSysCountLogUpdateForm" txn-code="620100302" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>修改数据增长信息</description>
      <step index="10" bean-name="cn.gwssi.dw.rd.log.txn.TxnViewSysCountLog" func-name="txn620100302" input-node="record" description="修改数据增长信息"/>
      <forward error-code="success" path="update-view_sys_count_log.jsp"/>
      <forward error-code="failure" path="update-view_sys_count_log.jsp"/>
    </action>

    <action input="insert-view_sys_count_log.jsp" name="ViewSysCountLogUpdateForm" txn-code="620100303" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>增加数据增长信息</description>
      <step index="10" bean-name="cn.gwssi.dw.rd.log.txn.TxnViewSysCountLog" func-name="txn620100303" input-node="record" description="增加数据增长信息"/>
      <forward error-code="success" path="insert-view_sys_count_log.jsp"/>
      <forward error-code="failure" path="insert-view_sys_count_log.jsp"/>
    </action>

    <action input="query-view_sys_count_log.jsp" name="ViewSysCountLogUpdateForm" txn-code="620100304" check-type="check" container="default" txn-type="transaction" re-entry="true" log-flag="true">
      <description>查询数据增长用于修改</description>
      <step index="10" bean-name="cn.gwssi.dw.rd.log.txn.TxnViewSysCountLog" func-name="txn620100304" input-node="primary-key" output-node="record" description="查询数据增长用于修改"/>
      <forward error-code="success" path="update-view_sys_count_log.jsp"/>
      <forward error-code="failure" path="update-view_sys_count_log.jsp"/>
    </action>

    <action input="query-view_sys_count_log.jsp" name="ViewSysCountLogDeleteForm" txn-code="620100305" check-type="check" container="default" txn-type="transaction" re-entry="false" log-flag="true">
      <description>删除数据增长信息</description>
      <step index="10" bean-name="cn.gwssi.dw.rd.log.txn.TxnViewSysCountLog" func-name="txn620100305" input-node="primary-key" description="删除数据增长信息"/>
      <forward error-code="success" path="/script/deletesuccess.jsp"/>
      <forward error-code="failure" path="/script/deletefailure.jsp"/>
    </action>
  </action-mappings>

  <resource>
    <resource-file type="model" file-name="/WEB-INF/resource/jsp-frame/menu-view_sys_count_log.xml" description="模板文件"/>
    <resource-file type="jsp" file-name="/page/log/query-view_sys_count_log.jsp" description="查询数据增长列表"/>
    <resource-file type="jsp" file-name="/page/log/insert-view_sys_count_log.jsp" description="增加数据增长信息"/>
    <resource-file type="jsp" file-name="/page/log/update-view_sys_count_log.jsp" description="修改数据增长信息"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.ViewSysCountLogContext" description="应用对应的数据上下文对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.VoViewSysCountLog" description="数据表字段对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.VoViewSysCountLogSelectKey" description="查询条件对应的VO对象"/>
    <resource-file type="vo-bean" file-name="cn.gwssi.dw.rd.log.vo.VoViewSysCountLogPrimaryKey" description="数据库主键对应的VO对象"/>
  </resource>
</struts-config>

