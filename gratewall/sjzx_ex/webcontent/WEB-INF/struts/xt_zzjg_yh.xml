        <button button-name="record_goBack" function-name="goBack" caption="返 回" next-page="/txn807001.do" hint="返回到用户明细窗口" property="hotkey=CLOSE"/>
        <field field-name="yhid_pk" field-type="hidden" caption="用户ID" property="style=width:90%,datatype=string,maxlength=32,minlength=1"/>
        <field field-name="jgid_fk" field-type="browsebox" caption="所属机构" value-set="orglist" property="style=width:90%,datatype=string,maxlength=32,show=name,data=value"/>
        <field field-name="yhzh" field-type="text" caption="用户帐号" property="style=width:90%,datatype=string,maxlength=20,minlength=1"/>
        <field field-name="yhmm" field-type="password" caption="用户密码" property="style=width:90%,datatype=string,maxlength=32"/>
        <field field-name="yhxm" field-type="text" caption="用户姓名" property="style=width:90%,datatype=string,maxlength=20,minlength=1"/>
        <field field-name="sfyx" field-type="select" caption="是否有效" value-set="orgstatus" property="style=width:90%,datatype=string,maxlength=2,minlength=1"/>
        <field field-name="plxh" field-type="text" caption="排列序号" property="style=width:90%,datatype=string,maxlength=10"/>
        <field field-name="sfz" field-type="text" caption="身份证号" property="style=width:90%,datatype=string,maxlength=20"/>
        <field field-name="zw" field-type="text" caption="职位" property="style=width:90%,datatype=string,maxlength=64"/>
        <field field-name="zyzz" field-type="textarea" caption="主要职责" property="style=width:90%,datatype=string,maxlength=255,colspan=2,rows=2"/>
        <field field-name="gzdh" field-type="text" caption="工作电话" property="style=width:90%,datatype=string,maxlength=20"/>
