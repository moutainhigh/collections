        <field field-name="yhid_pk" field-type="hidden" caption="用户ID" property="style=width:10%,datatype=string,maxlength=32"/>
        <field field-name="jgid_fk" field-type="cell" caption="所属机构" value-set="orglist" property="style=width:15%,show=name"/>
        <field field-name="yhzh" field-type="cell" caption="用户帐号" property="style=width:15%,datatype=string,maxlength=20"/>
        <field field-name="yhxm" field-type="cell" caption="用户姓名" property="style=width:15%,datatype=string,maxlength=20"/>
        <field field-name="sfyx" field-type="cell" caption="是否有效" property="style=width:10%,datatype=string,maxlength=2"/>
        <field field-name="zw" field-type="cell" caption="职位" property="style=width:15%,datatype=string,maxlength=64"/>
        <field field-name="roleids" field-type="cell" caption="角色ID列表" property="style=width:30%,datatype=string,maxlength=2000"/>
      </frame>

    </jsp-page>

