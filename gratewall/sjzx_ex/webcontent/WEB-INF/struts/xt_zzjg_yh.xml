        <field field-name="yhxm" field-type="text" caption="用户姓名" property="style=width:90%,datatype=string,maxlength=20"/>
        <field field-name="jgid_fk" field-type="browsebox" caption="所属机构" value-set="orglist" property="style=width:90%,datatype=string,maxlength=32,show=name,data=value"/>
        <field field-name="roleids" field-type="browsebox" caption="所属角色" value-set="用户角色信息" property="style=width:90%,show=name,data=value"/>
      </frame>

      <frame frame-name="record" frame-type="grid" caption="用户管理列表" keylist="record:yhid_pk" width="95%" property="navbar=bottom,fixrow=true">
        <button button-name="record_addRecord" function-name="addRecord" caption="增加用户" next-page="insert-yhgl.jsp" hint="增加用户" txn-code="807003" property="enablerule=0,hotkey=ADD"/>
