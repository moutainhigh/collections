<chart caption='(${query_date})异常响应的原因分布'  showBorder='0' palette='2' animation='1' formatNumberScale='0'  showPercentageInLabel='0' showValues='1' showLegend='1'  pieSliceDepth='30' baseFontSize='12'
	startingAngle='125'>
	<#list list as t>
	 <set label='${t.error_name}' value='${t.count}' isSliced='0'/>
	</#list>
	<styles>
		<definition>
			<style type='font' name='CaptionFont' size='15' color='666666' />
			<style type='font' name='SubCaptionFont' bold='0' />
		</definition>
		<application>
			<apply toObject='caption' styles='CaptionFont' />
			<apply toObject='SubCaption' styles='SubCaptionFont' />
		</application>
	</styles>
</chart>