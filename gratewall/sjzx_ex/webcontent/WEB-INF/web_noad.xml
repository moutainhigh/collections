		<servlet-class>com.gwssi.dw.aic.bj.SearchServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>DisplayChart</servlet-name>
		<servlet-class>
			org.jfree.chart.servlet.DisplayChart
		</servlet-class>
	</servlet>
	<!-- WLS8RpcServlet -->
	<servlet>
		<servlet-name>WLS8RpcServlet</servlet-name>
		<servlet-class>
			cn.gwssi.monitor.collect.servlet.WLS8RpcServlet
