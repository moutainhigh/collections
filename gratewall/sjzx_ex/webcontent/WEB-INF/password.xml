<?xml version="1.0"?>
<CheckRule enable="true">
	<pwLength enable="true">
		<description>密码长度校验</description>
		<minLength>6</minLength>
	</pwLength>
	<complication enable="true">
		<description>密码复杂度要求</description>
		<lowercase>false</lowercase>
		<uppercase>false</uppercase>
		<ArabicNumerals>false</ArabicNumerals>
		<specialCharacter>false</specialCharacter>
	</complication>
	<reservation enable="true">
		<description>密码留存期</description>
		<days>30</days>
	</reservation>
	<validatecode  enable="false">
		<description>是否使用验证码</description>
	</validatecode>
</CheckRule>
