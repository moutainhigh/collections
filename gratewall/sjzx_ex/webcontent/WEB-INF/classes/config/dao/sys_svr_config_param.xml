  <column name="right_paren" data-type="varchar" length="5" not-null="false" value-from="input" description="右括号"/>
  <column name="param_type" data-type="varchar" length="32" not-null="true" value-from="input" description="参数类型：0 系统参数；1 用户参数"/>
  <column name="param_order" data-type="long" length="0" not-null="true" value-from="input" description="排序字段"/>
  <column name="param_text" data-type="varchar" length="4000" not-null="true" value-from="input" description="参数中文"/>
  <primary-key name="pk_sys_svr_config_param" column-list="sys_svr_config_param_id"/>
  <index name="pk_sys_svr_config_param" index-type="unique" cluster="cluster" column-list="sys_svr_config_param_id"/>
  <function func-name="select one sys_svr_config_param" func-type="select" column-list="sys_svr_config_param_id,sys_svr_config_id,operator1,left_paren,left_table_no,left_table_name,left_table_name_cn,left_column_no,left_column_name,left_column_name_cn,operator2,param_value,right_paren,param_type,param_text">
