  <function func-name="select sys_svr_config_param list" func-type="rowset" column-list="sys_svr_config_param_id,sys_svr_config_id,operator1,left_paren,left_table_no,left_table_name,left_table_name_cn,left_column_no,left_column_name,left_column_name_cn,operator2,param_value,right_paren,param_type,param_text" sort-columns="param_order" max-record="10">
