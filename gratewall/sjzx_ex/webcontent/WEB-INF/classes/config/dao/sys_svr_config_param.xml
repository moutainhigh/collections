  <function func-name="insert one sys_svr_config_param" func-type="insert" column-list="sys_svr_config_param_id,sys_svr_config_id,operator1,left_paren,left_table_no,left_table_name, left_table_name_cn, left_column_no,left_column_name,left_column_name_cn, operator2,param_value,right_paren,param_type,param_order,param_text">
    <description>插入记录</description>
  </function>

  <function func-name="delete one sys_svr_config_param" func-type="delete">
    <filter-column column-name="sys_svr_config_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>
</table>

