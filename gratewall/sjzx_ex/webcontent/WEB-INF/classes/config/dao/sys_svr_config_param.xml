<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>共享服务</sort-name>
  <table-info name="sys_svr_config_param" class-name="com.gwssi.dw.runmgr.services.dao.SysSvrConfigParam" check-flag="false" backup-flag="false">
    <description>服务配置参数表</description>
  </table-info>

  <column name="sys_svr_config_param_id" data-type="varchar" length="32" not-null="true" value-from="input" description="共享服务配置参数编号"/>
  <column name="sys_svr_config_id" data-type="varchar" length="32" not-null="true" value-from="input" description="共享服务配置编号"/>
  <column name="operator1" data-type="varchar" length="10" not-null="false" value-from="input" description="连接符1"/>
  <column name="left_paren" data-type="varchar" length="5" not-null="false" value-from="input" description="左括号"/>
  <column name="left_table_no" data-type="varchar" length="32" not-null="true" value-from="input" description="左表"/>
  <column name="left_table_name" data-type="varchar" length="32" not-null="true" value-from="input" description="左表英文名"/>
  <column name="left_table_name_cn" data-type="varchar" length="32" not-null="true" value-from="input" description="左表中文名"/>
  <column name="left_column_no" data-type="varchar" length="32" not-null="true" value-from="input" description="左字段"/>
  <column name="left_column_name" data-type="varchar" length="32" not-null="true" value-from="input" description="左字段英文名"/>
  <column name="left_column_name_cn" data-type="varchar" length="32" not-null="true" value-from="input" description="左字段中文名"/>
