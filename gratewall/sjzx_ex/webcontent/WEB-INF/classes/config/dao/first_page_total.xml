<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>首页</sort-name>
  <table-info name="first_page_total_new" class-name="com.gwssi.dw.runmgr.etl.dao.FirstPageTotal" check-flag="false" backup-flag="false">
    <description>首页数据量统计</description>
  </table-info>

  <column name="first_page_total_id" data-type="varchar" length="32" not-null="true" value-from="input" description="id"/>
  <column name="first_page_total_cls" data-type="varchar" length="50" not-null="false" value-from="input" description="cls"/>
  <column name="first_page_total_num" data-type="decimal" length="30" fraction-length="6" not-null="false" value-from="input" description="num"/>
  <primary-key name="pk_first_page_total" column-list="first_page_total_id"/>
  <index name="pk_first_page_total" index-type="unique" cluster="cluster" column-list="first_page_total_id"/>
</table>

