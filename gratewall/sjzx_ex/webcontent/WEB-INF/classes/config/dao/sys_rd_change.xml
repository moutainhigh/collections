<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>资源管理</sort-name>
  <table-info name="sys_rd_change" class-name="cn.gwssi.dw.rd.metadata.dao.SysRdChange" check-flag="false" backup-flag="false">
    <description>变更记录表</description>
  </table-info>

  <column name="sys_rd_change_id" data-type="varchar" length="32" not-null="true" value-from="generate" func-name="getUUID" description="表主键"/>
  <column name="sys_rd_data_source_id" data-type="varchar" length="32" not-null="false" value-from="input" description="数据源ID"/>
  <column name="db_name" data-type="varchar" length="100" not-null="false" value-from="input" description="数据源名称"/>
  <column name="db_username" data-type="varchar" length="100" not-null="false" value-from="input" description="用户名称"/>
  <column name="table_name" data-type="varchar" length="100" not-null="false" value-from="input" description="物理表"/>
  <column name="table_name_cn" data-type="varchar" length="100" not-null="false" value-from="input" description="物理表中文名"/>
  <column name="column_name" data-type="varchar" length="100" not-null="false" value-from="input" description="字段"/>
  <column name="column_name_cn" data-type="varchar" length="100" not-null="false" value-from="input" description="字段中文名"/>
  <column name="change_item" data-type="varchar" length="1" not-null="false" value-from="input" description="变更类型"/>
  <column name="change_before" data-type="varchar" length="100" not-null="false" value-from="input" description="变更前内容"/>
  <column name="change_after" data-type="varchar" length="100" not-null="false" value-from="input" description="变更后内容"/>
  <column name="change_result" data-type="varchar" length="1" not-null="false" value-from="input" description="处理结果"/>
  <column name="change_oprater" data-type="varchar" length="100" not-null="false" value-from="input" description="变更人"/>
  <column name="change_time" data-type="varchar" length="32" not-null="false" value-from="input" description="变更时间"/>
  <column name="change_reason" data-type="varchar" length="1000" not-null="false" value-from="input" description="变更原因"/>
  <column name="timestamp" data-type="varchar" length="32" not-null="false" value-from="input" description="时间戳"/>
  
  
  <primary-key name="pk_sys_rd_change" column-list="sys_rd_change_id"/>
  <index name="pk_sys_rd_change" index-type="unique" cluster="cluster" column-list="sys_rd_change_id"/>
  <function func-name="select one sys_rd_change" func-type="select" column-list="sys_rd_change_id,sys_rd_data_source_id,db_name,db_username,table_name,table_name_cn,column_name,column_name_cn,change_item,change_before,change_after,change_result,change_oprater,change_time,change_reason,timestamp">
    <filter-column column-name="sys_rd_change_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="select sys_rd_change list" func-type="rowset" column-list="sys_rd_change_id,sys_rd_data_source_id,db_name,db_username,table_name,table_name_cn,column_name,column_name_cn,change_item,change_before,change_after,change_result,change_oprater,change_time,change_reason,timestamp" max-record="10">
    <filter-column column-name="sys_rd_change_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="table_name" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="column_name" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="change_item" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="change_result" operator="eq" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one sys_rd_change" func-type="insert" column-list="sys_rd_change_id,sys_rd_data_source_id,db_name,db_username,table_name,table_name_cn,column_name,column_name_cn,change_item,change_before,change_after,change_result,change_oprater,change_time,change_reason,timestamp">
    <description>插入记录</description>
  </function>

  <function func-name="delete one sys_rd_change" func-type="delete">
    <description>删除记录</description>
  </function>

  <function func-name="update one sys_rd_change" func-type="update" column-list="sys_rd_data_source_id,db_name,db_username,table_name,table_name_cn,column_name,column_name_cn,change_item,change_before,change_after,change_result,change_oprater,change_time,change_reason,timestamp">
    <filter-column column-name="sys_rd_change_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>
</table>

