<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>采集服务</sort-name>
  <table-info name="collect_task" class-name="com.gwssi.collect.webservice.dao.CollectTask" check-flag="false" backup-flag="false">
    <description>采集任务表</description>
  </table-info>

  <column name="collect_task_id" data-type="varchar" length="32" not-null="true" value-from="input" description="采集任务ID"/>
  <column name="service_targets_id" data-type="varchar" length="32" not-null="false" value-from="input" description="所属服务对象"/>
  <column name="data_source_id" data-type="varchar" length="32" not-null="false" value-from="input" description="数据源"/>
  <column name="task_name" data-type="varchar" length="100" not-null="false" value-from="input" description="任务名称"/>
  <column name="collect_type" data-type="varchar" length="20" not-null="false" value-from="input" description="采集类型"/>
  <column name="task_description" data-type="varchar" length="2000" not-null="false" value-from="input" description="任务描述"/>
  <column name="record" data-type="varchar" length="2000" not-null="false" value-from="input" description="备案说明"/>
  <column name="task_status" data-type="varchar" length="20" not-null="false" value-from="input" description="任务状态"/>
  <column name="is_markup" data-type="varchar" length="1" not-null="false" value-from="input" description="有效标记"/>
  <column name="creator_id" data-type="varchar" length="32" not-null="false" value-from="input" description="创建人ID"/>
  <column name="created_time" data-type="varchar" length="19" not-null="false" value-from="input" description="创建时间"/>
  <column name="last_modify_id" data-type="varchar" length="32" not-null="false" value-from="input" description="最后修改人ID"/>
  <column name="last_modify_time" data-type="varchar" length="19" not-null="false" value-from="input" description="最后修改时间"/>
  <column name="fj_fk" data-type="varchar" length="2000" not-null="false" value-from="input" description="附件id"/>
  <column name="fjmc" data-type="varchar" length="2000" not-null="false" value-from="input" description="附件名称"/>
  <column name="log_file_path" data-type="varchar" length="1000" not-null="false" value-from="input" description="日志文件路径信息"/>
  <column name="collect_status" data-type="varchar" length="2" not-null="false" value-from="input" description="采集状态"/>
  <primary-key name="pk_collect_task" column-list="collect_task_id"/>
  <index name="pk_collect_task" index-type="unique" cluster="cluster" column-list="collect_task_id"/>
  <function func-name="select one collect_task" func-type="select" column-list="collect_task_id,service_targets_id,data_source_id,task_name,collect_type,task_description,record,task_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time,fj_fk,fjmc">
   <filter-column column-name="collect_task_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="select collect_task list" func-type="rowset" column-list="collect_task_id,service_targets_id,data_source_id,task_name,collect_type,task_description,record,task_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time" sort-columns="created_time" max-record="10">
    <filter-column column-name="service_targets_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="task_name" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="collect_type" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="collect_status" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="task_status" operator="eq" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one collect_task" func-type="insert" column-list="collect_task_id,service_targets_id,data_source_id,task_name,collect_type,task_description,record,task_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time,fj_fk,fjmc,collect_status">
    <description>插入记录</description>
  </function>
  
  <function func-name="insert one collect_task for fileupload" func-type="insert" column-list="collect_task_id,service_targets_id,data_source_id,task_name,collect_type,task_description,record,task_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time,fj_fk,fjmc,log_file_path,collect_status">
    <description>插入记录</description>
  </function>

  <function func-name="delete one collect_task" func-type="delete">
    <description>删除记录</description>
  </function>

  <function func-name="update one collect_task" func-type="update" column-list="service_targets_id,data_source_id,task_name,collect_type,task_description,record,task_status,is_markup,last_modify_id,last_modify_time,fj_fk,fjmc,log_file_path,collect_status">
    <description>修改记录</description>
  </function>
  
  <function func-name="update one collect_task For Database" func-type="update" column-list="service_targets_id,data_source_id,task_name,collect_type,task_description,task_status,is_markup,fj_fk,fjmc">
    <description>修改记录</description>
  </function>
  
  <function func-name="update one collect_task for fileupload" func-type="update" column-list="service_targets_id,task_name,collect_type,task_description,record,task_status,last_modify_id,last_modify_time,fj_fk,fjmc,log_file_path,collect_status">
    <filter-column column-name="collect_task_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>文件上传修改记录</description>
  </function>
</table>

