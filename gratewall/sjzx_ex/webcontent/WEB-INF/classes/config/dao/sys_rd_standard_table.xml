  </function>

  <function func-name="select sys_rd_standard_table list" func-type="rowset" column-list="sys_rd_standard_table_id,sys_rd_standard_id,standard_name,table_name,table_belongs,memo,sort,timestamp" max-record="10">
    <filter-column column-name="sys_rd_standard_id" operator="eq" value-from="input" empty-flag="skip"/>
