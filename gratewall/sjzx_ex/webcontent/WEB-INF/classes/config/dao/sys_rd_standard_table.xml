  <primary-key name="pk_jc_standard_table" column-list="sys_rd_standard_table_id"/>
  <index name="pk_jc_standard_table" index-type="unique" cluster="cluster" column-list="sys_rd_standard_table_id"/>
  <function func-name="select one sys_rd_standard_table" func-type="select" column-list="sys_rd_standard_table_id,sys_rd_standard_id,standard_name,table_name,table_belongs,memo,sort,timestamp">
    <filter-column column-name="sys_rd_standard_table_id" operator="eq" value-from="input" empty-flag="skip"/>
