    <filter-column column-name="table_name" operator="like" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one sys_rd_standard_table" func-type="insert" column-list="sys_rd_standard_table_id,sys_rd_standard_id,standard_name,table_name,table_belongs,memo,sort,timestamp">
    <description>插入记录</description>
  </function>

  <function func-name="delete one sys_rd_standard_table" func-type="delete">
    <filter-column column-name="sys_rd_standard_table_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>

  <function func-name="update one sys_rd_standard_table" func-type="update" column-list="sys_rd_standard_table_id,sys_rd_standard_id,standard_name,table_name,table_belongs,memo,sort,timestamp">
    <filter-column column-name="sys_rd_standard_table_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>
