  <column name="table_name" data-type="varchar" length="100" not-null="false" value-from="input" description="实体信息名称"/>
  <column name="table_belongs" data-type="varchar" length="100" not-null="false" value-from="input" description="所属体系"/>
  <column name="memo" data-type="varchar" length="2000" not-null="false" value-from="input" description="备注"/>
