<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>资源管理</sort-name>
  <table-info name="res_service_targets" class-name="com.gwssi.resource.svrobj.dao.ResServiceTargets" check-flag="false" backup-flag="false">
    <description>服务对象表</description>
  </table-info>

  <column name="service_targets_id" data-type="varchar" length="32" not-null="true" value-from="generate" func-name="getUUID" description="服务对象ID"/>
  <column name="service_targets_no" data-type="varchar" length="50" not-null="false" value-from="input" description="服务对象代码"/>
  <column name="service_targets_name" data-type="varchar" length="100" not-null="false" value-from="input" description="服务对象名称"/>
  <column name="service_targets_type" data-type="varchar" length="20" not-null="false" value-from="input" description="服务对象类型"/>
  <column name="is_bind_ip" data-type="varchar" length="1" not-null="false" value-from="input" description="是否绑定IP"/>
  <column name="ip" data-type="varchar" length="200" not-null="false" value-from="input" description="IP"/>
  <column name="service_password" data-type="varchar" length="50" not-null="false" value-from="input" description="服务口令"/>
  <column name="service_status" data-type="varchar" length="20" not-null="false" value-from="input" description="服务状态"/>
  <column name="service_desc" data-type="varchar" length="2000" not-null="false" value-from="input" description="服务对象描述"/>
  <column name="is_markup" data-type="varchar" length="1" not-null="false" value-from="input" description="有效标记"/>
  <column name="fj_fk" data-type="varchar" length="2000" not-null="false" value-from="input" description="附件id"/>
  <column name="fjmc" data-type="varchar" length="4000" not-null="false" value-from="input" description="附件名称"/>
  <column name="creator_id" data-type="varchar" length="32" not-null="false" value-from="input" description="创建人ID"/>
  <column name="created_time" data-type="time" length="11" fraction-length="6" not-null="false" value-from="input" description="创建时间"/>
  <column name="last_modify_id" data-type="varchar" length="32" not-null="false" value-from="input" description="最后修改人ID"/>
  <column name="last_modify_time" data-type="time" length="11" fraction-length="6" not-null="false" value-from="input" description="最后修改时间"/>
  <column name="show_order" data-type="integer" length="2" not-null="true" unique="true" value-from="input" description="显示顺序"/>
  <primary-key name="pk_res_service_targets" column-list="service_targets_id"/>
  <index name="pk_res_service_targets" index-type="unique" cluster="cluster" column-list="service_targets_id"/>
  <function func-name="select res_service_targets list" func-type="rowset" column-list="service_targets_id,service_targets_no,service_targets_name,service_targets_type,is_bind_ip,ip,service_password,service_status,service_desc,fj_fk,fjmc,is_markup,creator_id,created_time,last_modify_id,last_modify_time" sort-columns="last_modify_time" max-record="10">
    <filter-column column-name="service_targets_name" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="service_targets_type" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="service_status" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="created_time" operator="between" value-from="input" value-item="created_time_start" value-item2="created_time_end" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="select one res_service_targets" func-type="select" column-list="service_targets_id,service_targets_no,service_targets_name,service_targets_type,is_bind_ip,ip,service_password,service_status,service_desc,fj_fk,fjmc,is_markup,creator_id,created_time,last_modify_id,last_modify_time,show_order" sort-columns="last_modify_time">
    <description>单记录查询</description>
  </function>

  <function func-name="insert one res_service_targets" func-type="insert" column-list="service_targets_id,service_targets_no,service_targets_name,service_targets_type,is_bind_ip,ip,service_password,service_status,service_desc,fj_fk,fjmc,is_markup,creator_id,created_time,last_modify_id,last_modify_time,show_order">
    <description>插入记录</description>
  </function>

  <function func-name="update one res_service_targets" func-type="update" column-list="service_targets_no,service_targets_name,service_targets_type,is_bind_ip,ip,service_password,service_status,service_desc,is_markup,last_modify_id,fj_fk,fjmc,last_modify_time,show_order">
    <description>修改记录</description>
  </function>

  <function func-name="delete one res_service_targets" func-type="delete">
    <description>删除记录</description>
  </function>
</table>

