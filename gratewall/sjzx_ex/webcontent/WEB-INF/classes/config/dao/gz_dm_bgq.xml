<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>csdb</sort-name>
  <table-info name="gz_dm_bgq" class-name="cn.gwssi.csdb.ysjgl.dao.GzDmBgq" check-flag="false" backup-flag="false">
    <description>报告期代码表</description>
  </table-info>

  <column name="bgq_dm" data-type="varchar" length="10" not-null="true" value-from="input" description="报告期代码"/>
  <column name="dclb_dm" data-type="char" length="1" not-null="false" value-from="input" description="调查类别代码"/>
  <column name="bgqb_dm" data-type="varchar" length="10" not-null="false" value-from="input" description="报告期别代码"/>
  <column name="bgq_cn_mc" data-type="varchar" length="32" not-null="false" value-from="input" description="报告期中文名称"/>
  <column name="bgq_en_mc" data-type="varchar" length="32" not-null="false" value-from="input" description="报告期英文名称"/>
  <column name="bgq_cn_ms" data-type="varchar" length="128" not-null="false" value-from="input" description="报告期中文描述"/>
  <column name="bgq_en_ms" data-type="varchar" length="128" not-null="false" value-from="input" description="报告期英文描述"/>
  <column name="sj_lx" data-type="char" length="1" not-null="false" value-from="input" description="时间类型"/>
  <column name="qs_sj" data-type="char" length="19" not-null="false" value-from="input" description="起始时间"/>
  <column name="js_sj" data-type="char" length="19" not-null="false" value-from="input" description="结束时间"/>
  <column name="nf" data-type="varchar" length="4" not-null="false" value-from="input" description="年份"/>
  <primary-key name="p_bgq_dm" column-list="bgq_dm"/>
  <index name="p_bgq_dm" index-type="unique" cluster="other" column-list="bgq_dm"/>
  <function func-name="select one gz_dm_bgq" func-type="select" column-list="bgq_dm,dclb_dm,bgqb_dm,bgq_cn_mc,bgq_en_mc,bgq_cn_ms,bgq_en_ms,sj_lx,qs_sj,js_sj">
    <filter-column column-name="bgq_dm" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="select gz_dm_bgq list" func-type="rowset" column-list="bgq_dm,dclb_dm,bgqb_dm,bgq_cn_mc,bgq_en_mc,bgq_cn_ms,bgq_en_ms,sj_lx,qs_sj,js_sj" max-record="10">
    <filter-column column-name="dclb_dm" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="bgqb_dm" operator="eq" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one gz_dm_bgq" func-type="insert" column-list="bgq_dm,dclb_dm,bgqb_dm,bgq_cn_mc,bgq_en_mc,bgq_cn_ms,bgq_en_ms,sj_lx,qs_sj,js_sj">
    <description>插入记录</description>
  </function>

  <function func-name="delete one gz_dm_bgq" func-type="delete">
    <filter-column column-name="bgq_dm" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>

  <function func-name="update one gz_dm_bgq" func-type="update" column-list="bgq_dm,dclb_dm,bgqb_dm,bgq_cn_mc,bgq_en_mc,bgq_cn_ms,bgq_en_ms,sj_lx,qs_sj,js_sj">
    <filter-column column-name="bgq_dm" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>

  <function func-name="报告期代码对照表" func-type="rowset" column-list="bgq_dm as codevalue, bgq_cn_mc as codename" max-record="-1">
    <description>多记录查询</description>
  </function>
</table>

