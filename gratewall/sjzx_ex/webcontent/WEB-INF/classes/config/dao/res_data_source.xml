<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>资源管理</sort-name>
  <table-info name="res_data_source" class-name="com.gwssi.resource.svrobj.dao.ResDataSource" check-flag="false" backup-flag="false">
    <description>数据源表</description>
  </table-info>

  <column name="data_source_id" data-type="varchar" length="32" not-null="true" value-from="input" description="数据源ID"/>
  <column name="service_targets_id" data-type="varchar" length="32" not-null="false" value-from="input" description="服务对象ID"/>
  <column name="data_source_type" data-type="varchar" length="20" not-null="false" value-from="input" description="数据源类型"/>
  <column name="data_source_name" data-type="varchar" length="100" not-null="false" value-from="input" description="数据源名称"/>
  <column name="data_source_ip" data-type="varchar" length="50" not-null="false" value-from="input" description="数据源IP"/>
  <column name="access_port" data-type="varchar" length="8" not-null="false" value-from="input" description="访问端口"/>
  <column name="access_url" data-type="varchar" length="255" not-null="false" value-from="input" description="访问URL"/>
  <column name="db_type" data-type="varchar" length="20" not-null="false" value-from="input" description="数据库类型"/>
  <column name="db_instance" data-type="varchar" length="30" not-null="false" value-from="input" description="数据源实例"/>
  <column name="db_username" data-type="varchar" length="50" not-null="false" value-from="input" description="数据源用户名"/>
  <column name="db_password" data-type="varchar" length="50" not-null="false" value-from="input" description="数据源密码"/>
  <column name="db_desc" data-type="varchar" length="2000" not-null="false" value-from="input" description="数据源描述"/>
  <column name="db_status" data-type="varchar" length="20" not-null="false" value-from="input" description="数据源状态"/>
  <column name="is_markup" data-type="varchar" length="1" not-null="false" value-from="input" description="有效标记"/>
  <column name="creator_id" data-type="varchar" length="32" not-null="false" value-from="input" description="创建人ID"/>
  <column name="created_time" data-type="varchar" length="19" not-null="false" value-from="input" description="创建时间"/>
  <column name="last_modify_id" data-type="varchar" length="32" not-null="false" value-from="input" description="最后修改人ID"/>
  <column name="last_modify_time" data-type="varchar" length="19" not-null="false" value-from="input" description="最后修改时间"/>
  <column name="req_que_name" data-type="varchar" length="100" not-null="false" value-from="input" description="请求队列名称"/>
  <column name="res_que_name" data-type="varchar" length="100" not-null="false" value-from="input" description="接收队列名称"/>
  <primary-key name="pk_res_data_source" column-list="data_source_id"/>
  <index name="pk_res_data_source" index-type="unique" cluster="cluster" column-list="data_source_id"/>
  <function func-name="select one res_data_source" func-type="select" column-list="data_source_id,service_targets_id,data_source_type,data_source_name,data_source_ip,access_port,access_url,db_type,db_instance,db_username,db_password,db_desc,db_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time,req_que_name,res_que_name" sort-columns="created_time">
    <filter-column column-name="data_source_id" operator="eq" value-from="input" empty-flag="skip"/>
    
    <description>单记录查询</description>
  </function>

  <function func-name="select res_data_source list" func-type="rowset" column-list="data_source_id,service_targets_id,data_source_type,data_source_name,data_source_ip,access_port,access_url,db_type,db_instance,db_username,db_password,db_desc,db_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time,req_que_name,res_que_name" sort-columns="created_time" max-record="10">
    <filter-column column-name="data_source_name" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="data_source_type" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="service_targets_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="created_time" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="is_markup" operator="eq" value-from="input" empty-flag="skip" default="Y"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one res_data_source" func-type="insert" column-list="data_source_id,service_targets_id,data_source_type,data_source_name,data_source_ip,access_port,access_url,db_type,db_instance,db_username,db_password,db_desc,db_status,is_markup,creator_id,created_time,last_modify_id,last_modify_time,req_que_name,res_que_name">
    <description>插入记录</description>
  </function>

  <function func-name="delete one res_data_source" func-type="delete">
    <filter-column column-name="data_source_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>

  <function func-name="update one res_data_source" func-type="update" column-list="data_source_type,data_source_name,data_source_ip,access_port,access_url,db_type,db_instance,db_username,db_password,db_desc,db_status,is_markup,last_modify_id,last_modify_time,service_targets_id,req_que_name,res_que_name">
    <filter-column column-name="data_source_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>

  <function func-name="update markup" func-type="update" column-list="is_markup" >
    <filter-column column-name="data_source_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>
</table>

