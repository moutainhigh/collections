<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>系统管理</sort-name>
  <table-info name="sys_advanced_query" class-name="com.gwssi.share.query.dao.SysAdvancedQuery" check-flag="false" backup-flag="false">
    <description>高级查询</description>
  </table-info>

  <column name="sys_advanced_query_id" data-type="varchar" length="32" not-null="true" value-from="input" description="高级查询编号"/>
  <column name="name" data-type="varchar" length="200" not-null="true" value-from="input" description="高级查询主题名称"/>
  <column name="display_columns" data-type="binary" length="2147483647" not-null="false" value-from="input" description="高级查询关联表编号"/>
  <column name="table_no" data-type="varchar" length="2000" not-null="false" value-from="input" description="数据表连接条件"/>
  <column name="query_sql" data-type="binary" length="2147483647" not-null="false" value-from="input" description="高级查询SQL"/>
  <column name="create_by" data-type="varchar" length="32" not-null="true" value-from="input" description="创建人"/>
  <column name="sys_svr_user_id" data-type="varchar" length="32" not-null="true" value-from="input" description="创建人id"/>
  <column name="create_date" data-type="varchar" length="20" not-null="true" value-from="input" description="创建日期"/>
  <column name="last_exec_date" data-type="varchar" length="20" not-null="false" value-from="input" description="最后执行日期"/>
  <column name="exec_total" data-type="long" length="38" not-null="true" value-from="input" description="执行次数"/>
  <column name="order_col" data-type="integer" length="38" not-null="true" empty-is-null="false" unique="false" value-from="input" description="排序字段"/>
  <column name="display_columns_en_array" data-type="binary" length="2147483647" not-null="false" value-from="input" description="所有显示字段的英文名"/>
  <column name="display_columns_cn_array" data-type="binary" length="2147483647" not-null="false" value-from="input" description="所有显示字段的中文名"/>
  <primary-key name="pk_sys_advanced_query" column-list="sys_advanced_query_id"/>
  <index name="pk_sys_advanced_query" index-type="unique" cluster="cluster" column-list="sys_advanced_query_id"/>
  <function func-name="select one sys_advanced_query" func-type="select" column-list="sys_advanced_query_id,name,table_no,display_columns,query_sql, create_by,sys_svr_user_id, create_date, last_exec_date, exec_total, display_columns_en_array, display_columns_cn_array">
    <filter-column column-name="sys_advanced_query_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="select sys_advanced_query list" func-type="rowset" column-list="sys_advanced_query_id, name, create_by,sys_svr_user_id, create_date, last_exec_date, exec_total" sort-columns="order_col desc" max-record="10">
    <filter-column column-name="name" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="create_by" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="sys_svr_user_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="create_date" operator="ge" value-from="input" empty-flag="skip"/>
    <filter-column column-name="create_date" operator="le" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one sys_advanced_query" func-type="insert" column-list="sys_advanced_query_id, name, display_columns, table_no, create_by,sys_svr_user_id, create_date, exec_total, order_col, query_sql, display_columns_en_array, display_columns_cn_array">
    <description>插入记录</description>
  </function>

  <function func-name="delete one sys_advanced_query" func-type="delete">
    <filter-column column-name="sys_advanced_query_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>

  <function func-name="update one sys_advanced_query" func-type="update" column-list="name, display_columns, table_no, query_sql, display_columns_en_array, display_columns_cn_array">
    <filter-column column-name="sys_advanced_query_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>

  <function func-name="select sys_advanced_query name" func-type="rowset" column-list="sys_advanced_query_id,name">
    <filter-column column-name="name" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="create_by" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="sys_svr_user_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="sys_advanced_query_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>根据查询名称查询，用于插入及修改记录是的名称唯一性的校验</description>
  </function>

  <function func-name="update sql sys_advanced_query" func-type="update" column-list="query_sql">
    <filter-column column-name="sys_advanced_query_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>
</table>

