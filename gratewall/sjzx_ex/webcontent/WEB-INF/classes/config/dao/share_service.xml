<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>共享服务</sort-name>
  <table-info name="share_service" class-name="com.gwssi.share.service.dao.ShareService" check-flag="false" backup-flag="false">
    <description>共享服务的服务表</description>
  </table-info>

  <column name="service_id" data-type="varchar" length="32" not-null="true" value-from="generate" func-name="getUUID" description="服务ID"/>
  <column name="interface_id" data-type="varchar" length="32" not-null="false" value-from="input" description="接口ID"/>
  <column name="service_targets_id" data-type="varchar" length="32" not-null="false" value-from="input" description="所属服务对象"/>
  <column name="service_name" data-type="varchar" length="100" not-null="false" value-from="input" description="服务名称"/>
  <column name="service_type" data-type="varchar" length="20" not-null="false" value-from="input" description="服务类型"/>
  <column name="service_no" data-type="varchar" length="20" not-null="false" value-from="input" description="服务编号"/>
  <column name="old_service_no" data-type="varchar" length="20" not-null="false" value-from="input" description="服务编号"/>
  <column name="column_no" data-type="varchar" length="4000" not-null="false" value-from="input" description="数据项ID"/>
  <column name="column_name_en" data-type="varchar" length="4000" not-null="false" value-from="input" description="数据项英文名称"/>
  <column name="column_name_cn" data-type="varchar" length="4000" not-null="false" value-from="input" description="数据项中文名称"/>
  <column name="column_alias" data-type="varchar" length="2000" not-null="false" value-from="input" description="数据项别名"/>
  <column name="sort_column" data-type="varchar" length="100" not-null="false" value-from="input" description="排序字段"/>
  <column name="sql" data-type="varchar" length="4000" not-null="false" value-from="input" description="SQL"/>
  <column name="sql_one" data-type="varchar" length="4000" not-null="false" value-from="input" description="SQL1"/>
  <column name="sql_two" data-type="varchar" length="4000" not-null="false" value-from="input" description="SQL2"/>
  <column name="service_description" data-type="varchar" length="2000" not-null="false" value-from="input" description="服务说明"/>
  <column name="regist_description" data-type="varchar" length="2000" not-null="false" value-from="input" description="备案说明"/>
  <column name="service_state" data-type="varchar" length="20" not-null="false" value-from="input" description="服务状态"/>
  <column name="is_markup" data-type="varchar" length="1" not-null="false" value-from="input" description="有效标记"/>
  <column name="creator_id" data-type="varchar" length="32" not-null="false" value-from="input" description="创建人ID"/>
  <column name="created_time" data-type="varchar" length="19" not-null="false" value-from="input" description="创建时间"/>
  <column name="last_modify_id" data-type="varchar" length="32" not-null="false" value-from="input" description="最后修改人ID"/>
  <column name="last_modify_time" data-type="varchar" length="19" not-null="false" value-from="input" description="最后修改时间"/>
  <column name="jsoncolumns" data-type="varchar" length="4000" not-null="false" value-from="input" description="JSON格式条件"/>
  <column name="fj_fk" data-type="varchar" length="2000" not-null="false" value-from="input" description="附件id"/>
  <column name="fjmc" data-type="varchar" length="4000" not-null="false" value-from="input" description="附件名称"/>
  <column name="limit_data" data-type="varchar" length="4000" not-null="false" value-from="input" description="JSON格式条件"/>
  <column name="is_month_data" data-type="varchar" length="1" not-null="false" value-from="input" description="是否限制本月数据"/>
  <column name="visit_period" data-type="varchar" length="3" not-null="false" value-from="input" description="访问时间间隔"/>
  <column name="is_single" data-type="varchar" length="1" not-null="false" value-from="input" description="是否单户"/>
  <primary-key name="pk_share_service" column-list="service_id"/>
  <index name="pk_share_service" index-type="unique" cluster="cluster" column-list="service_id"/>
  <function func-name="select one share_service" func-type="select" column-list="service_id,interface_id,service_targets_id,service_name,service_type,service_no,old_service_no,column_no,column_name_en,column_name_cn,column_alias,sort_column,sql,sql_one,sql_two,service_description,regist_description,service_state,is_markup,creator_id,created_time,last_modify_id,last_modify_time,jsoncolumns,fj_fk,fjmc,limit_data,is_month_data,visit_period,is_single" sort-columns="sort_column">
    <description>单记录查询</description>
  </function>

  <function func-name="select share_service list" func-type="rowset" column-list="service_id,interface_id,service_targets_id,service_name,service_type,service_no,old_service_no,column_no,column_name_en,column_name_cn,column_alias,sort_column,sql,sql_one,sql_two,service_description,regist_description,service_state,is_markup,creator_id,created_time,last_modify_id,last_modify_time,jsoncolumns,fj_fk,fjmc,limit_data,is_month_data,visit_period" sort-columns="sort_column" max-record="10">
    <filter-column column-name="service_targets_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="service_name" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="service_type" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="service_state" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="interface_id" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="created_time" operator="between" value-from="input" value-item="created_time_start" value-item2="created_time_end" empty-flag="skip"/>
    <!-- <filter-column column-name="created_time" operator="eq" value-from="input" empty-flag="skip"/> -->
    <description>多记录查询</description>
  </function>

  <function func-name="insert one share_service" func-type="insert" column-list="service_id,interface_id,service_targets_id,service_name,service_type,service_no,old_service_no,column_no,column_name_en,column_name_cn,column_alias,sort_column,sql,sql_one,sql_two,service_description,regist_description,service_state,is_markup,creator_id,created_time,last_modify_id,last_modify_time,jsoncolumns,fj_fk,fjmc,limit_data,is_month_data,visit_period,is_single">
    <description>插入记录</description>
  </function>

  <function func-name="delete one share_service" func-type="delete">
    <description>删除记录</description>
  </function>

  <function func-name="update one share_service" func-type="update" column-list="interface_id,service_targets_id,service_name,service_type,service_no,column_no,column_name_en,column_name_cn,column_alias,sort_column,sql,sql_one,sql_two,service_description,regist_description,service_state,is_markup,creator_id,created_time,last_modify_id,last_modify_time,jsoncolumns,fj_fk,fjmc,limit_data,is_month_data,visit_period,is_single">
    <description>修改记录</description>
  </function>

  <function func-name="status one share_service" func-type="update" column-list="is_markup">
    <filter-column column-name="service_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录的有效标志</description>
  </function>
</table>

