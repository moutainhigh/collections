<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>共享服务</sort-name>
  <table-info name="sys_db_view" class-name="com.gwssi.dw.runmgr.db.dao.SysDbView" check-flag="false" backup-flag="false">
    <description>数据库共享视图信息</description>
  </table-info>

  <column name="sys_db_view_id" data-type="varchar" length="32" not-null="true" value-from="generate" func-name="getUUID" description="视图ID"/>
  <column name="view_name" data-type="varchar" length="1000" not-null="true" value-from="input" description="视图名称"/>
  <column name="dczd_dm" data-type="varchar" length="32" not-null="false" value-from="input" description="主题代码（备用）"/>
  <column name="table_no" data-type="varchar" length="2000" not-null="true" value-from="input" description="表ID（逗号分隔）"/>
  <column name="column_no" data-type="varchar" length="2000" not-null="false" value-from="input" description="字段ID（逗号分隔）"/>
  <column name="column_cn" data-type="varchar" length="4000" not-null="false" value-from="input" description="字段中文（逗号分隔）"/>
  <column name="column_en" data-type="varchar" length="4000" not-null="false" value-from="input" description="字段英文（逗号分隔）"/>
  <column name="create_date" data-type="varchar" length="10" not-null="true" value-from="input" description="创建日期"/>
  <column name="create_by" data-type="varchar" length="32" not-null="true" value-from="input" description="创建人"/>
  <column name="view_desc" data-type="varchar" length="2000" not-null="false" value-from="input" description="视图描述"/>
  <column name="view_code" data-type="varchar" length="1000" not-null="true" value-from="input" description="视图代码"/>
  <column name="max_records" data-type="long" length="38" not-null="false" value-from="input" description="最大记录数（无用）"/>
  <column name="view_type" data-type="varchar" length="10" not-null="true" value-from="input" description="视图类型"/>
  <column name="view_order" data-type="long" length="38" not-null="true" value-from="input" description="视图顺序"/>
  <primary-key name="sys_c009524" column-list="sys_db_view_id"/>
  <index name="sys_c009524" index-type="unique" cluster="cluster" column-list="sys_db_view_id"/>
  <function func-name="select one sys_db_view" func-type="select" column-list="sys_db_view_id,view_name,dczd_dm,table_no,column_no,create_date,create_by,view_desc,view_code,max_records,view_type,column_cn,column_en,view_order">
    <filter-column column-name="sys_db_view_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="select sys_db_view list" func-type="rowset" column-list="sys_db_view_id,view_name,dczd_dm,table_no,column_no,create_date,create_by,view_desc,view_code,view_type" sort-columns="view_order" max-record="10">
    <filter-column column-name="view_code" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="view_name" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="create_by" operator="eq" value-from="input" empty-flag="skip"/>
    <filter-column column-name="create_date" operator="like" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="insert one sys_db_view" func-type="insert" column-list="sys_db_view_id,view_name,table_no,column_no,column_cn,column_en,create_date,create_by,view_desc,view_code,view_order">
    <description>插入记录</description>
  </function>

  <function func-name="delete one sys_db_view" func-type="delete">
    <filter-column column-name="sys_db_view_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>

  <function func-name="update one sys_db_view" func-type="update" column-list="view_code,view_name,view_desc,table_no,column_no,column_cn,column_en">
    <filter-column column-name="sys_db_view_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>

  <function func-name="select one sys_db_view_code" func-type="rowset" column-list="sys_db_view_id">
    <filter-column column-name="view_code" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询-数据库共享视图代码</description>
  </function>

  <function func-name="select one sys_db_view_name" func-type="rowset" column-list="sys_db_view_id">
    <filter-column column-name="view_name" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询-数据库共享视图名称</description>
  </function>

  <function func-name="select one sys_db_view_id" func-type="rowset" column-list="sys_db_view_id,view_name,table_no,create_date,create_by,view_desc,view_code">
    <filter-column column-name="sys_db_view_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询-数据库共享视图名称</description>
  </function>

  <function func-name="select max sys_db_view_db_order" func-type="rowset" column-list="max(view_order) as view_order">
    <description>单记录查询</description>
  </function>
</table>

