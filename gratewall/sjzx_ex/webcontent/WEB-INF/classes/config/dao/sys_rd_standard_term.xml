<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>资源管理</sort-name>
  <table-info name="sys_rd_standard_term" class-name="cn.gwssi.dw.rd.standard.dao.SysRdStandardTerm" check-flag="false" backup-flag="false">
    <description>术语定义</description>
  </table-info>

  <column name="sys_rd_standar_term_id" data-type="varchar" length="32" not-null="true" value-from="input" description="术语ID"/>
  <column name="standard_term_cn" data-type="varchar" length="64" not-null="false" value-from="input" description="术语名称"/>
  <column name="standard_term_en" data-type="varchar" length="64" not-null="false" value-from="input" description="英文名称"/>
  <column name="standard_term_definition" data-type="varchar" length="1024" not-null="false" value-from="input" description="术语定义"/>
  <column name="memo" data-type="varchar" length="2000" not-null="false" value-from="input" description="备注"/>
  <primary-key name="pk_sys_rd_standard_term" column-list="sys_rd_standar_term_id"/>
  <index name="pk_sys_rd_standard_term" index-type="unique" cluster="cluster" column-list="sys_rd_standar_term_id"/>
  <function func-name="select sys_rd_standard_term list" func-type="rowset" column-list="sys_rd_standar_term_id,standard_term_cn,standard_term_en,standard_term_definition,memo" sort-columns="sys_rd_standar_term_id" max-record="10">
    <filter-column column-name="standard_term_cn" operator="like" value-from="input" empty-flag="skip"/>
    <filter-column column-name="standard_term_en" operator="like" value-from="input" empty-flag="skip"/>
    <description>多记录查询</description>
  </function>

  <function func-name="select one sys_rd_standard_term" func-type="select" column-list="sys_rd_standar_term_id,standard_term_cn,standard_term_en,standard_term_definition,memo">
    <filter-column column-name="sys_rd_standar_term_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="insert one sys_rd_standard_term" func-type="insert" column-list="sys_rd_standar_term_id,standard_term_cn,standard_term_en,standard_term_definition,memo">
    <description>插入记录</description>
  </function>

  <function func-name="delete one sys_rd_standard_term" func-type="delete">
    <filter-column column-name="sys_rd_standar_term_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>删除记录</description>
  </function>

  <function func-name="update one sys_rd_standard_term" func-type="update" column-list="sys_rd_standar_term_id,standard_term_cn,standard_term_en,standard_term_definition,memo">
    <filter-column column-name="sys_rd_standar_term_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>修改记录</description>
  </function>
