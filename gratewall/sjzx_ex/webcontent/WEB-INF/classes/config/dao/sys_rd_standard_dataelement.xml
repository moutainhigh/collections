  <column name="sys_rd_standard_dataelement_id" data-type="varchar" length="32" not-null="true" value-from="input" description="基础数据元ID"/>
  <column name="standard_category" data-type="varchar" length="3" not-null="false" value-from="input" description="规范类型"/>
  <column name="identifier" data-type="varchar" length="8" not-null="false" value-from="input" description="标识符"/>
  <column name="cn_name" data-type="varchar" length="256" not-null="false" value-from="input" description="中文名称"/>
  <column name="en_name" data-type="varchar" length="256" not-null="false" value-from="input" description="英文名称"/>
  <column name="column_nane" data-type="varchar" length="64" not-null="false" value-from="input" description="字段名称"/>
  <column name="data_type" data-type="varchar" length="2" not-null="false" value-from="input" description="数据类型"/>
  <column name="data_length" data-type="varchar" length="32" not-null="false" value-from="input" description="数据长度"/>
