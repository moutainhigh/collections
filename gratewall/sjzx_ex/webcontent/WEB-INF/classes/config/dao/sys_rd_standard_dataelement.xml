  <column name="synonyms" data-type="varchar" length="100" not-null="false" value-from="input" description="同义词"/>
  <column name="version" data-type="varchar" length="8" not-null="false" value-from="input" description="版本"/>
  <column name="memo" data-type="varchar" length="2000" not-null="false" value-from="input" description="备注"/>
  <primary-key name="pk_sys_rd_standard_dataelement" column-list="sys_rd_standard_dataelement_id"/>
  <index name="pk_sys_rd_standard_dataelement" index-type="unique" cluster="cluster" column-list="sys_rd_standard_dataelement_id"/>
  <function func-name="select one sys_rd_standard_dataelement" func-type="select" column-list="sys_rd_standard_dataelement_id,standard_category,identifier,cn_name,en_name,column_nane,data_type,data_length,data_format,value_domain,jc_standar_codeindex,representation,unit,synonyms,version,memo" sort-columns="sys_rd_standard_dataelement_id">
