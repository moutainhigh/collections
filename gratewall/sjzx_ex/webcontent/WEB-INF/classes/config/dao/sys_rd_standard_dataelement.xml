  </function>

  <function func-name="update one sys_rd_standard_dataelement" func-type="update" column-list="sys_rd_standard_dataelement_id,standard_category,identifier,cn_name,en_name,column_nane,data_type,data_length,data_format,value_domain,jc_standar_codeindex,representation,unit,synonyms,version,memo">
