  </function>

  <function func-name="select sys_rd_standard_dataelement list" func-type="rowset" column-list="sys_rd_standard_dataelement_id,standard_category,identifier,cn_name,en_name,column_nane,data_type,data_length,data_format,value_domain,jc_standar_codeindex,representation,unit,synonyms,version,memo" sort-columns="sys_rd_standard_dataelement_id" max-record="10">
