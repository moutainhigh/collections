<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE table SYSTEM "gwssi-database.dtd">
<table>
  <database>gwssi</database>
  <default-config>no</default-config>
  <sort-name>资源管理</sort-name>
  <table-info name="res_share_table" class-name="com.gwssi.resource.share.dao.ResShareTable" check-flag="false" backup-flag="false">
    <description>共享表信息表</description>
  </table-info>

  <column name="share_table_id" data-type="varchar" length="32" not-null="true" value-from="generate" func-name="getUUID" description="共享表ID"/>
  <column name="business_topics_id" data-type="varchar" length="32" not-null="false" value-from="input" description="业务主题ID"/>
  <column name="table_name_en" data-type="varchar" length="100" not-null="false" value-from="input" description="表名称"/>
  <column name="table_name_cn" data-type="varchar" length="100" not-null="false" value-from="input" description="表中文名称"/>
  <column name="table_no" data-type="varchar" length="32" not-null="false" value-from="input" description="表编号"/>
  <column name="show_order" data-type="long" length="0" not-null="false" value-from="input" description="显示顺序"/>
  <column name="time_" data-type="varchar" length="10" not-null="false" value-from="input" description="时间字段"/>
  <column name="table_type" data-type="varchar" length="20" not-null="false" value-from="input" description="表类型"/>
  <column name="table_index" data-type="varchar" length="256" not-null="false" value-from="input" description="表索引"/>
  <column name="is_markup" data-type="varchar" length="1" not-null="false" value-from="input" description="有效标记"/>
  <primary-key name="pk_res_share_table" column-list="share_table_id"/>
  <index name="pk_res_share_table" index-type="unique" cluster="cluster" column-list="share_table_id"/>
  <function func-name="select one res_share_table" func-type="select" column-list="share_table_id,business_topics_id,table_name_en,table_name_cn,table_no,show_order,time_,table_type,table_index,is_markup">
    <filter-column column-name="business_topics_id" operator="eq" value-from="input" empty-flag="skip"/>
    <description>单记录查询</description>
  </function>

  <function func-name="select res_share_table list" func-type="rowset" column-list="share_table_id,business_topics_id,table_name_en,table_name_cn,table_no,show_order,time_,table_type,table_index,is_markup" sort-columns="show_order" max-record="10">
    <description>多记录查询</description>
  </function>

  <function func-name="insert one res_share_table" func-type="insert" column-list="share_table_id,business_topics_id,table_name_en,table_name_cn,table_no,show_order,time_,table_type,table_index,is_markup">
    <description>插入记录</description>
  </function>

  <function func-name="delete one res_share_table" func-type="delete">
    <description>删除记录</description>
  </function>

  <function func-name="update one res_share_table" func-type="update">
    <description>修改记录</description>
  </function>
</table>

