<?xml version="1.0" encoding="GB2312"?>
<service provider="com.trs.ajaxservice.UserServiceProvider">

	<method name="getOnlineUsers" desc="获取在线用户信息" params-required="false">
		<parameter name="PageSize" desc="分页大小(此方法不分页)" type="int" required="false" default="-1"/>
		<parameter name="SelectFields" desc="提取那些字段" type="string" required="false" default="UserId,UserName,NickName,TrueName,Email,LoginTime,Attribute"/>
	</method>

	<method name="getUsersByNames" desc="获取在线用户信息">
		<parameter name="UserNames" desc="查询的user names" type="string" required="true"/>
	</method>
	
	<method name="save" desc="保存用户信息">
		<parameter name="UserId" desc="获取用户的ID" type="string" required="true"/>
		<parameter name="GroupId" desc="获取用户组织ID" type="string" required="false"/>
		<parameter name="USERNAME" desc="获取用户名" type="string" required="true"/>
		<parameter name="NICKNAME" desc="获取用户呢称" type="string" required="true"/>
		<parameter name="EMAIL" desc="获取用户EMAIL" type="string" required="true"/>
		<parameter name="PASSWORD" desc="获取用户密码" type="string" required="false"/>
		<parameter name="TRUENAME" desc="获取用户真实姓名" type="string" required="false"/>
		<parameter name="TEL" desc="获取用户电话" type="string" required="false"/>
		<parameter name="MOBILE" desc="获取用户移动电话" type="string" required="false"/>
		<parameter name="ADDRESS" desc="获取用户地址" type="string" required="false"/>
		<parameter name="SUPERVISORID" desc="上级领导" type="string" required="false"/>
	</method>		
</service>