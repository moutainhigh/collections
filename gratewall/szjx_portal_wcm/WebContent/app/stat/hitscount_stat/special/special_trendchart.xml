<?xml version="1.0" encoding="UTF-8"?>
<parameters>	
	<parameter name="StartTime"  desc="统计的开始时间"  type="string" required="false"/>
	<parameter name="EndTime"  desc="统计的结束时间"  type="string" required="false"/>
	<parameter name="PageSize"  desc="页面大小"  type="int" required="false"/>
	<parameter name="PageIndex"  desc="当前页面"  type="int" required="false"/>
	<parameter name="TimeItem"  desc="创建时间的方式"  type="int" required="false" default="0"/>
	<parameter name="HitsTimeItem"  desc="点击量时间的指定方式"  type="int" required="false" default="0"/>
</parameters>