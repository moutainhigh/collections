<?xml version="1.0" encoding="GB2312"?>
<parameters>	
	<parameter name="GroupId"  desc="默认定位用户组ID"  min-value="1" type="int" required="false"/>
	<parameter name="GroupIds"  desc="默认定位用户组ID序列" type="string" required="false" default=""/>
	
	<parameter name="TreeType"  desc="类型"  min-value="0" type="int" default="0"/>
	<parameter name="OperType"  desc="导航操作类型"  min-value="0" type="int" default="12"/>

	<parameter name="ObjTreeName"  desc="JavaScript TREE对象名称" type="string" default="oTRSTree"/>
	
	<parameter name="TreeWidth" desc="导航树宽度" type="int" required="false"/>
	
	<parameter name="ShowAll" desc="是否显示所有用户组" type="boolean" required="false" default="0"/>

	<parameter name="FilterRight" desc="是否过滤没有管理权限的用户组" type="boolean" required="false" default="0"/>

</parameters>